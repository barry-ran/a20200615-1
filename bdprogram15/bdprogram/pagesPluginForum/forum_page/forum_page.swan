<block s-if="{{$imgurl&&baseinfo.can_fs==1}}"><view><block s-if="{{needAuth}}"><auth vue-id="1" needAuth="{{needAuth}}" data-event-opts="{{[['^closeAuth',[['closeAuth']]],['^cell',[['cell']]]]}}" bind:closeAuth="__e" bind:cell="__e"></auth></block><block s-if="{{needBind}}"><bind-phone vue-id="2" needBind="{{needBind}}" data-event-opts="{{[['^closeBind',[['closeBind']]]]}}" bind:closeBind="__e"></bind-phone></block><view class="dynamic_single"><view class="hbj"><image class="dynamic_tx" src="{{article_con.avatar}}" mode="aspectFill"></image><view class="dynamic_view1_center"><view class="dynamic_name"><text class="dynamic_name_text1">{{article_con.nickname}}</text><text class="dynamic_name_text2">{{article_con.createtime}}</text></view><view class="forum_cardbox hbj"><block s-if="{{article_con.stick==1}}"><view class="forum_card_single" style="background:#2F74FD;">置顶</view></block><block s-if="{{article_con.hot==1}}"><view class="forum_card_single" style="background:#E12735;">推荐</view></block></view></view><view class="flex1"></view><block s-if="{{article_con.telphone!=''&&article_con.telphone!='undefined'}}"><block><image class="forum_phone" src="{{$imgurl+'phone.png'}}" mode="aspectFill" data-tel="{{article_con.telphone}}" data-event-opts="{{[['tap',[['makephone',['$event']]]]]}}" bindtap="__e"></image><block s-if="{{article_con.stick==2}}"><view class="forum_page_line"></view></block></block></block><block s-if="{{article_con.stick==2&&suid==article_con.suid&&settop==1}}"><image class="forum_phone" src="{{$imgurl+'set_top.png'}}" mode="aspectFill" data-event-opts="{{[['tap',[['setStick',['$event']]]]]}}" bindtap="__e"></image></block></view><view class="dynamic_title">{{article_con.title}}</view><view class="dynamic_content">{{article_con.content}}</view><block s-for="{{article_con.img}}" s-for-item="item" s-for-index="k" s-key="k"><block><image class="forum_page_dynamic_img" src="{{item}}" mode="widthFix"></image></block></block><block s-if="{{article_con.address!=''}}"><view class="forum_address hbj"><image class="forum_address_left" src="{{$imgurl+'position.png'}}" mode="aspectFill"></image><view class="forum_address_right">{{article_con.address}}</view></view></block><view class="dynamic_single_bot hbj"><view class="dynamic_single_bot_view"><text class="dynamic_single_bot_text1 iconfont icon-c-kan"></text><text class="dynamic_single_bot_text2">{{article_con.hits}}</text></view><view data-event-opts="{{[['tap',[['changeLikes',['$event']]]]]}}" class="dynamic_single_bot_view" bindtap="__e"><text class="dynamic_single_bot_text1 iconfont icon-x-zan1" style="{{(article_con.is_like==1?'color:#FF0000':'')}}"></text><text class="dynamic_single_bot_text2">{{article_con.likes}}</text></view><view data-event-opts="{{[['tap',[['changeCollection',['$event']]]]]}}" class="dynamic_single_bot_view" bindtap="__e"><text class="dynamic_single_bot_text1 iconfont icon-c-xin1" style="{{'vertical-align:'+('-2rpx')+';'+('color:'+(article_con.is_collect==1?'#FF0000':'')+';')}}"></text><text class="dynamic_single_bot_text2">{{article_con.collection}}</text></view><view class="flex1"></view><block s-if="{{article_con.suid==suid}}"><view style="font-size:28rpx;color:#0061CE;" data-id="{{article_con.id}}" data-types="1" data-event-opts="{{[['tap',[['pagedel',['$event']]]]]}}" bindtap="__e">删除</view></block></view></view><view class="plbox"><view class="plhead hbj" style="{{(count==0?'padding-bottom:26rpx':'')}}"><view class="plhead_left">全部评论</view><view class="plhead_right">{{count+"条"}}</view></view><block s-for="{{commentList}}" s-for-item="item" s-for-index="idx" s-key="idx"><block s-if="{{count>0}}"><block><view class="pl_single"><view class="hbj"><image class="dynamic_tx" src="{{item.avatar}}" mode="aspectFill"></image><view class="dynamic_view1_center" style="width:420rpx;"><view class="dynamic_name">{{item.nickname}}</view><view class="dynamic_time">{{item.createtime}}</view></view><view class="flex1"></view><view><block s-if="{{article_con.suid==suid}}"><text data-uid="{{item.suid}}" data-nickname="{{item.nickname}}" data-commentid="{{item.id}}" data-event-opts="{{[['tap',[['toReply',['$event']]]]]}}" bindtap="__e">回复 ·</text></block></view><view class="hbj" style="margin-left:10rpx;" data-type="1" data-index="{{idx}}" data-commentid="{{item.id}}" data-event-opts="{{[['tap',[['commentAddLikes',['$event']]]]]}}" bindtap="__e"><text class="forum_page_like iconfont icon-x-zan1" style="{{(item.is_like==1?'color:#FF0000':'')}}"></text><text>{{item.likesNum}}</text></view></view><block s-if="{{item.suid==suid}}"><view class="dynamic_content" data-id="{{item.id}}" data-types="2" data-event-opts="{{[['tap',[['alertdel',['$event']]]]]}}" bindtap="__e">{{item.content}}</view></block><block s-if="{{item.suid!=suid}}"><view class="dynamic_content">{{item.content}}</view></block><block s-if="{{item.reply.length>0}}"><view class="reply_con"><block s-for="{{item.reply}}" s-for-item="reply" s-for-index="idr" s-key="idr"><block><view class="reply_list"><view class="hbj"><image class="dynamic_tx" src="{{reply.avatar}}" mode="aspectFill"></image><view class="dynamic_view1_center" style="width:420rpx;"><view class="dynamic_name">{{reply.nickname}}</view><view class="dynamic_time">{{reply.createtime}}</view></view><view class="flex1"></view><view class="hbj" data-type="2" data-index="{{idr}}" data-topindex="{{idx}}" data-commentid="{{reply.id}}" data-event-opts="{{[['tap',[['commentAddLikes',['$event']]]]]}}" bindtap="__e"><text class="forum_page_like iconfont icon-x-zan1" style="{{(reply.is_like==1?'color:#FF0000':'')}}"></text><text>{{reply.likesNum}}</text></view></view><block s-if="{{article_con.suid==suid}}"><view class="dynamic_content" data-id="{{reply.id}}" data-types="3" data-event-opts="{{[['tap',[['alertdel',['$event']]]]]}}" bindtap="__e">{{reply.content}}</view></block><block s-if="{{article_con.suid!=suid}}"><view class="dynamic_content">{{reply.content}}</view></block></view></block></block></view></block></view></block></block></block></view><block s-if="{{alert_del==1}}"><view class="alert_del"><view class="alert_delbtn" data-id="{{delid}}" data-types="{{types}}" data-event-opts="{{[['tap',[['pagedel',['$event']]]]]}}" bindtap="__e">删除</view><view data-event-opts="{{[['tap',[['hide_alert_del',['$event']]]]]}}" class="alert_delbtn2" bindtap="__e">取消</view></view></block><block s-if="{{suid==article_con.suid}}"><image style="width:90rpx;height:90rpx;position:fixed;right:20rpx;top:50%;" src="{{$imgurl+'btn_edit.png'}}" data-event-opts="{{[['tap',[['setChange',['$event']]]]]}}" bindtap="__e"></image></block><view style="height:100rpx;"></view><view class="comment hbj"><input class="comment_left" placeholder="{{nickname!=''?'@'+nickname:'发布评论,内容小于100个字'}}" placeholder-class="placeholderstyle" data-event-opts="{{[['input',[['getInputCon',['$event']]]]]}}" bindinput="__e"/><view class="flex1"></view><image class="comment_right" src="{{$imgurl+'fb.png'}}" mode="aspectFill" data-event-opts="{{[['tap',[['commentSub',['$event']]]]]}}" bindtap="__e"></image></view></view></block>