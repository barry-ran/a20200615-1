<block s-if="{{$imgurl&&baseinfo.can_fs==1}}"><view><block s-if="{{needAuth}}"><auth vue-id="1" needAuth="{{needAuth}}" data-event-opts="{{[['^closeAuth',[['closeAuth']]]]}}" bind:closeAuth="__e"></auth></block><block s-if="{{needBind}}"><bind-phone vue-id="2" needBind="{{needBind}}" data-event-opts="{{[['^closeBind',[['closeBind']]]]}}" bind:closeBind="__e"></bind-phone></block><view class="set_top_money hbj"><view class="set_top_left flex1">付费置顶每天金额</view><view class="set_top_right">￥<text>{{stick_money}}</text></view></view><view class="set_top_money hbj"><view class="set_top_day">置顶天数</view><input class="set_top_input flex1" placeholder="请填写置顶天数" data-event-opts="{{[['input',[['getStickDays',['$event']]]]]}}" bindinput="__e"/></view><view style="height:20rpx;background:#F8F8F8;"></view><view class="allmoney hbj"><view class="allmoney_left flex1">合计金额</view><view class="allmoney_right">￥<text>{{allmoney}}</text></view></view><view class="allmoney hbj"><view class="allmoney_left flex1">账户余额</view><view class="allmoney_right">￥<text>{{userMoney}}</text></view></view><form report-submit="true" data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" bindsubmit="__e"><button class="relase_btn" formType="submit">立即支付</button></form></view></block>