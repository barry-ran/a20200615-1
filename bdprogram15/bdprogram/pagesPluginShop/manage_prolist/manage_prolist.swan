<block s-if="{{$imgurl&&baseinfo.can_show==1}}"><view class="container"><scroll-view class="order_nav" scroll-x><view class="{{['order_nav_single',flag==1?'order_nav_single_on':'']}}" data-flag="1" data-event-opts="{{[['tap',[['changflag',['$event']]]]]}}" bindtap="__e">已审核</view><view class="{{['order_nav_single',flag==0?'order_nav_single_on':'']}}" data-flag="0" data-event-opts="{{[['tap',[['changflag',['$event']]]]]}}" bindtap="__e">待审核</view></scroll-view><view class="weui-tab__content"><block s-for="{{prolist}}" s-for-item="item" s-for-index="key" s-key="key"><view class="main-products"><view class="title"><view class="title-time">{{'发布时间：'+item.createtime+''}}</view><view class="title-edit"><view class="button" data-pid="{{item.id}}" data-event-opts="{{[['tap',[['prodel',['$event']]]]]}}" bindtap="__e"><image src="{{$imgurl+'del.png'}}"></image>删</view><view class="button" data-pid="{{item.id}}" data-event-opts="{{[['tap',[['proedit',['$event']]]]]}}" bindtap="__e"><image src="{{$imgurl+'edit.png'}}"></image>编</view></view></view><view data-id="{{item.id}}" data-flag="{{item.flag}}" data-event-opts="{{[['tap',[['goodsDetail',['$event']]]]]}}" bindtap="__e"><view class="main"><view><image src="{{item.thumb}}"></image></view><view class="main-mid"><view class="main-mid-top">{{''+item.title}}<block s-if="{{item.flag==2}}"><label style="color:#ee3333;margin-left:20rpx;font-size:20rpx;" class="_span">(未上架)</label></block></view><view class="main-mid-foot"><text>{{"库存："+item.storage}}</text><text>{{"销量："+item.sales}}</text></view></view><view class="main-money"><text class="money"><text class="money-sign">￥</text>{{item.sellprice}}</text></view></view></view></view></block></view><navigator class="flex-button" url="../manage_pro/manage_pro"><image src="{{$imgurl+'add.png'}}"></image></navigator></view></block>