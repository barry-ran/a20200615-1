<block s-if="{{$imgurl&&baseinfo.can_show==1}}"><view><view class="bg_f mb10"><view class="selectarea"><view class="menu _dl"><block s-for="{{menuContent}}" s-for-item="item" s-for-index="key" s-key="key"><block><view class="{{['_dt',menuCss[index],'btnarea']}}" data-index="{{key}}" data-event-opts="{{[['tap',[['tapMainMenu',['$event']]]]]}}" bindtap="__e"><text class="textarea greycolor">{{item.title}}</text><image class="{{[rotateRight[index],'icon-img']}}" style="width:24rpx;height:24rpx;margin:10rpx 0 0 10rpx;" src="{{$imgurl+'icon-arrowdown.png'}}"></image></view></block></block><block s-if="{{showMenu}}"><view class="font-sm _dd"><view class="_ul"><block s-for="{{content}}" s-for-item="item" s-for-index="index" s-key="index"><block><view class="font-xs _li" data-index="{{index}}" data-event-opts="{{[['tap',[['tapSubMenu',['$event']]]]]}}" bindtap="__e">{{''+item+''}}</view></block></block></view></view></block></view></view></view><block s-for="{{shopList}}" s-for-item="item" s-for-index="key" s-key="key"><view class="storeList"><navigator url="{{'/pagesPluginShop/shop/shop?id='+item.id}}"><view class="storeListThumb"><image mode="aspectFill" src="{{item.logo}}"></image></view><view class="storeListTitle"><view class="storeListTitle1">{{''+item.name+''}}</view><view class="storeListTitle2"><image mode="scaleToFill" src="{{$imgurl+'i_tel.png'}}"></image>{{item.tel+''}}</view><view class="storeListTitle2"><image mode="scaleToFill" src="{{$imgurl+'i_add.png'}}"></image>{{item.address+''}}</view></view><view class="storeListR"><view class="storeListEnter" style="{{'color:'+(baseinfo.base_color2)+';'+('border:'+('2rpx solid '+baseinfo.base_color2)+';')}}">进店逛逛</view><view class="storeListLocal">{{''+item.distance+''}}</view></view></navigator></view></block><block s-if="{{baseinfo.tabbar_t==1}}"><copyright vue-id="1" baseinfo="{{baseinfo}}"></copyright></block><myfooter vue-id="2" page_signs="{{page_signs}}" baseinfo="{{baseinfo}}"></myfooter></view></block>