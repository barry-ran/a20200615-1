<block s-if="{{baseinfo.can_show==1}}"><view class="tixian"><form report-submit="true" data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" bindsubmit="__e"><view class="txtitle">{{"可提现佣金 ￥"+allMoney}}</view><view class="txinput"><text class="txl">￥</text><input type="number" name="money" value="{{setmoney}}"/><text data-event-opts="{{[['tap',[['getall',['$event']]]]]}}" class="txr" bindtap="__e">全部</text></view><view class="txnotice">{{"最低提现 ￥"+miniNum}}</view><view class="txbox"><text class="txboxl">提现到</text><radio-group class="txboxr" name="buy_type" data-event-opts="{{[['change',[['radioChange',['$event']]]]]}}" bindchange="__e"><block s-if="{{weixin}}"><block><label class="radio"><radio value="1" checked="true"></radio>微信</label></block></block><block s-if="{{zhifubao}}"><block><label class="radio"><radio value="2"></radio>支付宝</label></block></block><block s-if="{{yinhangka}}"><block><label class="radio"><radio value="3"></radio>银行卡</label></block></block></radio-group></view><block s-if="{{flag}}"><block><viev class="txrow" vue-id="1" vue-slots="{{['default']}}">开户行：<input class="form-control" type="text" name="card" placeholder="请输入开户行"/></viev></block></block><viev class="txrow" vue-id="2" vue-slots="{{['default']}}">账号：<input class="form-control" type="text" name="account" placeholder="请输入账号"/></viev><viev class="txrow" vue-id="3" vue-slots="{{['default']}}">备注：<input class="form-control" type="text" name="beizhu" placeholder="请输入备注"/></viev><view class="txbutton"><button disabled="{{btndis}}" form-type="submit">提现</button></view></form></view></block>