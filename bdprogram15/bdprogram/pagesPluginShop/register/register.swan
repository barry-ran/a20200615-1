<block s-if="{{$imgurl&&baseinfo.can_show==1}}"><view><block s-if="{{needAuth}}"><auth vue-id="1" needAuth="{{needAuth}}" data-event-opts="{{[['^closeAuth',[['closeAuth']]],['^cell',[['cell']]]]}}" bind:closeAuth="__e" bind:cell="__e"></auth></block><block s-if="{{needBind}}"><bind-phone vue-id="2" needBind="{{needBind}}" data-event-opts="{{[['^closeBind',[['closeBind']]]]}}" bind:closeBind="__e"></bind-phone></block><block s-if="{{!apply_success}}"><block><image style="width:100%;" src="{{slide}}" mode="widthFix"></image><form report-submit="true" data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" bindsubmit="__e"><view class="con"><view class="tit"><view>欢迎加入<label class="color_red _span">{{system_name}}</label></view></view><view><view class="input_t">登录账号<text class="color_red">*</text></view><input placeholder="建议用手机号作为登录帐号" placeholder-style="color:#969696" name="username"/></view><view><view class="input_t">登录密码<text class="color_red">*</text></view><input placeholder="请填写密码" type="password" placeholder-style="color:#969696" name="password"/></view><block s-if="{{category.length}}"><block><view><view class="input_t">所属分类<text class="color_red">*</text></view><picker style="width:100%;" value="{{index}}" range-key="name" name="cid" range="{{category}}" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" bindchange="__e"><view class="picker picker_duo" style="padding-left:62rpx;color:#969696;border:none;">{{catename}}</view></picker></view></block></block><view><view class="input_t">商户名称<text class="color_red">*</text></view><input placeholder="请填写商户名称" placeholder-style="color:#969696" name="name"/></view><view><view class="input_t">商户电话<text class="color_red">*</text></view><input placeholder="请填写商户电话" placeholder-style="color:#969696" type="tel" name="tel" maxlength="11"/></view><view><view class="input_t">商户地址<text class="color_red">*</text></view><input placeholder="请填写商户地址" placeholder-style="color:#969696" name="address"/></view><view><view class="input_t">详细位置<text class="color_red">*</text></view><view data-event-opts="{{[['tap',[['getLatlong',['$event']]]]]}}" style="height:98rpx;" bindtap="__e"><input placeholder="点击获取经纬度" disabled placeholder-style="color:#969696" name="latlong" value="{{latlong}}"/></view></view><view><view class="input_t">营业时间<text class="color_red">*</text></view><input placeholder="例如:9:00-21:00" placeholder-style="color:#969696" name="worktime"/></view><view><view class="input_t">一句话简介<text class="color_red">*</text></view><input placeholder="请填写一句话简介" placeholder-style="color:#969696" name="intro"/></view><view><view class="input_t">营业执照<text class="color_red">*</text></view><input name="yyzz" hidden="true" value="{{yyzz?yyzz:''}}"/><view data-event-opts="{{[['tap',[['yyzz_upload',['$event']]]]]}}" class="tupview tupview_bg" bindtap="__e"><block s-if="{{yyzz}}"><block><image class="yikds" src="{{yyzz}}"></image></block></block><block s-if="{{!yyzz}}"><block><image class="yikds" src="{{$imgurl+'add.png'}}"></image></block></block></view></view><view><view class="input_t">门头/LOGO<text class="color_red">*</text></view><input name="logo" hidden="true" value="{{logo?logo:''}}"/><view data-event-opts="{{[['tap',[['logo_upload',['$event']]]]]}}" class="tupview tupview_bg" bindtap="__e"><block s-if="{{logo}}"><block><image class="yikds" src="{{logo}}"></image></block></block><block s-if="{{!logo}}"><block><image class="yikds" src="{{$imgurl+'add.png'}}"></image></block></block></view></view><view data-event-opts="{{[['tap',[['check_change',['$event']]]]]}}" bindtap="__e"><block s-if="{{check==0}}"><view class="check_box"></view></block><block s-if="{{check==1}}"><image style="width:32rpx;height:32rpx;margin-right:10rpx;" src="{{$imgurl+'change_h1.png'}}"></image></block><view>我已经阅读并了解</view><view data-event-opts="{{[['tap',[['',['$event']]]]]}}" catchtap="__e" class="_div"><view data-event-opts="{{[['tap',[['xieyi_hidden',['$event']]]]]}}" class="xieyi" bindtap="__e">【入驻协议】</view></view></view></view><button class="sub" formType="submit">申请注册</button></form><block s-if="{{xieyi_block==1}}"><block><view class="gwc" style="{{('height:'+h+'px;')}}"><view class="gwc_con"><view>入驻协议</view><scroll-view style="height:640rpx;width:500rpx;margin:0 auto;" scroll-y bindscrolltoupper="upper" bindscrolltolower="lower" bindscroll="scroll" scroll-into-view="{{toView}}" scroll-top="{{scrollTop}}"><rich-text nodes="{{protocol}}"></rich-text></scroll-view><view class="yidu"><view></view><view data-event-opts="{{[['tap',[['xieyi_close',['$event']]]]]}}" class="color_red" bindtap="__e">我已阅读</view></view></view></view></block></block></block></block><block s-if="{{apply_success}}"><view class="noticeCon"><view class="iconfont icon-x-dui2"></view><view class="tit1">恭喜您入驻成功，请从登录下面网址，管理店铺！</view><view class="tit2">管理后台登录地址：</view><view class="tit2" style="margin-bottom:20rpx;">{{data_url}}</view><view class="tit2">{{"登陆账号："+data_username}}</view><view class="tit2">{{"登录密码："+data_password}}</view></view></block></view></block>