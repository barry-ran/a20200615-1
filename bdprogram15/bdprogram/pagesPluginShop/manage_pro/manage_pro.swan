<block s-if="{{$imgurl&&baseinfo.can_show==1}}"><view class="container"><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" bindsubmit="__e"><view class="row formtitle">排序</view><view class="row formcon"><input class="m-wrap placeholder-no-fix" type="text" placeholder="序号越大越靠前，不填为0" name="num" value="{{proInfo.num?proInfo.num:0}}"/></view><view class="row formtitle">是否上架</view><radio-group class="radio-group formcon" name="flag"><label class="radio"><radio value="1" checked="{{proInfo.flag==1?'true':''}}"></radio>上架</label><label class="radio" style="margin-left:30rpx;"><radio value="2" checked="{{proInfo.flag==2?'true':''}}"></radio>下架</label></radio-group><view class="row formtitle">所属栏目</view><view class="row formcon"><view class="section"><picker value="{{index}}" range="{{cateinfo}}" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{'当前选择：'+cateinfo[index]+''}}</view></picker></view></view><view class="row formtitle">标题</view><view class="row formcon"><block s-if="{{proInfo.title}}"><block><input class="m-wrap placeholder-no-fix" type="text" placeholder="标题" name="title" value="{{proInfo.title}}"/></block></block><block s-else><block><input class="m-wrap placeholder-no-fix" type="text" placeholder="标题" name="title"/></block></block></view><view class="row formtitle">缩略图</view><view class="image_content formcon"><block s-if="{{thumb}}"><block><block s-if="{{hideAdd1}}"><block><view class="image"><image class="moment_img" src="{{proInfo.thumb}}" data-event-opts="{{[['tap',[['choosethumb',['$event']]]]]}}" bindtap="__e"></image><view class="delimg" data-id="{{key}}" data-event-opts="{{[['tap',[['choosethumb',['$event']]]]]}}" bindtap="__e">更换</view></view></block></block><block wx:else><view class="image"><image class="moment_img" src="{{thumb}}" data-event-opts="{{[['tap',[['choosethumb',['$event']]]]]}}" bindtap="__e"></image></view></block></block></block><block wx:else><image class="moment_img" style="{{'display:'+(thumb==''?'none':'block')+';'}}" src="{{thumb}}" data-event-opts="{{[['tap',[['choosethumb',['$event']]]]]}}" bindtap="__e"></image><view class="image" style="{{'display:'+(hideAdd1==0?'none':'block')+';'}}"><image class="moment_img" src="{{$imgurl+'add.png'}}" data-event-opts="{{[['tap',[['choosethumb',['$event']]]]]}}" bindtap="__e"></image></view></block></view><view class="row formtitle">产品组图</view><view class="image_content formcon"><block s-for="{{imgs}}" s-for-item="item" s-for-index="key" s-key="key"><view class="image"><image class="moment_img" src="{{item}}"></image><view class="delimg" data-id="{{key}}" data-event-opts="{{[['tap',[['delimg',['$event']]]]]}}" bindtap="__e">删除</view></view></block><view class="image" style="{{'display:'+(imgs.length>2?'none':'block')+';'}}"><image class="moment_img" src="{{$imgurl+'add.png'}}" data-event-opts="{{[['tap',[['chooseZutu',['$event']]]]]}}" bindtap="__e"></image></view></view><view class="row formtitle">售价</view><view class="row formcon"><block s-if="{{proInfo.sellprice}}"><block><input class="m-wrap placeholder-no-fix" type="digit" placeholder="售价" name="sellprice" value="{{proInfo.sellprice}}"/></block></block><block s-else><block><input class="m-wrap placeholder-no-fix" type="digit" placeholder="售价" name="sellprice"/></block></block></view><view class="row formtitle">库存量</view><view class="row formcon"><block s-if="{{proInfo.storage}}"><block><input class="m-wrap placeholder-no-fix" type="number" placeholder="库存量" name="storage" value="{{proInfo.storage}}"/></block></block><block s-else><block><input class="m-wrap placeholder-no-fix" type="number" placeholder="库存量" name="storage"/></block></block></view><view class="row formtitle">产品详情</view><textarea class="textarea formcon" name="descp" placeholder="产品详情" value="{{proInfo.descp}}"></textarea><button form-type="submit">确认发布/修改</button></form></view></block>