<block s-if="{{$imgurl&&baseinfo.can_show==1}}"><view class="container"><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" bindsubmit="__e"><view class="row formtitle">用户名</view><view class="row formcon"><input class="m-wrap placeholder-no-fix" type="text" auto-focus placeholder="商户登录时使用" name="username" value="{{proInfo.username}}"/></view><view class="row formtitle">密码</view><view class="row formcon"><input class="m-wrap placeholder-no-fix" type="text" placeholder="商户登录时使用" name="password" value="{{proInfo.password}}"/></view><view class="row formtitle">分类</view><view class="formcon"><view style="float:left;">当前选择</view><picker style="float:right;" name="cid" data-id="{{cateList.id}}" range="{{cateName}}" data-event-opts="{{[['change',[['changePicker',['$event']]]]]}}" bindchange="__e"><view class="weui-input">{{cateName[cate]}}</view></picker></view><view class="row formtitle">LOGO</view><view class="image_content formcon"><view class="image"><image class="moment_img" src="{{logo}}" data-event-opts="{{[['tap',[['chooselogo',['$event']]]]]}}" bindtap="__e"></image></view><view class="image" style="{{'display:'+(logo?'none':'block')+';'}}"><image class="moment_img" src="{{$imgurl+'add.png'}}" data-event-opts="{{[['tap',[['chooselogo',['$event']]]]]}}" bindtap="__e"></image></view></view><view class="row formtitle">店铺名称</view><view class="row formcon"><input class="m-wrap placeholder-no-fix" type="text" placeholder="请填写店铺名称" name="name" value="{{proInfo.name}}"/></view><view class="row formtitle">客户介绍</view><textarea class="textarea formcon" name="descp" placeholder="客户介绍" value="{{proInfo.descp}}"></textarea><button form-type="submit">保存</button></form></view></block>