<block s-if="{{$imgurl&&baseinfo.can_show==1}}"><view class="page _div"><block s-if="{{needAuth}}"><auth vue-id="1" needAuth="{{needAuth}}" data-event-opts="{{[['^closeAuth',[['closeAuth']]],['^cell',[['cell']]]]}}" bind:closeAuth="__e" bind:cell="__e"></auth></block><block s-if="{{needBind}}"><bind-phone vue-id="2" needBind="{{needBind}}" data-event-opts="{{[['^closeBind',[['closeBind']]]]}}" bind:closeBind="__e"></bind-phone></block><view class="pro_head"><view class="wrap" style="{{('height:'+minHeight+'px;')}}"><swiper class="slide" style="height:100%;" indicator-active-color="{{baseinfo.base_color2}}" interval="3000" duration="1000" autoplay="{{autoplay}}" indicator-color="rgba(0, 0, 0, .3)" data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" bindchange="__e"><block s-for="{{picList}}" s-for-item="item" s-for-index="index" s-key="index"><block><swiper-item><image class="slide-image" src="{{item}}" width="100%" mode="widthFix" data-event-opts="{{[['load',[['swiperLoad',['$event']]]]]}}" bindload="__e"></image><block s-if="{{!isplay&&pic_video&&index==0}}"><view><image class="play-image" src="{{$imgurl+'play_audio.png'}}" mode="aspectFill" data-event-opts="{{[['tap',[['playvideo',['$event']]]]]}}" bindtap="__e"></image></view></block><block s-if="{{isplay}}"><view style="height:100%;"><image class="play-image1" src="{{$imgurl+'c.png'}}" mode="widthFix" data-event-opts="{{[['tap',[['endvideo',['$event']]]]]}}" bindtap="__e"></image><video style="margin:0;height:100%;width:100%;overflow:hidden;" src="{{pic_video}}" objectFit="cover" autoplay="true"></video></view></block></swiper-item></block></block></swiper><block s-if="{{!isplay}}"><view class="dots hbj" style="{{'width:'+((dlength-1)*14+dlength*14+'rpx')+';'}}"><block s-for="{{picList}}" s-for-item="item" s-for-index="index" s-key="index"><block><view class="{{['dot',index==currentSwiper?'active':'']}}"></view></block></block></view></block></view><block s-if="{{autoplay}}"><view></view></block></view><view class="price"><view class="price1" style="{{('color:'+baseinfo.base_color2)}}"><view>¥</view></view><view class="price2" style="{{('color:'+baseinfo.base_color2)}}"><view>{{datas.sellprice}}</view></view><view class="price3 pline"><view>{{"¥"+datas.marketprice}}</view></view><block s-if="{{datas.pro_kc>=0}}"><view class="price3"><view>{{"库存量："+datas.storage}}</view></view></block><block s-if="{{baseinfo.share_open==1}}"><block><view data-event-opts="{{[['tap',[['open_share',['$event']]]]]}}" class="sale_num" bindtap="__e"><image style="width:34rpx;" src="{{$imgurl+'btn_share.png'}}" mode="widthFix"></image></view></block></block><view class="sale_num"><view>{{"已售："+datas.rsales}}</view></view></view><block s-if="{{autoplay}}"><view class="goods_duo">{{datas.title}}</view></block><view class="spxq"><view class="P_title"><view class="{{['detail_font',pcon,nowcon=='con'?'active':'']}}" style="{{'width:'+('100%')+';'+('color:'+(baseinfo.base_color2)+';')}}" data-id="con" data-event-opts="{{[['tap',[['tabChange',['$event']]]]]}}" bindtap="__e">商品/服务详情<view style="{{'text-align:left;'+('background:'+baseinfo.base_color2)}}" class="_i"></view></view></view><block s-if="{{nowcon=='con'}}"><view class="xqnr"><rich-text nodes="{{datas.descp}}"></rich-text></view></block></view><view class="pro_footer_bg"></view><view class="pro_footer"><view class="pro_f1 pro_f_home"><navigator open-type="redirectTo" url="/pages/index/index"><image src="{{$imgurl+'i_home.png'}}"></image><text>首页</text></navigator></view><view class="pro_f1 pro_f_star" data-name="{{datas.id}}" data-event-opts="{{[['tap',[['collect',['$event']]]]]}}" bindtap="__e"><block s-if="{{sc==0}}"><image src="{{$imgurl+'i_like.png'}}"></image></block><block s-if="{{sc==1}}"><image src="{{$imgurl+'u_star.png'}}"></image></block><text>收藏</text></view><view class="pro_f1 pro_f_tel" data-tel="{{tel}}" data-event-opts="{{[['tap',[['makePhoneCall',['$event']]]]]}}" bindtap="__e"><image src="{{$imgurl+'i_tel.png'}}"></image><text>客服</text></view><view class="pro_f1 pro_f_price" style="{{'background:'+(baseinfo.tabbar_bg3)+';'+('color:'+(baseinfo.tabbar_bg1)+';')}}">{{datas.sellprice+"元"}}</view><view data-gid="{{datas.id}}" data-event-opts="{{[['tap',[['goBuy',['$event']]]]]}}" bindtap="__e"><view class="pro_f1 pro_f_buy" style="{{'background:'+(baseinfo.base_color2)+';'+('color:'+(baseinfo.tabbar_bg2)+';')}}">立即购买</view></view></view><block s-if="{{shareHome==1}}"><navigator class="shareBtnArt shareBtnHome" url="/pages/index/index"><image src="{{$imgurl+'more.png'}}"></image></navigator></block><block s-if="{{share==1}}"><view data-event-opts="{{[['tap',[['share_close',['$event']]]]]}}" class="mask" bindtap="__e"></view></block><block s-if="{{share==1}}"><view class="share_ceng" style="{{'z-index:'+(shareimg==1?'800':1000)+';'}}"><view class="share_con flex-row"><view class="share_con_box"><button class="flex-row" open-type="share" data-event-opts="{{[['tap',[['onShareAppMessage',['$event']]]]]}}" bindtap="__e"><view class="iconfont icon-x-fenxiang5" style="font-size:40rpx;color:#56bb3a;"></view><view style="margin-left:10rpx;color:#333;">分享给好友</view></button></view><view class="share_con_box"><view data-event-opts="{{[['tap',[['getShareImg',['$event']]]]]}}" class="flex-row" style="justify-content:center;" bindtap="__e"><view class="iconfont icon-x-tupian" style="font-size:40rpx;color:#e47b2f;"></view><view style="margin-left:10rpx;">生成分享海报</view></view></view></view></view></block><block s-if="{{shareimg==1}}"><view class="fx_box" style="{{'width:'+(img_w+20+'px')+';'+('height:'+(img_h+25+'px')+';')+('left:'+((system_w-img_w-10)/2+'px')+';')}}"><view class="fx_close"><view data-event-opts="{{[['tap',[['closeShare',['$event']]]]]}}" class="fx_X" bindtap="__e">x</view><view class="fx_content"><image class="haibao_img _img" style="{{'width:'+(img_w+'px')+';'+('height:'+(img_h+'px')+';')}}" src="{{shareimg_url}}"></image></view><button class="haibao_btn" type="primary" data-event-opts="{{[['tap',[['saveImg',['$event']]]]]}}" bindtap="__e">保存图片并分享</button></view></view></block></view></block>