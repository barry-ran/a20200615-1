<block s-if="{{$imgurl}}"><view><view class="active_nav_box"><view class="{{['active_nav',type==1?'active_nav_on':'']}}" data-id="1" data-event-opts="{{[['tap',[['changeTab',['$event']]]]]}}" bindtap="__e"><text>已报名</text></view><view class="{{['active_nav',type==2?'active_nav_on':'']}}" data-id="2" data-event-opts="{{[['tap',[['changeTab',['$event']]]]]}}" bindtap="__e"><text>已收藏</text></view></view><view style="height:86rpx;"></view><block s-if="{{list_length>0}}"><block><block s-for="{{list}}" s-for-item="item" s-for-index="index" s-key="index"><view class="list" data-id="{{item.aid}}" data-event-opts="{{[['tap',[['goactiveinfo',['$event']]]]]}}" bindtap="__e"><view class="top_img"><image class="thumb" src="{{item['thumb']}}" mode="aspectFill"></image></view><view class="active_cons"><view class="title">{{item['name']}}</view><view class="active_time"><view class="iconfont icon-x-shijian"></view><view class="active_times">{{item['activetime']}}</view></view><view class="active_foot"><block s-if="{{type==1}}"><block><block s-if="{{item['flag']==1}}"><block><view class="active_pnum">我已报名，待审核</view></block></block><block s-if="{{item['flag']==2}}"><block><view class="active_pnum">我已报名，待参加</view></block></block><block s-if="{{item['flag']==3}}"><block><view class="active_pnum">活动已完成</view></block></block><block s-if="{{item['flag']==4}}"><block><view class="active_pnum">审核未通过</view></block></block></block></block><block s-else><view class="active_pnum0"><view class="active_peopleimg"><block s-for="{{item.avatar_arr}}" s-for-item="items" s-for-index="key" s-key="key"><block><image src="{{items}}"></image></block></block></view><view style="margin-left:10rpx;"><block s-if="{{item.applynum>6}}"><text>... 等</text></block><text>{{item.applynum==0?'暂无报名':item.applynum+'人已报名'}}</text></view></view></block><view style="flex:1;"></view><block s-if="{{item.t_flag==1}}"><view class="tips_btn tips_btn1">未开始</view></block><block s-if="{{item.t_flag==2}}"><view class="tips_btn tips_btn1">已结束</view></block><block s-if="{{item.t_flag==4}}"><view class="tips_btn tips_btn1">人数已满</view></block><block s-if="{{item.t_flag==3||item.t_flag==0}}"><view class="tips_btn tips_btn2">进行中</view></block></view></view></view></block></block></block><block s-else><block><image class="pageno_address" src="{{$imgurl+'no_active.png'}}"></image><view style="text-align:center;color:#666;">还没有相关活动哟~</view></block></block><myfooter vue-id="1" page_signs="{{page_signs}}" baseinfo="{{baseinfo}}"></myfooter></view></block>