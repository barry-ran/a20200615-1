<block s-if="{{$imgurl}}"><view class="_div"><block s-if="{{needAuth}}"><auth vue-id="1" needAuth="{{needAuth}}" data-event-opts="{{[['^closeAuth',[['closeAuth']]],['^cell',[['cell']]]]}}" bind:closeAuth="__e" bind:cell="__e"></auth></block><block s-if="{{needBind}}"><bind-phone vue-id="2" needBind="{{needBind}}" data-event-opts="{{[['^closeBind',[['closeBind']]]]}}" bind:closeBind="__e"></bind-phone></block><view class="pro-container"><view class="active_content"><view class="wrap" style="{{('height:'+minHeight+'px;')}}"><swiper class="slide" style="height:100%;" indicator-active-color="{{baseinfo.base_color2}}" interval="3000" duration="1000" autoplay="{{autoplay}}" indicator-color="rgba(0, 0, 0, .3)" circular="true" data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" bindchange="__e"><block s-for="{{activeinfo['pics']}}" s-for-item="item" s-for-index="index" s-key="index"><block><swiper-item><image class="slide-image" src="{{item}}" bindload="swiperLoad" width="100%" mode="aspectFill"></image></swiper-item></block></block></swiper><view class="dots hbj" style="{{'width:'+((dlength-1)*14+dlength*14+'rpx')+';'}}"><block s-for="{{activeinfo['pics']}}" s-for-item="item" s-for-index="index" s-key="index"><block><view class="{{[index==currentSwiper?' active':'','dot']}}"></view></block></block></view><block s-if="{{activeinfo.active_endtime}}"><view class="end_time" style="{{(activeinfo.endtime==0?'display:none':'')}}"><view class="end_time_left"><view class="iconfont icon-x-shandian"></view><view class="end_time_text">距报名截止 :</view></view><view class="end_time_white"></view><view class="end_times">{{activeinfo.active_enddate}}<text>{{activeinfo.active_endh}}</text>:<text>{{activeinfo.active_endm}}</text>:<text>{{activeinfo.active_ends}}</text></view></view></block></view><view class="pro_title"><view class="pro_title_top">{{activeinfo.name}}</view><view class="flex-row"><view class="iconfont icon-x-shijian"></view><view class="pro_content_text">{{activeinfo.activetime==''?'暂无时间':activeinfo.activetime}}</view></view><view class="flex-row" style="margin-top:10rpx;align-items:flex-start;"><view class="iconfont icon-x-addre"></view><view class="pro_content_text">{{!activeinfo.address?'地址暂未定':activeinfo.address}}</view></view><view class="flex-row" style="margin-top:10rpx;"><view class="iconfont icon-x-renshu"></view><view class="pro_content_text">{{activeinfo.allperson==0?'无人数限制':'仅限'+activeinfo.allperson+'人'}}</view></view><view data-event-opts="{{[['tap',[['share111',['$event']]]]]}}" class="share_box" bindtap="__e"><text class="iconfont icon-x-fenxiang2"></text><text style="font-size:24rpx;">转发</text></view></view><view class="pro_process"><view data-event-opts="{{[['tap',[['goapplylist',['$event']]]]]}}" class="process_title flex-row" bindtap="__e"><view><block s-for="{{activeinfo.applylist}}" s-for-item="item" s-for-index="index" s-key="index"><block><block s-if="{{index<6}}"><image src="{{item.avatar?item.avatar:$imgurl+'pay_list_person.png'}}"></image></block></block></block><block s-if="{{applylist_count>6}}"><text style="margin-left:20rpx;">...</text></block></view><view>已有<text class="app_count_num">{{activeinfo.applylist_count}}</text>人报名<text class="iconfont icon-x-you"></text></view></view></view></view><view class="pro_detail"><view class="detail_title">活动详情</view><view class="texts"><rich-text nodes="{{texts}}"></rich-text></view></view></view><view class="buybox hbj"><view class="shopcart"><navigator open-type="redirect" url="/pages/index/index"><view class="iconfont icon-x-shouye home_icon"></view><text class="home">首页</text></navigator></view><view class="shopcart" data-name="{{aid}}" data-event-opts="{{[['tap',[['collect',['$event']]]]]}}" bindtap="__e"><block s-if="{{sc==0}}"><view class="iconfont icon-x-shoucang home_icon"></view></block><block s-if="{{sc==1}}"><view class="iconfont icon-x-yishoucang home_icon" style="{{'color:'+(baseinfo.base_color)+';'}}"></view></block><block s-if="{{sc==0}}"><text class="home">收藏</text></block><block s-if="{{sc==1}}"><text class="home">已收藏</text></block></view><view data-event-opts="{{[['tap',[['makePhoneCall',['$event']]]]]}}" class="shopcart" bindtap="__e"><view class="iconfont icon-x-wodekefu home_icon"></view><text class="home">客服</text></view><view class="secon_btn_box"><block s-if="{{(activeinfo.allapply==0||activeinfo.applynum<activeinfo.allapply&&activeinfo.allapply!=0)&&activeinfo.t_flag!=1&&activeinfo.t_flag!=2&&activeinfo.is_apply!=1}}"><view data-event-opts="{{[['tap',[['goApply',['$event']]]]]}}" class="secon_kill_btn" style="{{'background:'+(baseinfo.base_color)+';'}}" bindtap="__e">我要报名</view></block><block s-if="{{activeinfo.applynum>=activeinfo.allapply&&activeinfo.allapply>0||activeinfo.t_flag==1||activeinfo.t_flag==2||activeinfo.is_apply==1}}"><view class="secon_kill_btn"><block s-if="{{activeinfo.is_apply==1}}"><block>我已报名</block></block><block s-else><block s-if="{{activeinfo.t_flag==1}}"><block>报名未开始</block></block><block s-else><block s-if="{{activeinfo.t_flag==2}}"><block>报名已结束</block></block><block s-else><block s-if="{{activeinfo.applynum>=activeinfo.allapply&&activeinfo.allapply>0}}"><block>报名人数已满</block></block></block></block></block></view></block></view></view><block s-if="{{share==1}}"><view data-event-opts="{{[['tap',[['share_close',['$event']]]]]}}" class="mask" bindtap="__e"></view></block><block s-if="{{share==1}}"><view class="share_ceng" style="{{'z-index:'+(shareimg==1?'800':1000)+';'}}"><view class="share_con flex-row"><view class="share_con_box"><button class="flex-row" open-type="share" data-event-opts="{{[['tap',[['onShareAppMessage',['$event']]]]]}}" bindtap="__e"><view class="iconfont icon-x-fenxiang5" style="font-size:40rpx;color:#56bb3a;"></view><view style="margin-left:10rpx;color:#333;">分享给好友</view></button></view><view class="share_con_box"><view data-event-opts="{{[['tap',[['getShareImg',['$event']]]]]}}" class="flex-row" style="justify-content:center;" bindtap="__e"><view class="iconfont icon-x-tupian" style="font-size:40rpx;color:#e47b2f;"></view><view style="margin-left:10rpx;">生成分享海报</view></view></view></view></view></block><block s-if="{{shareimg==1}}"><view class="fx_box" style="{{'width:'+(img_w+20+'px')+';'+('height:'+(img_h-25+'px')+';')+('left:'+((system_w-img_w-10)/2+'px')+';')}}"><view class="fx_close"><view data-event-opts="{{[['tap',[['closeShare',['$event']]]]]}}" class="fx_X" bindtap="__e">x</view><view class="fx_content"><image class="haibao_img _img" style="{{'width:'+(img_w+'px')+';'+('height:'+(img_h+'px')+';')}}" src="{{shareimg_url}}"></image></view><button class="haibao_btn" type="primary" data-event-opts="{{[['tap',[['saveImg',['$event']]]]]}}" bindtap="__e">保存图片并分享</button></view></view></block></view></block>