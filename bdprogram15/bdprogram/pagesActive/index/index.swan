<block s-if="{{$imgurl}}"><view class="_div"><form class="searchBox" report-submit="true" data-event-opts="{{[['submit',[['search',['$event']]]]]}}" bindsubmit="__e"><view class="list_search"><view class="iconfont icon-x-sousuo1 ssk-icon"></view><input class="input-style" type="text" name="keywords" confirm-type="search" placeholder="搜索" placeholder-style="color:#ccc" data-event-opts="{{[['confirm',[['search',['$event']]]],['input',[['serachInput',['$event']]]]]}}" bindconfirm="__e" bindinput="__e"/></view></form><scroll-view class="active_nav_box" scroll-x><view class="{{['active_nav',cateid==0?'active_nav_on':'']}}" data-id="{{0}}" data-event-opts="{{[['tap',[['changeTab',['$event']]]]]}}" bindtap="__e">全部</view><block s-for="{{catelist}}" s-for-item="item" s-for-index="index"><view class="{{['active_nav',cateid==item.id?'active_nav_on':'']}}" data-id="{{item.id}}" data-event-opts="{{[['tap',[['changeTab',['$event']]]]]}}" bindtap="__e">{{item.name}}</view></block></scroll-view><view style="height:176rpx;"></view><block s-if="{{activeList_length>0}}"><view class="active_list"><block s-for="{{activeList}}" s-for-item="item" s-for-index="index" s-key="index"><block><view class="aclist_box" data-id="{{item.id}}" data-event-opts="{{[['tap',[['goactiveinfo',['$event']]]]]}}" bindtap="__e"><view class="active_thumb"><image style="width:100%;" src="{{item.thumb}}" mode="aspectFill"></image><block s-if="{{item.active_endtime}}"><view class="end_time" style="{{(item.active_endtime==0?'display:none':'')}}"><view class="end_time_left"><view class="iconfont icon-x-shandian"></view><view class="end_time_text">距报名截止 :</view></view><view class="end_time_white"></view><view class="end_times">{{activeList[index].active_enddate}}<text>{{activeList[index].active_endh}}</text>:<text>{{activeList[index].active_endm}}</text>:<text>{{activeList[index].active_ends}}</text></view></view></block></view><view class="active_cons"><view class="active_name">{{item.name}}</view><view class="active_time"><view class="iconfont icon-x-shijian"></view><view class="active_times">{{item.activetime}}</view></view><view class="active_foot"><block s-if="{{item.applynum==0}}"><view class="active_pnum">暂无报名</view></block><block s-else><view class="active_pnum0"><view class="active_peopleimg"><block s-for="{{item.avatar_arr}}" s-for-item="items" s-for-index="key" s-key="key"><block><image src="{{items}}"></image></block></block></view><view style="margin-left:10rpx;"><block s-if="{{item.applynum>6}}"><text>... 等</text></block><text>{{item.applynum+"人已报名"}}</text></view></view></block><view style="flex:1;"></view><block s-if="{{item.t_flag==1}}"><view class="tips_btn tips_btn1">未开始</view></block><block s-if="{{item.t_flag==2}}"><view class="tips_btn tips_btn1">已结束</view></block><block s-if="{{item.t_flag==4}}"><view class="tips_btn tips_btn1">人数已满</view></block><block s-if="{{item.t_flag==3||item.t_flag==0}}"><view class="tips_btn tips_btn2">进行中</view></block></view></view></view></block></block></view></block><block s-else><view><image class="pageno_address" src="{{$imgurl+'no_active.png'}}"></image><view style="text-align:center;color:#666;">还没有相关活动哟~</view></view></block><block s-if="{{baseinfo.tabbar_t==1}}"><copyright vue-id="1" baseinfo="{{baseinfo}}"></copyright></block><myfooter vue-id="2" page_signs="{{page_signs}}" baseinfo="{{baseinfo}}"></myfooter></view></block>