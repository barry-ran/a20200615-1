<block s-if="{{$imgurl}}"><view><view><view class="topdata_box"><view class="flex_box"><image class="com_img" src="{{logo}}"></image><view class="com_name">{{pro_name}}</view><view style="flex:1;"></view><view data-event-opts="{{[['tap',[['backIndex',['$event']]]]]}}" class="iconfont icon-tuichu" bindtap="__e"></view></view><view class="flex_box msg_box"><view data-event-opts="{{[['tap',[['orderTip',['$event']]]]]}}" class="msg_item" bindtap="__e"><image class="msg_img" src="{{$imgurl+'ddtx.png'}}"></image><view>订单提醒</view></view><view data-event-opts="{{[['tap',[['shManage',['$event']]]]]}}" class="msg_item" bindtap="__e"><image class="msg_img" src="{{$imgurl+'shgl.png'}}"></image><view>审核管理</view></view><view data-event-opts="{{[['tap',[['moneyApply',['$event']]]]]}}" class="msg_item" bindtap="__e"><image class="msg_img" src="{{$imgurl+'txsq.png'}}"></image><view>提现申请</view></view><view data-event-opts="{{[['tap',[['userManage',['$event']]]]]}}" class="msg_item" bindtap="__e"><image class="msg_img" src="{{$imgurl+'yhgl.png'}}"></image><view>用户管理</view></view></view><view class="xxls_box"><view class="xxls_topname">销售流水</view></view><view class="qiun-charts"><canvas class="charts" style="{{'width:'+(cWidth+'px')+';'+('height:'+(cHeight+'px')+';')}}" canvas-id="canvasLineA" id="canvasLineA" width="{{cWidth*pixelRatio}}" height="{{cHeight*pixelRatio}}" data-event-opts="{{[['touchstart',[['touchLineA',['$event']]]]]}}" bindtouchstart="__e"></canvas></view><view class="xxls_box flex_bbox"><view class="xxls_topname">订单数据</view><view class="more_date"><picker class="flex_bbox" range="{{arr1}}" data-event-opts="{{[['change',[['chooseDate',['$event']]]]]}}" bindchange="__e"><view style="display:inline-block;">{{arr1[index1]}}</view><view class="iconfont icon-x-xia"></view></picker></view></view><view style="margin-bottom:120rpx;"><view class="dddata_item"><view class="data_num">{{dateData[index1].nums}}</view><view class="data_numname">成交量(笔)</view></view><view class="dddata_item"><view class="data_num">{{dateData[index1].money}}</view><view class="data_numname">成交额(元)</view></view><view class="dddata_item" style="margin-right:0;"><view class="data_num">{{dateData[index1].avg}}</view><view class="data_numname">订单平均消费(元)</view></view><view class="dddata_item"><view class="data_num">{{dateData[index1].flag0}}</view><view class="data_numname">待发货订单(笔)</view></view><view class="dddata_item"><view class="data_num">{{dateData[index1].flag1}}</view><view class="data_numname">待核销订单(笔)</view></view><view class="dddata_item" style="margin-right:0;"><view class="data_num">{{dateData[index1].flag2}}</view><view class="data_numname">售后订单(笔)</view></view></view></view></view><bsfooter vue-id="1"></bsfooter></view></block>