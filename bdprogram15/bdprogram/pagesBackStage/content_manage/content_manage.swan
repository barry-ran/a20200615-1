<block s-if="{{$imgurl}}"><view><view class="active_nav_box"><view class="{{['active_nav',type==1?'active_nav_on':'']}}" data-id="1" data-event-opts="{{[['tap',[['changeTab',['$event']]]]]}}" bindtap="__e"><text>文章</text></view><view class="{{['active_nav',type==2?'active_nav_on':'']}}" data-id="2" data-event-opts="{{[['tap',[['changeTab',['$event']]]]]}}" bindtap="__e"><text>组图</text></view></view><view style="height:86rpx;"></view><view><form class="searchBox" report-submit="true"><view class="list_search"><view class="iconfont icon-x-sousuo1 ssk-icon"></view><input class="input-style" type="text" name="keywords" confirm-type="search" placeholder="请输入关键词搜索" placeholder-style="color:#ccc;" data-event-opts="{{[['confirm',[['search',['$event']]]],['input',[['serachInput',['$event']]]]]}}" value="{{searchtitle}}" bindconfirm="__e" bindinput="__e"/></view></form><block s-for="{{artPiclist}}" s-for-item="item" s-for-index="key" s-key="key"><view class="pro_box"><block s-if="{{type==1}}"><view class="pro_box_item"><scroll-view class="scroll_item" scroll-x="true"><view class="flex_box scroll_itembox"><view class="pro_msg"><view class="flex_bbox pro_msg_top"><view style="color:#999999;">{{"发布时间："+item.ctime}}</view><view class="flex_box"><view class="iconfont icon-x-xiaoxi1"></view><view style="margin-right:24rpx;" data-aid="{{item.id}}" data-event-opts="{{[['tap',[['toComment',['$event']]]]]}}" bindtap="__e">评论管理</view><view class="iconfont icon-x-bianji"></view><view data-pro="{{item.id}}" data-event-opts="{{[['tap',[['toEditArt',['$event']]]]]}}" bindtap="__e">编辑</view></view></view><view class="flex_box pro_msg_bottombox"><image class="pro_item_img" src="{{item.thumb}}" mode="aspectFill"></image><view class="flex_box pro_item_msg"><view class="pro_msg_title">{{item.title}}</view><view class="pro_msg_num">{{item.desc==null?'暂无简介':item.desc}}</view><view style="flex:1;"></view><view class="flex_box pro_msg_num"><view class="iconfont icon-x-liulan"></view><view>{{item.hits==null?'0':item.hits}}</view></view></view></view></view><view class="pro_make" data-aid="{{item.id}}" data-event-opts="{{[['tap',[['artdelete',['$event']]]]]}}" bindtap="__e">删除</view></view></scroll-view></view></block><block s-if="{{type==2}}"><view class="pro_box_item"><scroll-view class="scroll_item" scroll-x="true"><view class="flex_box scroll_itembox"><view class="pro_msg"><view class="flex_bbox pro_msg_top"><view style="color:#999999;">{{"发布时间："+item.ctime}}</view><view class="flex_box"><view class="iconfont icon-x-bianji"></view><view data-pro="{{item.id}}" data-event-opts="{{[['tap',[['toEditPic',['$event']]]]]}}" bindtap="__e">编辑</view></view></view><view class="flex_box pro_msg_bottombox"><image class="pro_item_img" src="{{item.thumb}}" mode="aspectFill"></image><view class="flex_box pro_item_msg"><view class="pro_msg_title">{{item.title}}</view><view class="pro_msg_num">{{item.count+"张图"}}</view><view style="flex:1;"></view><view class="flex_box pro_msg_num" style="justify-content:flex-end;"><view class="iconfont icon-x-liulan"></view><view>{{item.hits==null?'0':item.hits}}</view></view></view></view></view><view class="pro_make" data-pid="{{item.id}}" data-event-opts="{{[['tap',[['picdelete',['$event']]]]]}}" bindtap="__e">删除</view></view></scroll-view></view></block></view></block></view><view data-event-opts="{{[['tap',[['addContent',['$event']]]]]}}" class="iconfont icon-x-jia2" bindtap="__e"></view><bsfooter vue-id="1"></bsfooter></view></block>