<block s-if="{{$imgurl}}"><view><view class="active_nav_box"><view class="{{['active_nav',type==1?'active_nav_on':'']}}" data-id="1" data-event-opts="{{[['tap',[['changeTab',['$event']]]]]}}" bindtap="__e"><text>用户</text></view><view class="{{['active_nav',type==2?'active_nav_on':'']}}" data-id="2" data-event-opts="{{[['tap',[['changeTab',['$event']]]]]}}" bindtap="__e"><text>分销商</text></view></view><view style="height:86rpx;"></view><view><form class="searchBox" report-submit="true"><view class="list_search"><view class="iconfont icon-x-sousuo1 ssk-icon"></view><input class="input-style" type="text" name="keywords" confirm-type="search" placeholder="请输入手机号搜索" placeholder-style="color:#ccc;" data-event-opts="{{[['confirm',[['search',['$event']]]],['input',[['serachInput',['$event']]]]]}}" value="{{searchtitle}}" bindconfirm="__e" bindinput="__e"/></view></form><block s-for="{{userslist}}" s-for-item="item" s-for-index="keys" s-key="keys"><view class="pro_box"><view class="pro_boxitems"><view class="flex_box" style="align-items:flex-start;"><image class="users_img" src="{{item.avatar}}" mode="aspectFill"></image><view><view class="users_name flex_box"><view class="users_nameitem">{{item.nickname}}</view><block s-if="{{type==1&&item.grade!=0}}"><view class="vip_user"><text class="iconfont icon-x-huiyuan"></text>{{item.vipname}}</view></block><view class="users_tel">{{item.phone}}</view></view><block s-if="{{type==1}}"><block><view class="users_tx"><text class="users_money">余额</text><text>{{"￥"+item.money}}</text></view><view class="users_tx"><text class="users_money">积分</text><text>{{item.score}}</text></view></block></block><block s-if="{{type==2}}"><block><view class="users_tx"><text class="users_money">累计佣金</text><text>{{"￥"+item.fx_allmoney}}</text></view><view class="users_tx"><text class="users_money">可提现佣金</text><text>{{"￥"+item.fx_getmoney}}</text></view></block></block></view></view><block s-if="{{type==1}}"><view class="flex_box admin_btns"><view class="user_no" data-id="{{item.id}}" data-event-opts="{{[['tap',[['userDelete',['$event']]]]]}}" bindtap="__e">删除</view><view class="user_yes" data-id="{{item.id}}" data-event-opts="{{[['tap',[['rechargeMoney',['$event']]]]]}}" bindtap="__e">充值</view></view></block></view></view></block></view><block s-if="{{rechargeBox==1}}"><view class="add_ggbox"><view class="add_ggmsg"><view class="add_msg_top"><view class="{{['active_recharge',typeRecharge==1?'recharge_on':'']}}" data-id="1" data-event-opts="{{[['tap',[['changeRechargeTab',['$event']]]]]}}" bindtap="__e"><text>积分</text></view><view class="{{['active_recharge',typeRecharge==2?'recharge_on':'']}}" data-id="2" data-event-opts="{{[['tap',[['changeRechargeTab',['$event']]]]]}}" bindtap="__e"><text>余额</text></view></view><view class="add_content"><view class="flex_box"><view class="rechargeType">类型</view><view class="flex_box"><view class="{{['type_add',typeOpe==1?'add_on':'']}}" data-id="1" data-event-opts="{{[['tap',[['changeOpe',['$event']]]]]}}" bindtap="__e">增加</view><view class="{{['type_add',typeOpe==2?'add_on':'']}}" data-id="2" data-event-opts="{{[['tap',[['changeOpe',['$event']]]]]}}" bindtap="__e">扣除</view></view></view><view class="flex_box money_change"><block s-if="{{typeRecharge==1}}"><view class="rechargeType">积分</view></block><block s-if="{{typeRecharge==2}}"><view class="rechargeType">金额</view></block><view><input type="number" value="" placeholder="请填写数值" placeholder-style="color:#cccccc;font-size:24rpx;" data-event-opts="{{[['input',[['jfMoney',['$event']]]]]}}" bindinput="__e"/></view></view></view><view class="flex_box add_msgbtn"><view data-event-opts="{{[['tap',[['closeRecharge',['$event']]]]]}}" class="close_add" bindtap="__e">取消</view><view data-event-opts="{{[['tap',[['tjRecharge',['$event']]]]]}}" class="sure_add" bindtap="__e">确定</view></view></view></view></block></view></block>