<block s-if="{{$imgurl}}"><view><view class="sh_box"><view class="user_msg_box"><view class="user_topbox"><block s-if="{{type==1}}"><block><image class="user_img" src="{{money_info.avatar}}" mode="aspectFill"></image><view class="user_name">{{money_info.nickname}}</view></block></block><block s-if="{{type==2}}"><block><image class="user_img" src="{{money_info.logo}}" mode="aspectFill"></image><view class="user_name">{{money_info.shopname}}</view></block></block><view class="flex_box sqxx"><view>————</view><view class="sqxx_text">申请信息</view><view>————</view></view></view><view class="flex_box user_msgitem"><view class="msg_itemleft">姓名 :</view><view>{{money_info.nickname}}</view></view><view class="flex_box user_msgitem"><view class="msg_itemleft">电话 :</view><view>{{money_info.phone}}</view></view><view class="flex_box user_msgitem"><view class="msg_itemleft">提现金额 :</view><view>{{money_info.money}}</view></view><view class="flex_box user_msgitem"><view class="msg_itemleft">提现方式 :</view><block s-if="{{type==1}}"><block><block s-if="{{money_info.types==1}}"><view>提现到余额</view></block><block s-if="{{money_info.types==2}}"><view>提现到微信</view></block><block s-if="{{money_info.types==3}}"><view>提现到支付宝</view></block><block s-if="{{money_info.types==4}}"><view>提现到银行卡</view></block></block></block><block s-if="{{type==2}}"><block><block s-if="{{money_info.types==1}}"><view>提现到微信</view></block><block s-if="{{money_info.types==2}}"><view>提现到支付宝</view></block><block s-if="{{money_info.types==3}}"><view>提现到银行卡</view></block></block></block></view><block s-if="{{type==1}}"><block><block s-if="{{money_info.types==3||money_info.types==4}}"><view class="flex_box user_msgitem"><view class="msg_itemleft">账户信息 :</view><block s-if="{{money_info.types==3}}"><view>{{money_info.zfbxm+"-"+money_info.zfbzh}}</view></block><block s-if="{{money_info.types==4}}"><view>{{money_info.yhkuser+"-"+money_info.yhkcard}}</view></block></view></block></block></block><block s-if="{{type==2}}"><block><view class="flex_box user_msgitem"><view class="msg_itemleft">账户信息 :</view><view>{{money_info.account}}</view></view></block></block><view class="flex_box user_msgitem"><view class="msg_itemleft">申请时间 :</view><view>{{money_info.createtime}}</view></view><block s-if="{{type==2&&money_info.beizhu!=''}}"><view class="flex_box user_msgitem"><view class="msg_itemleft">备注 :</view><view>{{money_info.beizhu}}</view></view></block><block s-if="{{type==1}}"><block><block s-if="{{money_info.flag==1}}"><view class="flex_bbox sh_btns"><view data-event-opts="{{[['tap',[['userPass',['$event']]]]]}}" class="sh_passbtn" bindtap="__e">通过</view><view data-event-opts="{{[['tap',[['userRefuse',['$event']]]]]}}" class="sh_refusebtn" bindtap="__e">拒绝</view></view></block></block></block><block s-if="{{type==2}}"><block><block s-if="{{money_info.flag==0}}"><view class="flex_bbox sh_btns"><view data-event-opts="{{[['tap',[['userPass',['$event']]]]]}}" class="sh_passbtn" bindtap="__e">通过</view><view data-event-opts="{{[['tap',[['userRefuse',['$event']]]]]}}" class="sh_refusebtn" bindtap="__e">拒绝</view></view></block></block></block></view></view></view></block>