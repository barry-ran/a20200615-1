<block s-if="{{$imgurl}}"><view><view class="active_nav_box"><view class="{{['active_nav',type==1?'active_nav_on':'']}}" data-id="1" data-event-opts="{{[['tap',[['changeTab',['$event']]]]]}}" bindtap="__e"><text>{{"好评("+comnum1+")"}}</text></view><view class="{{['active_nav',type==2?'active_nav_on':'']}}" data-id="2" data-event-opts="{{[['tap',[['changeTab',['$event']]]]]}}" bindtap="__e"><text>{{"中评("+comnum2+")"}}</text></view><view class="{{['active_nav',type==3?'active_nav_on':'']}}" data-id="3" data-event-opts="{{[['tap',[['changeTab',['$event']]]]]}}" bindtap="__e"><text>{{"差评("+comnum3+")"}}</text></view></view><view style="height:86rpx;"></view><view class="comment_box"><block s-for="{{evaluatelist}}" s-for-item="item" s-for-index="keys" s-key="keys"><view class="pro_box"><view class="pro_box_item"><view class="flex_bbox"><view class="flex_box"><image class="user_img" src="{{item.avatar}}" mode="aspectFill"></image><view class="user_name">{{item.nickname}}</view></view><view class="user_pltime">{{item.creattime}}</view></view><view class="user_content">{{item.content}}</view><block s-if="{{item.imgs!=''}}"><view class="pl_imgbox"><block s-for="{{item.imgs}}" s-for-item="ite" s-for-index="key" s-key="key"><block><block s-if="{{key<3}}"><block><image class="pl_imgs" src="{{ite}}" mode="aspectFill"></image></block></block></block></block></view></block><view class="plbtn_box"><view class="plbtn1" data-plid="{{item.id}}" data-event-opts="{{[['tap',[['getdelete',['$event']]]]]}}" bindtap="__e">删除</view><view class="plbtn2" data-plid="{{item.id}}" data-event-opts="{{[['tap',[['toReply',['$event']]]]]}}" bindtap="__e">查看详情</view></view></view></view></block></view></view></block>