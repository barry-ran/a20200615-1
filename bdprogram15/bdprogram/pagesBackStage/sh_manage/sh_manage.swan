<block s-if="{{$imgurl}}"><view><view class="active_nav_box"><view class="{{['active_nav',type==1?'active_nav_on':'']}}" data-id="1" data-event-opts="{{[['tap',[['changeTab',['$event']]]]]}}" bindtap="__e"><text>会员</text></view><view class="{{['active_nav',type==2?'active_nav_on':'']}}" data-id="2" data-event-opts="{{[['tap',[['changeTab',['$event']]]]]}}" bindtap="__e"><text>分销商</text></view><view class="{{['active_nav',type==3?'active_nav_on':'']}}" data-id="3" data-event-opts="{{[['tap',[['changeTab',['$event']]]]]}}" bindtap="__e"><text>入驻商户</text></view></view><view style="height:86rpx;"></view><view class="sh_box"><view class="sh_userlist"><block s-for="{{reviewlist}}" s-for-item="item" s-for-index="keys" s-key="keys"><view class="sh_users flex_bbox"><view class="flex_box" data-adid="{{item.id}}" data-event-opts="{{[['tap',[['toShDetail',['$event']]]]]}}" bindtap="__e"><block s-if="{{type!=3}}"><image class="users_img" src="{{item.avatar}}" mode="aspectFill"></image></block><block s-if="{{type==3}}"><image class="users_img" src="{{item.logo}}" mode="aspectFill"></image></block><view><block s-if="{{type==1}}"><view class="users_name">{{item.nickname}}</view></block><block s-if="{{type==2}}"><view class="users_name">{{item.truename}}</view></block><block s-if="{{type==3}}"><view class="users_name">{{item.name}}</view></block><block s-if="{{type==1}}"><view class="users_phone">{{item.applytime}}</view></block><block s-if="{{type==2}}"><view class="users_phone">{{item.truetel}}</view></block><block s-if="{{type==3}}"><view class="users_phone">{{item.tel}}</view></block></view></view><block s-if="{{type==1}}"><block><block s-if="{{item.flag==3}}"><view class="flex_box admin_btns"><view class="user_no" data-adid="{{item.id}}" data-event-opts="{{[['tap',[['userRefuse',['$event']]]]]}}" bindtap="__e">拒绝</view><view>|</view><view class="user_yes" data-adid="{{item.id}}" data-event-opts="{{[['tap',[['userPass',['$event']]]]]}}" bindtap="__e">通过</view></view></block><block s-if="{{item.flag==1||item.flag==4}}"><view><image class="result_img" src="{{$imgurl+'sh_apply.png'}}"></image></view></block><block s-if="{{item.flag==2}}"><view><image class="result_img" src="{{$imgurl+'sh_refuse.png'}}"></image></view></block></block></block><block s-if="{{type==2}}"><block><block s-if="{{item.flag==1}}"><view class="flex_box admin_btns"><view class="user_no" data-adid="{{item.id}}" data-event-opts="{{[['tap',[['userRefuse2',['$event']]]]]}}" bindtap="__e">拒绝</view><view>|</view><view class="user_yes" data-adid="{{item.id}}" data-event-opts="{{[['tap',[['userPass',['$event']]]]]}}" bindtap="__e">通过</view></view></block><block s-if="{{item.flag==2}}"><view><image class="result_img" src="{{$imgurl+'sh_apply.png'}}"></image></view></block><block s-if="{{item.flag==3}}"><view><image class="result_img" src="{{$imgurl+'sh_refuse.png'}}"></image></view></block></block></block><block s-if="{{type==3}}"><block><block s-if="{{item.status==0}}"><view class="flex_box admin_btns"><view class="user_no" data-adid="{{item.id}}" data-event-opts="{{[['tap',[['userRefuse2',['$event']]]]]}}" bindtap="__e">拒绝</view><view>|</view><view class="user_yes" data-adid="{{item.id}}" data-event-opts="{{[['tap',[['userPass',['$event']]]]]}}" bindtap="__e">通过</view></view></block><block s-if="{{item.status==1}}"><view><image class="result_img" src="{{$imgurl+'sh_apply.png'}}"></image></view></block><block s-if="{{item.status==2}}"><view><image class="result_img" src="{{$imgurl+'sh_refuse.png'}}"></image></view></block></block></block></view></block></view></view><block s-if="{{refuseBox==1}}"><view class="add_ggbox"><view class="add_ggmsg"><view class="add_msg_top">审核拒绝</view><view class="add_content"><textarea placeholder="请填写拒绝原因..." placeholder-style="color:#cccccc" data-event-opts="{{[['input',[['refuseCont',['$event']]]]]}}" bindinput="__e"></textarea></view><view class="flex_box add_msgbtn"><view data-event-opts="{{[['tap',[['closeRefusebox',['$event']]]]]}}" class="cl