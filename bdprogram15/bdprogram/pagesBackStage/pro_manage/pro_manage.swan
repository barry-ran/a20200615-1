<block s-if="{{$imgurl}}"><view><view class="active_nav_box"><view class="{{['active_nav',type==1?'active_nav_on':'']}}" data-id="1" data-event-opts="{{[['tap',[['changeTab',['$event']]]]]}}" bindtap="__e"><text>出售中</text></view><view class="{{['active_nav',type==2?'active_nav_on':'']}}" data-id="2" data-event-opts="{{[['tap',[['changeTab',['$event']]]]]}}" bindtap="__e"><text>已售罄</text></view><view class="{{['active_nav',type==3?'active_nav_on':'']}}" data-id="3" data-event-opts="{{[['tap',[['changeTab',['$event']]]]]}}" bindtap="__e"><text>未上架</text></view></view><view style="height:86rpx;"></view><view><form class="searchBox" report-submit="true"><view class="list_search"><view class="iconfont icon-x-sousuo1 ssk-icon"></view><input class="input-style" type="text" name="keywords" confirm-type="search" placeholder="请输入关键词搜索" placeholder-style="color:#ccc;" data-event-opts="{{[['confirm',[['search',['$event']]]],['input',[['serachInput',['$event']]]]]}}" value="{{searchtitle}}" bindconfirm="__e" bindinput="__e"/></view></form><block s-for="{{goodslist}}" s-for-item="item" s-for-index="keys" s-key="keys"><view class="pro_box"><view class="pro_box_item"><scroll-view class="scroll_item" scroll-x="true"><view class="flex_box scroll_itembox" style="{{'width:'+(type==3?'880rpx':'810rpx')+';'}}"><view class="pro_msg"><view class="flex_bbox pro_msg_top"><view style="color:#999999;">{{item.ctime?'发布时间：'+item.ctime:''}}</view><view class="flex_box"><view class="iconfont icon-x-xiaoxi1"></view><view style="margin-right:24rpx;" data-pro="{{item.id}}" data-event-opts="{{[['tap',[['toComment',['$event']]]]]}}" bindtap="__e">评论管理</view><view class="iconfont icon-x-bianji"></view><view data-pro="{{item.id}}" data-event-opts="{{[['tap',[['toEdit',['$event']]]]]}}" bindtap="__e">编辑</view></view></view><view class="flex_bbox pro_msg_bottom"><view class="flex_box pro_msg_bottombox"><image class="pro_item_img" src="{{item.thumb}}" mode="aspectFill"></image><view class="pro_item_msg"><view class="pro_msg_title">{{item.title}}</view><view class="flex_bbox pro_msg_num"><view>{{"库存"+item.kc}}</view><view>{{"销量"+item.xsl}}</view></view></view></view><view class="pro_msg_price">{{"￥"+item.price}}</view></view></view><block s-if="{{type!=3}}"><view class="pro_make" data-pro="{{item.id}}" data-event-opts="{{[['tap',[['getdown',['$event']]]]]}}" bindtap="__e">下架</view></block><block s-if="{{type==3}}"><view class="pro_make" data-pro="{{item.id}}" data-event-opts="{{[['tap',[['getsale',['$event']]]]]}}" bindtap="__e">上架</view></block><block s-if="{{type==3}}"><view class="pro_make_del" data-pro="{{item.id}}" data-event-opts="{{[['tap',[['getdelete',['$event']]]]]}}" bindtap="__e">删除</view></block></view></scroll-view></view></view></block></view><view data-event-opts="{{[['tap',[['addPro',['$event']]]]]}}" class="iconfont icon-x-jia2" bindtap="__e"></view><bsfooter vue-id="1"></bsfooter></view></block>