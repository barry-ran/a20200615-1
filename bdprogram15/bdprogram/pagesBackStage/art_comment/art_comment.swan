<block s-if="{{$imgurl}}"><view><view class="active_nav_box"><view class="{{['active_nav',type==1?'active_nav_on':'']}}" data-id="1" data-event-opts="{{[['tap',[['changeTab',['$event']]]]]}}" bindtap="__e"><text>未审核</text></view><view class="{{['active_nav',type==2?'active_nav_on':'']}}" data-id="2" data-event-opts="{{[['tap',[['changeTab',['$event']]]]]}}" bindtap="__e"><text>已审核</text></view></view><view style="height:86rpx;"></view><view class="comment_box"><block s-if="{{commentlist.length>0}}"><block><block s-for="{{commentlist}}" s-for-item="item" s-for-index="keys" s-key="keys"><view class="pro_box"><view class="pro_box_item"><view class="flex_bbox"><view class="flex_box"><image class="user_img" src="{{item.avatar}}" mode="aspectFill"></image><view class="user_name">{{item.nickname}}</view></view><view class="user_pltime">{{item.ctime}}</view></view><view class="user_content">{{item.text}}</view><view class="sh_type_img"><block s-if="{{item.flag==1}}"><image src="{{$imgurl+'sh_apply.png'}}"></image></block><block s-if="{{item.flag==2}}"><image src="{{$imgurl+'sh_refuse.png'}}"></image></block></view><view class="plbtn_box"><block s-if="{{type==2}}"><view class="plbtn1" data-plid="{{item.id}}" data-event-opts="{{[['tap',[['getdelete',['$event']]]]]}}" bindtap="__e">删除</view></block><block s-if="{{type==1}}"><view class="plbtn2" data-plid="{{item.id}}" data-event-opts="{{[['tap',[['getRefuse',['$event']]]]]}}" bindtap="__e">拒绝</view></block><block s-if="{{type==1}}"><view class="plbtn3" data-plid="{{item.id}}" data-event-opts="{{[['tap',[['getPass',['$event']]]]]}}" bindtap="__e">通过</view></block></view></view></view></block></block></block><block s-if="{{commentlist.length==0}}"><block><image class="pageno_address" src="{{$imgurl+'no_active.png'}}"></image><view style="text-align:center;color:#666;">还没有相关内容哟~</view></block></block></view></view></block>