<block s-if="{{$imgurl}}"><view class="_div"><block s-for="{{staffs}}" s-for-item="item" s-for-index="index" s-key="index"><block><view class="card_listbox" data-text="{{item.id}}" data-event-opts="{{[['tap',[['staffcard',['$event']]]]]}}" bindtap="__e"><view data-event-opts="{{[['tap',[['',['$event']]]]]}}" class="card_list_head hbj" catchtap="__e"><block s-if="{{item.pic==''}}"><block><image class="card_list_head_left" src="{{$imgurl+'default_pic.png'}}" mode="aspectFill"></image></block></block><block s-else><block><image class="card_list_head_left" src="{{item.pic}}" mode="aspectFill"></image></block></block><view class="card_list_head_center">{{item.realname}}<text>{{item.title+"-"+item.job}}</text></view><image class="card_list_head_right" src="{{$imgurl+'card_ewm.png'}}" mode="aspectFill" data-text="{{item.id}}" data-event-opts="{{[['tap',[['sharestaffcard',['$event']]]]]}}" bindtap="__e"></image></view><view class="card_list_content"><view class="card_list_content_single">手机<text>{{item.mobile}}</text></view><block s-if="{{item.wxnumber==null||item.wxnumber==''}}"><block><view class="card_list_content_single">微信<text>{{item.mobile}}</text></view></block></block><block s-else><block><view class="card_list_content_single">微信<text>{{item.wxnumber}}</text></view></block></block><block s-if="{{item.email!=null&&item.email!=''}}"><block><view class="card_list_content_single">邮箱<text>{{item.email}}</text></view></block></block><view class="card_list_link"><image src="{{$imgurl+'card_list_yjt.png'}}" mode="aspectFill"></image></view></view></view></block></block><myfooter vue-id="1" page_signs="{{page_signs}}" baseinfo="{{baseinfo}}"></myfooter></view></block>