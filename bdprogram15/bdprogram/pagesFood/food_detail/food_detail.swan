<block s-if="{{$imgurl}}"><view class="_div"><block s-if="{{needAuth}}"><auth vue-id="1" needAuth="{{needAuth}}" data-event-opts="{{[['^closeAuth',[['closeAuth']]],['^cell',[['cell']]]]}}" bind:closeAuth="__e" bind:cell="__e"></auth></block><block s-if="{{needBind}}"><bind-phone vue-id="2" needBind="{{needBind}}" data-event-opts="{{[['^closeBind',[['closeBind']]]]}}" bind:closeBind="__e"></bind-phone></block><view><view class="head_img"><image src="{{foodInfo.thumb}}" mode="scaleToFill"></image></view><view class="detail_pro"><view class="pro_title">{{foodInfo.title}}</view><view class="pro_sales"><view class="count">{{"售量"+foodInfo.saleCount+"份"}}</view><view>{{"剩余"+foodInfo.kc+"份"}}</view></view><view class="pro_price"><view style="color:red;font-size:40rpx;">{{"￥"+foodInfo.price}}</view><view><block s-if="{{guige_arr}}"><button data-event-opts="{{[['tap',[['showGuige',['$event']]]]]}}" style="background:#0061CE;" bindtap="__e">选择规格</button></block></view></view></view><view style="width:100%;height:6rpx;background:#f5f5f5;"></view><view class="detail_description"><view class="description">商品描述</view><rich-text nodes="{{foodInfo.product_txt}}"></rich-text></view></view><block s-if="{{guige_show==1}}"><view class="zhezhao"><view class="guige"><view class="guige_title">{{foodInfo.title}}</view><image style="width:40rpx;height:40rpx;position:absolute;top:20rpx;right:20rpx;background-size:100%;" src="{{$imgurl+'delete.png'}}" mode data-event-opts="{{[['tap',[['closeGuige',['$event']]]]]}}" bindtap="__e"></image><view class="guige_main"><block s-for="{{guige_arr}}" s-for-item="item" s-for-index="index" s-key="index"><view><view>{{index+"："}}</view><view class="gg"><block s-for="{{item.val}}" s-for-item="itemsub" s-for-index="indexsub" s-key="indexsub"><block><view style="{{(indexsub==item.ck?'color:#ffffff;background:'+baseinfo.base_color2:'')}}" data-topindex="{{index}}" data-subindex="{{indexsub}}" data-event-opts="{{[['tap',[['chooseed',['$event']]]]]}}" bindtap="__e">{{itemsub}}</view></block></block></view></view></block></view><view class="guige_foot"><view class="foot_price"><text style="color:red;">￥<text style="font-size:40rpx;">{{realPay}}</text></text><view style="font-size:20rpx;margin-left:12rpx;display:inline-block;vertical-align:middle;text-align:center;">(<text style="font-size:20rpx;vertical-align:middle;max-width:160rpx;display:inline-block;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;">{{guige_chooseed}}</text>)</view></view><view><block s-if="{{gwcHidden==2}}"><button data-event-opts="{{[['tap',[['addGwc',['$event']]]]]}}" style="background:#0061CE;" bindtap="__e"><text class="iconfont icon-x-jia" style="margin-right:10rpx;"></text><text style="white-space:nowrap;">加入购物车</text></button></block><block s-if="{{gwcHidden==1}}"><button style="background:gray;"><text class="iconfont icon-x-jia" style="margin-right:10rpx;"></text><text style="white-space:nowrap;">加入购物车</text></button></block></view></view></view></view></block><view data-event-opts="{{[['tap',[['showShopCartFn',['$event']]]]]}}" class="{{['prompt',showShopCart&&totalPay>0?'':'hidden']}}" bindtap="__e"><view class="food_order"><view data-event-opts="{{[['tap',[['',['$event']]]]]}}" class="order_title" catchtap="__e"><view class="tilte_left"><text>已选商品</text></view><view data-event-opts="{{[['tap',[['clearGwc',['$event']]]]]}}" class="tilte_right" bindtap="__e"><text class="iconfont icon-x-shanchu" style="margin-right:6rpx;"></text><text>清空购物车</text></view></view><scroll-view style="max-height:257px;" scroll-y><block s-for="{{chooseGoodArr}}" s-for-item="good" s-for-index="index" s-key="index"><view class="order_list"><view class="orderlist_title">{{good[2]}}</view>[<text class="orderlist_guige">{{good[5]}}</text>]<view class="food_list"><text style="font-size:38rpx;vertical-align:middle;margin-right:20rpx;">{{"￥"+good[1]}}</text><view data-event-opts="{{[['tap',[['',['$event']]]]]}}" class="cartcontrol" style="display:inline-block;vertical-align:middle;" catchtap="__e"><view class="cart-decrease" data-index="{{index}}" data-event-opts="{{[['tap',[['decreaseGoodToCartFn',['$event']]]]]}}" bindtap="__e"><image class="cart-decrease" src="{{$imgurl+'dpsp_subtract.png'}}"></image><view class="inner icon-remove_circle_outline"></view></view><text class="cart-count">{{good[3]}}</text><image class="cart-add icon-add_circle" data-index="{{index}}" src="{{$imgurl+'dpsp_add.png'}}" data-event-opts="{{[['tap',[['addGoodToCartFn',['$event']]]]]}}" bindtap="__e"></image></view></view></view></block></scroll-view></view></view><view style="height:86px;"></view><view class="shopcart"><view class="content"><view data-event-opts="{{[['tap',[['showShopCartFn',['$event']]]]]}}" class="content-left" bindtap="__e"><view class="logo-wrapper"><image class="logo" src="{{totalNum>0?$imgurl+'icon_selected.png':$imgurl+'icon_normal.png'}}"></image><view class="icon-shopping_cart _i"></view><block s-if="{{totalNum}}"><view class="num" style="{{('background:'+baseinfo.base_color2)}}">{{totalNum}}</view></block></view><view class="{{['price',totalPay>0?'highlight':'']}}">{{"￥"+totalPay}}</view></view><view class="content-right"><block s-if="{{totalPay>0}}"><view data-event-opts="{{[['tap',[['goPayFn',['$event']]]]]}}" class="pay payClass" style="{{('background:'+baseinfo.base_color2)}}" bindtap="__e">去结算</view></block><block s-if="{{totalPay==0}}"><view class="pay" style="{{('background:'+baseinfo.base_color2)}}">去结算</view></block></view></view></view></view></block>