<block s-if="{{$imgurl}}"><view class="_div"><view style="padding-bottom:100rpx;"><view class="top_bigtab"><view class="top_bgcolor" style="{{'background:'+(baseinfo.base_color)+';'}}"></view><view class="tab_box"><view class="tab_show" style="{{'border-radius:'+('10rpx')+';'+('background-image:'+('url('+$imgurl+'tabbottom.png)')+';')}}"><view class="flex-row tab_show1"><view class="iconfont icon-x-dianhua6 icon_tabs"></view><view style="width:83%;"><view style="color:#232323;font-size:26rpx;line-height:60rpx;">预留电话</view><view class="gmadmin_info"><input type="number" name="default_concat" placeholder="请输入预留电话" placeholder-style="color:#969696" data-event-opts="{{[['input',[['concat_input',['$event']]]]]}}" value="{{default_concat}}" bindinput="__e"/></view></view></view></view></view></view><view class="baoyou_box"><view class="dd_infos flex-row"><view class="dd_infoimg"><image src="{{info.thumb}}" mode="aspectFill"></image></view><view class="flex-row" style="flex-flow:column;width:500rpx;"><view class="dd_protitle">{{info.name}}</view><view class="dd_proguige">{{info.type==1?info.money:info.pros_val}}</view></view></view></view><view class="baoyou_box"><view class="dd_infos"><view class="price_items flex_bflow"><view>价格</view><view>{{"￥"+info.price}}</view></view><view class="price_items flex_bflow"><view>数量</view><view>{{num}}</view></view><view class="price_all flex_bflow"><view class="price_all_text">总计</view><view class="price_all_price"><text>￥</text>{{paymoney}}</view></view></view></view></view><view class="foot_js flex_bflow"><view class="js_num">{{"共"+num+"件，实付金额："}}<text class="js_price">￥</text><text class="js_price" style="font-size:28rpx;">{{paymoney}}</text></view><view data-event-opts="{{[['tap',[['paybox',['$event']]]]]}}" class="js_btn" style="{{'background:'+(baseinfo.base_color)+';'}}" bindtap="__e">提交订单</view></view><block s-if="{{showPay==1}}"><block><view data-event-opts="{{[['tap',[['payboxclose',['$event']]]]]}}" class="mask" bindtap="__e"></view><view class="pay_box"><view class="pay_function"><view>选择支付方式</view><view data-event-opts="{{[['tap',[['payboxclose',['$event']]]]]}}" class="iconfont icon-x-guanbi bsyyyd" bindtap="__e"></view></view><view class="pay_funs flex-row"><view class="pay_funsimg"><image src="{{$imgurl+'yue.png'}}" mode="aspectFit"></image></view><view class="pay_moneys flex_bflow"><view>{{"余额支付（剩余：￥"+mymoney+"元）"}}</view><block s-if="{{mymoney_pay==1}}"><view class="{{['iconfont icon-x-gou',choosepayf==0?'choose_jf':'no_jf']}}" data-pay_type="{{1}}" data-type="{{0}}" data-event-opts="{{[['tap',[['choosepay',['$event']]]]]}}" bindtap="__e"></view></block></view><block s-if="{{mymoney_pay==2}}"><view class="ye_less"></view></block></view><block><view class="pay_funs flex-row"><view class="pay_funsimg"><image src="{{$imgurl+'baidu.png'}}" mode="aspectFit"></image></view><view class="pay_moneys flex_bflow"><view>百度支付</view><view class="{{['iconfont icon-x-gou',choosepayf==4?'choose_jf':'no_jf']}}" data-pay_type="{{2}}" data-type="{{4}}" data-event-opts="{{[['tap',[['choosepay',['$event']]]]]}}" bindtap="__e"></view></view></view></block><form report-submit="true" data-event-opts="{{[['submit',[['pay',['$event']]]]]}}" bindsubmit="__e"><block s-if="{{is_submit==1}}"><button class="pay_submit" formType="submit">确定</button></block><block s-if="{{is_submit==2}}"><button class="pay_submit" style="background:gray;" formType="submit">下单中...</button></block></form></view></block></block></view></block>