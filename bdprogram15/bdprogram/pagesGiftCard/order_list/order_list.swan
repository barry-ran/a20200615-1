<block s-if="{{$imgurl}}"><view class="_div"><view class="order_nav"><view class="{{['order_nav_single',flag==0?'order_nav_single_on':'']}}" data-flag="0" data-event-opts="{{[['tap',[['changflag',['$event']]]]]}}" bindtap="__e">全部</view><view class="{{['order_nav_single',flag==1?'order_nav_single_on':'']}}" data-flag="1" data-event-opts="{{[['tap',[['changflag',['$event']]]]]}}" bindtap="__e">待发货</view><view class="{{['order_nav_single',flag==2?'order_nav_single_on':'']}}" data-flag="2" data-event-opts="{{[['tap',[['changflag',['$event']]]]]}}" bindtap="__e">待收货</view><view class="{{['order_nav_single',flag==3?'order_nav_single_on':'']}}" data-flag="3" data-event-opts="{{[['tap',[['changflag',['$event']]]]]}}" bindtap="__e">待核销</view><view class="{{['order_nav_single',flag==4?'order_nav_single_on':'']}}" data-flag="4" data-event-opts="{{[['tap',[['changflag',['$event']]]]]}}" bindtap="__e">待评价</view></view><view style="padding-top:80rpx;"><block s-if="{{list.length>0}}"><block><form class="searchBox" report-submit="true"><view class="list_search"><view class="iconfont icon-x-sousuo1 ssk-icon"></view><input class="input-style" type="text" name="keywords" confirm-type="search" placeholder="搜索关键词" placeholder-style="color:#ccc" data-event-opts="{{[['confirm',[['search',['$event']]]],['input',[['serachInput',['$event']]]]]}}" value="{{searchkeyword}}" bindconfirm="__e" bindinput="__e"/></view></form><view class="dd_lists"><block s-for="{{list}}" s-for-item="item" s-for-index="index" s-key="index"><view class="order_list_single"><block><navigator url="{{'../order_detail/order_detail?order_id='+item.order_id}}"><view class="order_list_head hbj"><view class="order_list_head_left flex1">{{"订单号："+item.order_id}}</view><block s-if="{{item.status==1}}"><view class="order_list_head_right">待发货</view></block><block s-if="{{item.status==2}}"><view class="order_list_head_right">卖家已发货</view></block><block s-if="{{item.status==3}}"><view class="order_list_head_right">待核销</view></block><block s-if="{{item.status==4&&item.assess==1}}"><view class="order_list_head_right">待评价</view></block><block s-if="{{item.status==4&&item.assess==2}}"><view class="order_list_head_right">待追评</view></block><block s-if="{{item.status==5}}"><view class="order_list_head_right">交易成功</view></block></view><block><view class="order_list_productbox"><view class="order_list_product flex-row"><image class="product_img" src="{{item.pro_thumb}}" mode="aspectFill"></image><view class="order_list_product_center"><view class="order_product_title text_hide">{{item.pro_title}}</view><view class="order_product_des text_hide">{{item.pro_val}}</view></view><view class="flex1"></view><view><view class="order_product_price"><text>￥</text>{{item.pro_price}}</view><view class="order_product_count">X1</view></view></view></view></block></navigator><view class="order_list_pricebox"><block s-if="{{item.assess>1}}"><view class="allbtn_right"><view class="order_list_btn" data-order_id="{{item.order_id}}" data-event-opts="{{[['tap',[['deleteOrder',['$event']]]]]}}" bindtap="__e">删除订单</view></view></block><block s-if="{{item.status==1}}"><view class="allbtn_right"><view class="order_list_btn">待发货</view></view></block><block s-if="{{item.status>1&&item.delivery_type==1}}"><block><block s-if="{{item.express==-1}}"><block><view class="order_list_btn">商家配送</view></block></block><block s-else><block><view class="order_list_btn" data-kuaidi="{{item.express}}" data-kuaidihao="{{item.express_no}}" data-order_id="{{item.order_id}}" data-event-opts="{{[['tap',[['wlinfo',['$event']]]]]}}" bindtap="__e">查看物流</view></block></block></block></block><block s-if="{{item.status==2}}"><block><view class="order_list_price_btn" id="{{item.order_id}}" data-event-opts="{{[['tap',[['qrshouh',['$event']]]]]}}" bindtap="__e">确认收货</view></block></block><block s-if="{{item.status==3}}"><view class="allbtn_right"><view class="order_list_price_btn" id="{{item.order_id}}" data-event-opts="{{[['tap',[['hxshow',['$event']]]]]}}" bindtap="__e">立即核销</view></view></block><block s-if="{{item.status==4}}"><block><view class="allbtn_right"><block s-if="{{item.assess==1}}"><view class="order_list_price_btn" data-add="1" data-order_id="{{item.order_id}}" data-event-opts="{{[['tap',[['goevaluate',['$event']]]]]}}" bindtap="__e">立即评价</view></block></view><block s-if="{{item.assess==2}}"><view class="allbtn_right"><view class="order_list_price_btn" data-add="2" data-order_id="{{item.order_id}}" data-event-opts="{{[['tap',[['goevaluate',['$event']]]]]}}" bindtap="__e">追加评价</view></view></block></block></block></view></block></view></block></view></block></block><block s-else><block><image class="pageno_address" src="{{$imgurl+'no_search.png'}}"></image><view style="text-align:center;color:#666;">还没有相关订单哟~</view></block></block></view><block s-if="{{showhx==1}}"><view class="hx_con"><view data-event-opts="{{[['tap',[['hxhide',['$event']]]]]}}" class="mask" bindtap="__e"></view><view class="share_con flex-row"><view class="share_con_box"><view data-event-opts="{{[['tap',[['gethxmima',['$event']]]]]}}" class="flex-row" style="border-right:2rpx solid #c6cbd9;align-items:center;justify-content:center;" bindtap="__e"><view class="iconfont icon-x-mima1" style="font-size:40rpx;color:#56bb3a;"></view><view style="margin-left:10rpx;">密码核销</view></view></view><view class="share_con_box"><view data-event-opts="{{[['tap',[['gethxImg',['$event']]]]]}}" class="flex-row" style="align-items:center;justify-content:center;" bindtap="__e"><view class="iconfont icon-x-erweima1" style="font-size:40rpx;color:#e47b2f;"></view><view style="margin-left:10rpx;">二维码核销</view></view></view></view><block s-if="{{hx_choose==1}}"><view><view data-event-opts="{{[['tap',[['hxhide1',['$event']]]]]}}" class="mask" style="z-index:920;" bindtap="__e"></view><view class="hexiao_inp"><view data-event-opts="{{[['tap',[['hxhide1',['$event']]]]]}}" class="hx_titc" bindtap="__e"><view class="iconfont icon-x-guanbi"></view></view><view class="hexiao_mima"><view class="hx_tit">请输入核销密码</view><view class="code"><block s-for="{{hxmm_list}}" s-for-item="item" s-for-index="index" s-key="index"><text class="{{[item.fs?'focus':'']}}" type="number">{{item.val}}</text></block></view><view class="code_input"><input type="number" focus="true" hover-class="none" maxlength="6" data-event-opts="{{[['input',[['hxmmInput',['$event']]]]]}}" value="{{hxmm}}" bindinput="__e"/></view><view class="hx_yes"><view data-event-opts="{{[['tap',[['hxmmpass',['$event']]]]]}}" class="hx_btn" style="{{'background:'+(baseinfo.base_color)+';'+('color:'+('#fff')+';')}}" bindtap="__e">确定</view></view></view></view></view></block><block s-if="{{hx_choose==2}}"><view><view data-event-opts="{{[['tap',[['hxhide1',['$event']]]]]}}" class="mask" style="z-index:920;" bindtap="__e"></view><view class="hexiao"><view data-event-opts="{{[['tap',[['hxhide1',['$event']]]]]}}" class="hx_titc" bindtap="__e"><view class="iconfont icon-x-guanbi"></view></view><view style="text-align:center;"><image class="hx_ewm" src="{{hx_ewm}}" mode="aspectFit"></image></view><view class="he_img_tips">请将二维码出示给工作人员</view></view></view></block></view></block><myfooter vue-id="1" page_signs="{{page_signs}}" baseinfo="{{baseinfo}}"></myfooter></view></block>