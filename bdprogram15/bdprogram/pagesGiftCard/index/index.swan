<block s-if="{{$imgurl}}"><view><view class="nav_top"><scroll-view class="nav_top_scroll" style="{{'background:'+(baseinfo.base_color)+';'}}" scroll-x="true"><block s-for="{{cates}}" s-for-item="item" s-for-index="index" s-key="index"><block><view class="{{['nav_top_item',item.id==cid?'nav_top_active':'']}}" data-cate_id="{{item.id}}" data-event-opts="{{[['tap',[['handleTap',['$event']]]]]}}" bindtap="__e">{{item.name}}</view></block></block></scroll-view></view><block s-if="{{list.length>0}}"><block><view class="nav_top_radius" style="{{'background:'+(baseinfo.base_color)+';'}}"></view><view class="card_box"><block s-for="{{list}}" s-for-item="item" s-for-index="index" s-key="index"><block><view class="card_item flex_box"><image class="giftcard_img" src="{{item.thumb}}" mode="aspectFill"></image><view class="giftcard_info flex_bbox"><view class="info_title">{{item.name}}</view><block s-if="{{item.type==1}}"><view class="info_miane">{{"面额：￥"+item.money}}</view></block><view class="info_price"><text>￥</text>{{item.price}}</view><view class="gift_buybtn" data-id="{{item.id}}" data-event-opts="{{[['tap',[['gopay',['$event']]]]]}}" bindtap="__e">购买</view></view></view></block></block></view></block></block><block s-if="{{list.length==0}}"><block><image class="pageno_address" src="{{$imgurl+'no_active.png'}}"></image><view style="text-align:center;color:#666;">还没有相关内容哟~</view></block></block><myfooter vue-id="1" page_signs="{{page_signs}}" baseinfo="{{baseinfo}}"></myfooter></view></block>