<block s-if="{{$imgurl}}"><view class="_div"><view style="padding-bottom:100rpx;"><view class="top_bigtab"><view class="top_bgcolor" style="{{'background:'+(baseinfo.base_color)+';'}}"></view><view class="tab_box"><block s-if="{{take_self==2&&info.express_type==2}}"><view class="swiper-tab"><view class="{{['swiper-tab-item',currentTab==1?'tab_active':'']}}" style="{{'color:'+(baseinfo.base_color)+';'}}" data-current="{{1}}" data-event-opts="{{[['tap',[['clickTab',['$event']]]]]}}" bindtap="__e">{{ps_name}}</view><view class="{{['swiper-tab-item',currentTab==2?'tab_active':'']}}" style="{{'color:'+(baseinfo.base_color)+';'}}" data-current="{{2}}" data-event-opts="{{[['tap',[['clickTab',['$event']]]]]}}" bindtap="__e">{{zq_name}}</view></view></block><view class="{{[currentTab==1?'tab_show':'tab_hidden']}}" style="{{'border-radius:'+(take_self==2&&info.express_type==2?'0 0 10rpx 10rpx':'10rpx')+';'+('background-image:'+('url('+$imgurl+'tabbottom.png)')+';')}}"><view class="flex-row tab_show1"><view class="iconfont icon-x-geren2 icon_tabs"></view><view style="width:93%;"><view style="color:#232323;font-size:26rpx;">收件人信息</view><block s-if="{{!default_address}}"><view data-event-opts="{{[['tap',[['add_address',['$event']]]]]}}" class="gmadmin_info flex_bflow" bindtap="__e"><view>请选择购买人信息</view><view class="iconfont icon-x-you" style="font-size:24rpx;"></view></view></block><block s-if="{{default_address}}"><view data-event-opts="{{[['tap',[['add_address',['$event']]]]]}}" class="gmadmin_info flex_bflow" bindtap="__e"><view style="width:90%;"><view class="gmadmin_haveinfo"><text style="font-weight:bold;">{{default_address.name}}</text><text>{{default_address.mobile}}</text></view><view>{{default_address.address+" "+default_address.more_address}}</view><block s-if="{{error_num==2}}"><view style="color:#ff472a;margin-top:10rpx;">该地址超出配送范围，请重新选择！</view></block></view><view class="iconfont icon-x-you" style="font-size:24rpx;"></view></view></block></view></view></view><view class="{{[currentTab==2?'tab_show':'tab_hidden']}}" style="{{'border-radius:'+(take_self==2&&info.express_type==2?'0 0 10rpx 10rpx':'10rpx')+';'+('background-image:'+('url('+$imgurl+'tabbottom.png)')+';')}}"><view class="flex-row tab_show1"><view class="iconfont icon-c-dizhishixin icon_tabs"></view><view style="width:93%;"><view style="color:#232323;font-size:26rpx;">{{shop_info?shop_info.title:'门店信息'}}</view><block s-if="{{!shop_info}}"><view data-event-opts="{{[['tap',[['chooseStore',['$event']]]]]}}" class="gmadmin_info flex_bflow" bindtap="__e"><view>请选择门店</view><view class="iconfont icon-x-you" style="font-size:24rpx;"></view></view></block><block s-if="{{shop_info}}"><view data-event-opts="{{[['tap',[['chooseStore',['$event']]]]]}}" class="gmadmin_info flex_bflow" bindtap="__e"><view style="width:90%;"><view class="gmadmin_haveinfo"><text>{{"营业时间："+shop_info.times}}</text><text>{{shop_info.tel}}</text></view><view>{{"地址："+shop_info.province+shop_info.city+shop_info.country}}</view></view><view class="iconfont icon-x-you" style="font-size:24rpx;"></view></view></block></view></view><view class="tab_show2"><view class="flex-row tab_show2_time"><view>自取时间</view><view><view data-event-opts="{{[['tap',[['openDatetimePicker',['$event']]]]]}}" style="font-size:26rpx;color:#969696;" bindtap="__e">{{self_taking_time?self_taking_time:'请选择自取时间'}}</view><datetime class="vue-ref" vue-id="1" start-year="{{start_year}}" end-year="{{end_year}}" color="{{baseinfo.base_color}}" data-ref="myPicker" data-event-opts="{{[['^submit',[['handleSubmit']]]]}}" bind:submit="__e"></datetime></view></view><view class="flex-row tab_show2_time"><view>预留电话</view><input type="text" name="default_concat" placeholder="请输入电话" placeholder-style="color:#969696" data-event-opts="{{[['input',[['concat_input',['$event']]]]]}}" value="{{default_concat}}" bindinput="__e"/></view></view></view></view></view><view class="baoyou_box"><view class="dd_infos flex-row"><view class="dd_infoimg"><image src="{{info.pro_thumb}}" mode="aspectFill"></image></view><view class="flex-row" style="flex-flow:column;width:500rpx;"><view class="dd_protitle">{{info.pro_title}}</view><view class="dd_proguige">{{info.pro_val}}</view></view></view></view><view class="baoyou_box"><view class="dd_infos"><view class="price_items flex_bflow"><view>价格</view><view>{{"￥"+info.pro_price}}</view></view><view class="price_items flex_bflow"><view>数量</view><view>1</view></view><view class="price_all flex_bflow"><view class="price_all_text">总计</view><view class="price_all_price"><text>￥</text>{{info.pro_price}}</view></view></view></view></view><view class="foot_js flex_bflow"><view style="flex:1;"></view><view data-event-opts="{{[['tap',[['sureGetPro',['$event']]]]]}}" class="js_btn" style="{{'background:'+(baseinfo.base_color)+';'}}" bindtap="__e">兑换商品</view></view></view></block>