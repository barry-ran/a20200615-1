<block s-if="{{$imgurl}}"><view><block s-if="{{needAuth}}"><auth vue-id="1" needAuth="{{needAuth}}" data-event-opts="{{[['^closeAuth',[['closeAuth']]],['^cell',[['cell']]]]}}" bind:closeAuth="__e" bind:cell="__e"></auth></block><block s-if="{{needBind}}"><bind-phone vue-id="2" needBind="{{needBind}}" data-event-opts="{{[['^closeBind',[['closeBind']]]]}}" bind:closeBind="__e"></bind-phone></block><view><view class="active_nav_box"><view class="box_active_nav"><view class="{{['active_nav',type==1?'active_nav_on':'']}}" data-id="1" data-event-opts="{{[['tap',[['changeTab',['$event']]]]]}}" bindtap="__e">{{"可使用("+use_count+")"}}</view></view><view class="box_active_nav"><view class="{{['active_nav',type==2?'active_nav_on':'']}}" data-id="2" data-event-opts="{{[['tap',[['changeTab',['$event']]]]]}}" bindtap="__e">{{"不可使用("+not_use_count+")"}}</view></view></view><view style="height:86rpx;"></view><view class="card_listbox"><block s-for="{{list}}" s-for-item="item" s-for-index="index" s-key="index"><block><view class="list_item_box"><block s-if="{{type==2}}"><view class="no_use"></view></block><view class="list_item_top" style="{{'background-image:'+(item.type==1?'url('+$imgurl+'giftcard1.png)':'url('+$imgurl+'giftcard2.png)')+';'}}"><view class="cards_top_info"><view><view class="card_typename">{{item.type==1?'充值卡':'实物卡'}}</view><view>{{"卡号："+item.order_id}}</view></view><view class="cards_info_right"><view style="flex:1;">{{item.type==1?'￥'+item.card_con:''}}</view><view style="{{'color:'+(item.type==2?'#E4AFFF':'#B8D6FF')+';'}}">{{item.type==1?'面额':'指定商品'}}</view></view></view></view><view class="list_item_bottom"><view class="card_info pro_card_info">{{item.type==2?'商品：'+item.card_con:''}}</view><view class="card_info">{{"有效期："+item.use_time}}</view><block s-if="{{type==1}}"><view class="card_usebtn" style="{{'background:'+(item.type==2?'#AE56DB':'#3E88EC')+';'}}" data-index="{{index}}" data-event-opts="{{[['tap',[['showCardsBottom',['$event']]]]]}}" bindtap="__e">使用</view></block><block s-if="{{type==2&&item.status==2}}"><image class="card_usebtn_img" src="{{$imgurl+'giftuse.png'}}" mode="aspectFill"></image></block><block s-if="{{type==2&&item.status==3}}"><image class="card_losebtn_img" src="{{$imgurl+'giftlose.png'}}" mode="aspectFill"></image></block></view></view></block></block></view><view data-event-opts="{{[['tap',[['goorderlist',['$event']]]]]}}" class="btn_toorder" bindtap="__e">实物订单</view></view><block s-if="{{showbottom==1}}"><view data-event-opts="{{[['tap',[['closeBottom',['$event']]]]]}}" class="bottom_box" bindtap="__e"></view></block><block s-if="{{showbottom==1}}"><view class="bottom_cont"><view class="bottom_use flex-row"><view class="bottom_cont_box"><view data-event-opts="{{[['tap',[['useCard',['$event']]]]]}}" class="flex-row" style="justify-content:center;border-right:2rpx solid #C6CBD9;" bindtap="__e"><view class="iconfont icon-x-duihuan" style="font-size:40rpx;color:#5CC14B;"></view><view style="margin-left:20rpx;">立即兑换</view></view></view><view class="bottom_cont_box"><view data-event-opts="{{[['tap',[['getShareCard',['$event']]]]]}}" class="flex-row" style="justify-content:center;" bindtap="__e"><view class="iconfont icon-x-gift1l" style="font-size:40rpx;color:#FF685D;"></view><view style="margin-left:20rpx;">赠送他人</view></view></view></view></view></block><block s-if="{{cardsbox==1}}"><view class="bottom_box"></view></block><block s-if="{{cardsbox==1}}"><view class="cards_box"><view class="cards_contbox"><view data-event-opts="{{[['tap',[['closeCardBox',['$event']]]]]}}" class="iconfont icon-x-guanbi1" bindtap="__e"></view><image class="cards_img" src="{{shareimg_url}}" mode="aspectFill"></image><button class="save_imgs" type="primary" data-event-opts="{{[['tap',[['saveImg',['$event']]]]]}}" bindtap="__e"><view class="iconfont icon-xiazai"></view>保存图片并分享</button></view></view></block><myfooter vue-id="3" page_signs="{{page_signs}}" baseinfo="{{baseinfo}}"></myfooter></view></block>