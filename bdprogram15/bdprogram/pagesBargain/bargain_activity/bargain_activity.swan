<block s-if="{{$imgurl}}"><view style="{{'min-height:'+(windowH+'px')+';'+('height:'+('100%')+';')+('background:'+('linear-gradient(to top,'+baseinfo.base_color2+','+baseinfo.base_color+')')+';')+('padding:'+('0 20rpx 20rpx')+';')+('box-sizing:'+('border-box')+';')}}"><block s-if="{{needAuth}}"><auth vue-id="1" needAuth="{{needAuth}}" data-event-opts="{{[['^closeAuth',[['closeAuth']]],['^cell',[['cell']]]]}}" bind:closeAuth="__e" bind:cell="__e"></auth></block><block s-if="{{needBind}}"><bind-phone vue-id="2" needBind="{{needBind}}" data-event-opts="{{[['^closeBind',[['closeBind']]]]}}" bind:closeBind="__e"></bind-phone></block><view><view class="activity_bg"><view class="activity_head"><view data-event-opts="{{[['tap',[['look_regular',['$event']]]]]}}" class="gz" style="{{'background:'+(baseinfo.base_color)+';'}}" bindtap="__e">规则</view><image class="head" src="{{proInfo['avatar']}}"></image><view class="find"><view>{{proInfo['nickname']}}</view><view style="color:#969696;font-size:26rpx;margin-top:6rpx;">我发现一件好货，来一起砍价优惠购！</view></view><view class="activity_main"><image src="{{proInfo['thumb']}}" mode="aspectFill"></image><view class="activity_main_info"><view class="produce_title">{{proInfo['title']}}</view><view class="produce_title2"><view style="color:#f4361d;"><text>￥</text><text style="font-size:32rpx;">{{proInfo['nowPrice']}}</text></view><view style="color:#434343;"><text class="time_box">{{countDown_d}}</text>天<text class="time_box">{{countDown_h}}</text>:<text class="time_box">{{countDown_m}}</text>:<text class="time_box">{{countDown_s}}</text>后结束</view></view></view></view><view class="kj_detail_box"><view class="kj_detail_price">已砍<text>{{alreadyPrice}}</text>元，剩余<text>{{restPrice}}</text>元</view><view class="kj_progress"><progress percent="{{proInfo['percent']}}" stroke-width="8" activeColor="{{baseinfo.base_color}}" backgroundColor="#f6f6f6" border-radius="20"></progress><view class="kj_progress_nowPrice"><text class="kj_nowPrice_box" style="{{'background:'+('linear-gradient(to top,'+baseinfo.base_color+','+baseinfo.base_color2+')')+';'}}">{{'当前价：￥'+proInfo['nowPrice']+''}}</text></view></view><view class="progress_price"><view><text>原价：</text><text style="font-size:24rpx;">{{"￥"+proInfo['price']}}</text></view><view><text>底价：</text><text style="font-size:24rpx;color:#f4361d;">{{"￥"+proInfo['miniPrice']}}</text></view></view></view><block s-if="{{suid==proInfo['suid']}}"><view class="btn_kj"><block s-if="{{proInfo['flag']<3}}"><view><button data-event-opts="{{[['tap',[['goOrder',['$event']]]]]}}" class="btn_kj1" style="{{'background:'+('linear-gradient(to top right,'+baseinfo.base_color+','+baseinfo.base_color2+')')+';'}}" bindtap="__e">立即购买</button></view></block><block s-if="{{proInfo['flag']==3}}"><view><button class="btn_kj1" style="background:#cccccc;">已下单</button></view></block><block s-if="{{proInfo['flag']==1}}"><view><button class="btn_kj2" style="{{'border:'+('2rpx solid '+baseinfo.base_color)+';'+('color:'+(baseinfo.base_color)+';')}}" open-type="share" data-event-opts="{{[['tap',[['onShareAppMessage',['$event']]]]]}}" bindtap="__e">找人帮砍</button></view></block></view></block><block s-else><view class="btn_kj"><view><button data-event-opts="{{[['tap',[['golist',['$event']]]]]}}" class="btn_kj1" style="{{'background:'+('linear-gradient(to top right,'+baseinfo.base_color+','+baseinfo.base_color2+')')+';'}}" bindtap="__e">我也要发起砍价</button></view></view></block></view></view><view><view class="friend"><view style="padding-bottom:10rpx;"><view class="record"><view class="record_line"></view><text>砍价帮</text><view class="record_line"></view></view><block s-if="{{proInfo['flag']==1}}"><view class="friend_my">已有<text style="color:#f4361d;">{{proInfo['now_count']}}</text>{{"人参与砍价，还可继续邀请"+proInfo['sy_count']+"人"}}</view></block></view><block s-for="{{proInfo['receive']}}" s-for-item="item" s-for-index="index" s-key="index"><view class="friend_receive" style="{{'border-color:'+(baseinfo.base_color2)+';'}}"><view class="friend_receivel"><image src="{{item.avatar}}"></image><view><view class="friend_receive_infot">{{item.nickname}}</view><view class="friend_receive_infob">{{item.createtime}}</view></view></view><view>砍掉<text style="font-size:20rpx;margin-left:10rpx;">￥</text>{{item.price}}</view></view></block></view></view></view><block s-if="{{close==1}}"><block><view class="alertbox"><view class="hdgz"><view class="hdgz_head">活动规则</view><view class="hdgz_content"><rich-text nodes="{{rules}}"></rich-text></view><view data-event-opts="{{[['tap',[['closes',['$event']]]]]}}" class="iconfont icon-x-guanbi1 close_icon" bindtap="__e"></view></view></view></block></block><block s-if="{{my_receive_flag==2}}"><view class="mask"></view></block><block s-if="{{my_receive_flag==2}}"><view class="maskCon"><view class="userTit" style="margin:300rpx auto 50rpx;"><view class="receiveInfo"><image class="closeMask" style="width:40rpx;height:40rpx;" src="{{$imgurl+'bargain_close.png'}}" data-receive_id="{{proInfo['my_receive']['id']}}" data-event-opts="{{[['tap',[['changeReceiveFlag',['$event']]]]]}}" bindtap="__e"></image><image style="width:200rpx;" src="{{$imgurl+'bargain.png'}}" mode="widthFix"></image><view style="height:100rpx;line-height:100rpx;">成功砍价<text style="color:#f4361d;">{{proInfo['my_receive']['price']}}</text>元</view><block s-if="{{suid!=proInfo['suid']}}"><view data-event-opts="{{[['tap',[['golist',['$event']]]]]}}" class="btn1_kjBox" style="{{'background:'+('linear-gradient(to top right,'+baseinfo.base_color+','+baseinfo.base_color2+')')+';'}}" bindtap="__e">我也要发起砍价</view></block><block s-if="{{suid==proInfo['suid']&&proInfo['flag']==1}}"><button class="btn1_kjBox" style="{{'background:'+('linear-gradient(to top right,'+baseinfo.base_color+','+baseinfo.base_color2+')')+';'}}" open-type="share" data-event-opts="{{[['tap',[['onShareAppMessage',['$event']]]]]}}" bindtap="__e">邀请好友帮砍</button></block><block s-if="{{suid==proInfo['suid']&&proInfo['flag']==2}}"><button data-event-opts="{{[['tap',[['goOrder',['$event']]]]]}}" class="btn1_kjBox" style="{{'background:'+('linear-gradient(to top right,'+baseinfo.base_color+','+baseinfo.base_color2+')')+';'}}" bindtap="__e">已砍至最低价，立即购买</button></block></view></view></view></block></view></block>