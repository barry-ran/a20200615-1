<view class="_div"><view class="my-container"><view class="my_head"><view class="my_head_view"><text class="my_head_text" style="{{(flag==1?'color:'+baseinfo.base_color+';border-bottom: 4rpx solid '+baseinfo.base_color+';':'')}}" data-flag="1" data-event-opts="{{[['tap',[['changeNav',['$event']]]]]}}" bindtap="__e">砍价中</text></view><view class="my_head_view"><text class="my_head_text" style="{{(flag==2?'color: '+baseinfo.base_color+';border-bottom: 4rpx solid '+baseinfo.base_color+';':'')}}" data-flag="2" data-event-opts="{{[['tap',[['changeNav',['$event']]]]]}}" bindtap="__e">已过期</text></view></view><view style="height:80rpx;"></view><view class="my_main"><block s-for="{{list}}" s-for-item="item" s-for-index="index" s-key="index"><view class="bargain_main"><view class="bargain_main_top"><image src="{{item.thumb}}" mode="aspectFill"></image><view class="bargain_box_text"><text class="bargain_title">{{item.title}}</text><view class="bargain_title2"><view class="time"><text class="time_itemBox time_itemBox1">{{item.djs.day}}</text>天<text class="time_itemBox">{{item.djs.hour}}</text>:<text class="time_itemBox">{{item.djs.min}}</text>:<text class="time_itemBox">{{item.djs.second}}</text>结束</view></view><view class="bargain_title3"><text>￥</text><text style="font-size:28rpx;">{{item.miniPrice}}</text><text class="min_price"><text class="min_price_san"></text><text class="min_price_box">底价</text></text></view><block s-if="{{item.flag==1}}"><button class="jion1" data-pid="{{item.id}}" data-bargain_id="{{item.bargain_id}}" data-event-opts="{{[['tap',[['goActivity',['$event']]]]]}}" bindtap="__e">继续砍价</button></block><block s-if="{{item.flag==2}}"><button class="jion1" data-pid="{{item.id}}" data-bargain_id="{{item.bargain_id}}" data-event-opts="{{[['tap',[['createOrder',['$event']]]]]}}" bindtap="__e">立即购买</button></block><block s-if="{{flag==2}}"><view class="jion1" style="background:#cccccc;">已过期</view></block></view></view><view><view class="progress_view"><progress percent="{{item.percent}}" stroke-width="8" activeColor="#f4361d" backgroundColor="#f6f6f6" border-radius="20"></progress></view><view class="mini_price_cont"><view class="bargain_title4"><text>{{"当前价￥"+item.nowPrice}}</text></view><block s-if="{{item.flag==2}}"><view class="max">{{item.nowPrice==item.miniPrice?'已砍到最低价':'邀请人数已达上限'}}</view></block><block s-if="{{item.flag!=2}}"><view>{{item.joinCount+"人助砍"}}</view></block></view></view></view></block><block s-if="{{addmore==1}}"><view data-event-opts="{{[['tap',[['addmores',['$event']]]]]}}" class="addmore" bindtap="__e">点击加载更多</view></block><block s-if="{{addmore==0}}"><view class="addmore">全部数据加载完成</view></block></view></view><view class="bargain_footer"><view data-event-opts="{{[['tap',[['golist',['$event']]]]]}}" class="btn1" bindtap="__e"><view class="iconfont icon-x-shouye"></view></view></view><myfooter vue-id="1" page_signs="{{page_signs}}" baseinfo="{{baseinfo}}"></myfooter></view>