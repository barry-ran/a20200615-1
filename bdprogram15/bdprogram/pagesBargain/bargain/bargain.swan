<block s-if="{{$imgurl}}"><view class="_div"><view class="bargain-container"><view class="page_topbox" style="{{'background:'+(baseinfo.base_color)+';'}}"><form class="searchBox"><view class="list_search"><view class="iconfont icon-x-sousuo1 ssk-icon"></view><input class="input-style" type="text" placeholder="搜索" placeholder-style="color:#ccc" value="" data-event-opts="{{[['input',[['getSearchKey',['$event']]]]]}}" bindinput="__e"/></view></form><block s-if="{{banners.length>0}}"><view class="swiper_box" style="{{'display:'+(bannerClose==1?'none':'block')+';'}}"><swiper class="swiper_conbox" indicator-dots="{{indicatorDots}}" indicator-color="#ffffff" indicator-active-color="{{baseinfo.base_color}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" circular="{{duration}}" current="{{swiperCurrent}}"><block s-for="{{banners}}" s-for-item="item" s-for-index="index" s-key="index"><block><swiper-item class="swiper_item_box"><image class="swiper_item_image" src="{{item}}" mode="scaleToFill"></image></swiper-item></block></block></swiper></view></block><view class="nav_xtab"><scroll-view class="nav_x_one" scroll-into-view="{{'c'+cid}}" scroll-x="true"><block s-for="{{cates}}" s-for-item="item" s-for-index="key" s-key="key"><view class="nav_xt" style="{{'border-bottom:'+('2px solid '+(item.id==cid?'#ffffff':'transparent'))+';'}}" data-id="{{item.id}}" data-event-opts="{{[['tap',[['handleTap',['$event']]]]]}}" bindtap="__e">{{item.title}}</view></block></scroll-view></view></view><block s-if="{{banners.length==0}}"><view class="swiper_nobg_box0"></view></block><block s-if="{{banners.length>0}}"><view class="swiper_nobg_box" style="{{'height:'+(bannerClose==1?'':'430rpx')+';'}}"><block s-if="{{banners.length>0}}"><view class="swiper_nobg" style="{{'display:'+(bannerClose==1?'none':'block')+';'+('background:'+(baseinfo.base_color)+';')}}"></view></block></view></block><view class="bargain"><block s-for="{{list}}" s-for-item="item" s-for-index="index" s-key="index"><view class="bargain_main"><image src="{{item.thumb}}" mode="aspectFill"></image><view class="bargain_text"><text class="bargain_title">{{item.title}}</text><view class="bargain_title_play"><block s-if="{{item.bargained_num>0&&item.avatar.length>0}}"><view><block s-for="{{item.avatar}}" s-for-item="img" s-for-index="subindex" s-key="subindex"><image class="img01" src="{{img}}"></image></block></view></block><block s-if="{{item.bargained_num>4}}"><text style="margin-right:8rpx;">...</text></block><text>{{item.bargained_num>0?item.bargained_num+"人正在砍价":"发起一波砍价"}}</text></view><view class="bargain_title_price">{{"原价￥"+item.price}}</view><view class="bargain_title_minprice"><text>￥</text><text style="font-size:24rpx;">{{item.miniPrice}}</text><text class="min_price"><text class="min_price_san"></text><text class="min_price_box">底价</text></text></view><button class="jion" data-id="{{item.id}}" data-event-opts="{{[['tap',[['goPage',['$event']]]]]}}" bindtap="__e">{{item.bargained==1?'继续砍价':'去砍价'}}</button></view></view></block></view><block s-if="{{addmore==1}}"><view data-event-opts="{{[['tap',[['addmores',['$event']]]]]}}" class="addmore" bindtap="__e">点击加载更多</view></block><block s-if="{{addmore==0}}"><view class="addmore">全部数据加载完成</view></block></view><view class="bargain_footer"><view data-event-opts="{{[['tap',[['goMybargain',['$event']]]]]}}" class="btn2" bindtap="__e"><view class="iconfont icon-kanjia1"></view></view></view><myfooter vue-id="1" page_signs="{{page_signs}}" baseinfo="{{baseinfo}}"></myfooter></view></block>