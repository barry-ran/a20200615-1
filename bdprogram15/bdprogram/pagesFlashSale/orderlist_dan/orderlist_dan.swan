<view><scroll-view class="order_nav" scroll-x><block s-for="{{nav}}" s-for-item="item" s-for-index="index" s-key="index"><block><view class="{{['order_nav_single',type==item.id?'order_nav_single_on':'']}}" data-id="{{item.id}}" data-nav="{{item.nav}}" data-event-opts="{{[['tap',[['chonxhq',['$event']]]]]}}" bindtap="__e">{{item.text}}</view></block></block></scroll-view><view class="order_list_contentbox" style="padding-top:80rpx;"><block s-for="{{orderinfo}}" s-for-item="item" s-for-index="key" s-key="key"><view class="dd_box"><view class="order_list_single" data-order="{{item.order_id}}" data-event-opts="{{[['tap',[['orderinfoGo',['$event']]]]]}}" bindtap="__e"><view class="order_list_head hbj"><view class="order_list_head_left flex1">{{"订单号："+item.order_id}}</view><block s-if="{{item.flag==0}}"><view class="order_list_head_right">待付款</view></block><block s-if="{{item.flag==1&&item.nav==1}}"><view class="order_list_head_right">待发货</view></block><block s-if="{{item.flag==1&&item.nav==2}}"><view class="order_list_head_right">待核销</view></block><block s-if="{{item.flag==2}}"><view class="order_list_head_right">已完成</view></block><block s-if="{{item.flag==-1}}"><view class="order_list_head_right">已过期</view></block><block s-if="{{item.flag==4}}"><view class="order_list_head_right">已发货</view></block><block s-if="{{item.flag==5}}"><view class="order_list_head_right">订单被取消</view></block><block s-if="{{item.flag==6}}"><view class="order_list_head_right">订单取消中</view></block><block s-if="{{item.flag==7}}"><view class="order_list_head_right">退货审核中</view></block><block s-if="{{item.flag==8}}"><view class="order_list_head_right">退货成功</view></block><block s-if="{{item.flag==9}}"><view class="order_list_head_right">退货失败</view></block></view><view class="order_list_productbox"><view class="order_list_product hbj"><image class="product_img" src="{{item.thumb}}" mode="aspectFill"></image><view class="order_list_product_center"><view class="order_product_title text_hide">{{item.product}}</view><view class="order_product_des text_hide"></view></view><view class="flex1"></view><view><view class="order_product_price"><text>￥</text>{{item.price}}</view><view class="order_product_count">{{"X"+item.num}}</view></view></view></view><view data-event-opts="{{[['tap',[['',['$event']]]]]}}" class="order_list_pricebox" catchtap="__e"><view class="order_list_price_left">{{"共"+item.num+'件商品，实付：'}}<text style="color:#f4361d;">￥<text style="font-size:28rpx;">{{item.num*item.price}}</text></text><block s-if="{{item.kuaidi}}"><text style="color:#969696;">{{"（"+(item.kuaidi=='-1'?'商家配送':item.kuaidi)+" "+(item.kuaidihao?'- '+item.kuaidihao:'')+"）"}}</text></block></view><block s-if="{{item.flag==0}}"><view class="allbtn_right"><view class="order_list_price_btn" data-order="{{item.order_id}}" data-pid="{{item.pid}}" data-event-opts="{{[['tap',[['paybox',['$event']]]]]}}" bindtap="__e">立即付款</view></view></block><block s-if="{{item.flag==1||item.flag==2||item.flag==6}}"><view class="allbtn_right"><view class="order_list_price_btn" data-order="{{item.order_id}}" data-event-opts="{{[['tap',[['orderinfoGo',['$event']]]]]}}" bindtap="__e">查看详情</view><block s-if="{{item.flag==2&&item.assess==1}}"><view class="order_list_price_btn"><view data-order="{{item.order_id}}" data-type="miaosha" data-event-opts="{{[['tap',[['goevaluate',['$event']]]]]}}" bindtap="__e">我要评价</view></view></block><block s-if="{{item.flag==2&&item.assess==2}}"><view class="order_list_price_btn"><view>我已评价</view></view></block></view></block><block s-if="{{item.flag==4}}"><view class="allbtn_right"><view class="order_list_price_btn" data-order="{{item.order_id}}" data-event-opts="{{[['tap',[['qrshouh',['$event']]]]]}}" bindtap="__e">确认收货</view><view class="order_list_price_btn" data-kuaidi="{{item.kuaidi}}" data-kuaidihao="{{item.kuaidihao}}" data-event-opts="{{[['tap',[['wlinfo',['$event']]]]]}}" bindtap="__e">查看物流</view></view></block><block s-if="{{item.flag==6||item.flag==7||item.flag==9}}"><view class="allbtn_right"><view data-event-opts="{{[['tap',[['makePhoneCallB',['$event']]]]]}}" class="order_list_price_btn" bindtap="__e">联系商家</view></view></block><block s-if="{{item.flag==5&&item.qxbeizhu!=NULL}}"><view class="allbtn_right"><view class="order_list_msg">商家留言：<text>{{item.qxbeizhu}}</text></view></view></block><block s-if="{{item.flag==8}}"><view class="allbtn_right"><view class="order_list_price_btn" data-pid="{{item.pid}}" data-event-opts="{{[['tap',[['orderagain',['$event']]]]]}}" bindtap="__e">再次下单</view></view></block></view></view></view></block></view><block s-if="{{orderinfo_length==0}}"><block><image class="pageno_address" src="{{$imgurl+'no_search.png'}}"></image><view style="text-align:center;color:#666;">还没有相关订单哟~</view></block></block><block s-if="{{baseinfo.tabbar_t==1}}"><copyright vue-id="1" baseinfo="{{baseinfo}}"></copyright></block><myfooter vue-id="2" page_signs="{{page_signs}}" baseinfo="{{baseinfo}}"></myfooter><block s-if="{{showPay==1}}"><block><view data-event-opts="{{[['tap',[['payboxclose',['$event']]]]]}}" class="mask" bindtap="__e"></view><view class="pay_box"><view class="pay_function"><view>选择支付方式</view><view data-event-opts="{{[['tap',[['payboxclose',['$event']]]]]}}" class="iconfont icon-x-guanbi bsyyyd" bindtap="__e"></view></view><view class="pay_funs flex-row"><view class="pay_funsimg"><image src="{{$imgurl+'yue.png'}}" mode="aspectFit"></image></view><view class="pay_moneys flex_bflow"><view>{{"余额支付（剩余：￥"+mymoney+"元）"}}</view><block s-if="{{mymoney_pay==1}}"><view class="{{['iconfont icon-x-gou',choosepayf==0?'choose_jf':'no_jf']}}" data-pay_type="{{1}}" data-type="{{0}}" data-event-opts="{{[['tap',[['choosepay',['$event']]]]]}}" bindtap="__e"></view></block></view><block s-if="{{mymoney_pay==2}}"><view class="ye_less"></view></block></view><block s-if="{{pay_money>0}}"><block><view class="pay_funs flex-row"><view class="pay_funsimg"><image src="{{$imgurl+'baidu.png'}}" mode="aspectFit"></image></view><view class="pay_moneys flex_bflow"><view>百度支付</view><view class="{{['iconfont icon-x-gou',choosepayf==4?'choose_jf':'no_jf']}}" data-pay_type="{{2}}" data-type="{{4}}" data-event-opts="{{[['tap',[['choosepay',['$event']]]]]}}" bindtap="__e"></view></view></view></block></block><form report-submit="true" data-event-opts="{{[['submit',[['pay',['$event']]]]]}}" bindsubmit="__e"><button class="pay_submit" formType="submit">确定</button></form></view></block></block></view>