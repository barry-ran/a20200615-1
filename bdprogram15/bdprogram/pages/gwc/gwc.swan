<block s-if="{{$imgurl}}"><view><block s-if="{{needAuth}}"><auth vue-id="1" needAuth="{{needAuth}}" data-event-opts="{{[['^closeAuth',[['closeAuth']]],['^cell',[['cell']]]]}}" bind:closeAuth="__e" bind:cell="__e"></auth></block><block s-if="{{needBind}}"><bind-phone vue-id="2" needBind="{{needBind}}" data-event-opts="{{[['^closeBind',[['closeBind']]]]}}" bind:closeBind="__e"></bind-phone></block><block s-if="{{take_self==2}}"><view class="flex-row nav"><view data-nav="3" data-event-opts="{{[['tap',[['revisenav',['$event']]]]]}}" bindtap="__e"><view class="{{[nav==3?'active':'']}}">自选配送</view></view><view data-nav="1" data-event-opts="{{[['tap',[['revisenav',['$event']]]]]}}" bindtap="__e"><view class="{{[nav==1?'active':'']}}">{{ps_name}}</view></view><view data-nav="2" data-event-opts="{{[['tap',[['revisenav',['$event']]]]]}}" bindtap="__e"><view class="{{[nav==2?'active':'']}}">{{zq_name}}</view></view></view></block><block s-if="{{gwc_nodata}}"><view><view class="contents"><view class="normal"><view class="normal_top flex-row"><view>{{"有效商品"+normal_total+"件"}}</view><view class="flex1"></view><view data-event-opts="{{[['tap',[['manage',['$event']]]]]}}" bindtap="__e">管理</view></view><block s-for="{{mygwc.normal}}" s-for-item="item" s-for-index="index" s-key="index"><scroll-view class="pros" style="{{'border-radius:'+(index==mygwc.normal.length-1?'0 0 10rpx 10rpx;':'')+';'}}" scroll-x><view class="scroll_cons flex-row"><view class="cons" style="width:101%;"><view class="cons_top flex-row"><view class="choose" data-index="{{index}}" data-num="{{item.num}}" data-id="{{item.id}}" data-price="{{item.pro_info.discount_price*item.num}}" data-event-opts="{{[['tap',[['xuanz',['$event']]]]]}}" bindtap="__e"><block s-if="{{item.ck==0}}"><image style="z-index:1;" src="{{$imgurl+'change1.png'}}"></image></block><block s-if="{{item.ck==1}}"><image style="z-index:1;" src="{{$imgurl+'change_h1.png'}}"></image></block></view><view class="thumb"><image src="{{item.pro_info.thumb}}"></image></view><view class="proinfo"><view class="title">{{item.pro_info.title}}</view><view class="ggz"><block s-if="{{item.pro_info.ggz}}"><text>{{"规格:"+item.pro_info.ggz}}</text></block></view><view class="price"><text>{{"￥"+item.pro_info.discount_price}}</text><block s-if="{{item.pro_info.discounts>0}}"><text class="discount_price">会员专享价</text></block></view></view></view><view class="cons_bottom flex-row"><block s-if="{{item.num>1}}"><view class="iconfont icon-x-jian" data-index="{{index}}" data-event-opts="{{[['tap',[['delbtn',['$event']]]]]}}" bindtap="__e"></view></block><block s-if="{{item.num==1}}"><view class="iconfont icon-x-jian"></view></block><view class="color-434343">{{item.num}}</view><view class="iconfont icon-x-jia" data-index="{{index}}" data-event-opts="{{[['tap',[['addbtn',['$event']]]]]}}" bindtap="__e"></view></view></view><view class="del" data-id="{{item.id}}" data-event-opts="{{[['tap',[['deldata',['$event']]]]]}}" bindtap="__e">删除</view><block s-if="{{item.uid<-1000}}"><block>兼容H5删除单独选择不实时显示问题</block></block></view></scroll-view></block></view><block s-if="{{invalid_total>0}}"><view class="invalid"><view class="normal_top flex-row" style="background:#FFFFFF;margin-top:20rpx;"><view>{{"无效商品"+invalid_total+"件"}}</view><view class="flex1"></view><view data-type="{{1}}" data-event-opts="{{[['tap',[['delgwcs',['$event']]]]]}}" bindtap="__e">清除无效商品</view></view><block s-for="{{mygwc.invalid}}" s-for-item="item" s-for-index="index" s-key="index"><view class="pros" style="{{'height:220rpx;'+(index==mygwc.invalid.length-1?'border-radius: 0 0 10rpx 10rpx;':'')}}"><view class="flex-row" style="border-top:2rpx solid #eeeeee;height:100%;"><view class="cons padding0"><view class="cons_top flex-row padding-30-20"><view class="choose"><image style="z-index:1;" src="{{$imgurl+'change0.png'}}"></image></view><view class="thumb"><image src="{{item.pro_info.thumb}}"></image></view><view class="proinfo"><view class="title">{{item.pro_info.title}}</view><view class="ggz"><text>抱歉,宝贝已停售了</text></view></view></view><view class="cons_bottom flex-row"><image class="gwc_invalid" src="{{$imgurl+'gwc_invalid.png'}}"></image></view></view></view></view></block></view></block></view><view class="footer_box"></view></view></block><block s-else><view><image style="width:280rpx;height:300rpx;margin:200rpx auto 100rpx;display:block;" src="{{$imgurl+'gwc_nodata.png'}}"></image><view style="text-align:center;font-size:28rpx;color:#666;">购物车还是空的喔~</view></view></block><view class="gwc_footer" style="{{'bottom:'+(isIphoneX==true?'150rpx':'110rpx')+';'}}"><view data-event-opts="{{[['tap',[['allxuanz',['$event']]]]]}}" class="btn-alls" bindtap="__e"><block s-if="{{allxz==0}}"><image src="{{$imgurl+'change1.png'}}"></image></block><block s-if="{{allxz==1}}"><image src="{{$imgurl+'change_h1.png'}}"></image></block><text>全选</text></view><view class="flex1"></view><block s-if="{{manage_statue==0}}"><view class="allprice_desc"><view class="allprice"><text>合计：</text><text>{{"￥"+allprice}}</text></view><view class="color-969696">(不含运费及优惠)</view></view></block><block s-if="{{manage_statue==0}}"><view data-event-opts="{{[['tap',[['topay',['$event']]]]]}}" class="gm_del" style="{{(jiesuan==0?'background:#666666':'background:#ec5c3e')}}" bindtap="__e">去结算</view></block><block s-if="{{manage_statue==1}}"><view class="gm_del" style="{{(jiesuan==0?'background:#666666':'background:#ec5c3e')}}" data-type="{{2}}" data-event-opts="{{[['tap',[['delgwcs',['$event']]]]]}}" bindtap="__e">删除</view></block></view><myfooter vue-id="3" page_signs="{{page_signs}}" baseinfo="{{baseinfo}}"></myfooter></view></block>