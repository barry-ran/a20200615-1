<block s-if="{{$imgurl}}"><view class="_div"><view class="head mb-20"><block s-for="{{processData}}" s-for-item="item" s-for-index="index" s-key="index"><block><view class="headBgColor" style="{{'background:'+(item['bgcolor'])+';'+('display:'+(grade!=item.grade?'none':'block')+';')}}"><image src="{{item['card_img']}}" mode="widthFix"></image><text class="card_num color-c4ad7d">{{vipid}}</text><view class="card_name"><view></view><text class="color-c4ad7d">{{item['name']}}</text><view></view></view></view><image class="head_bg w-750" style="height:100% !important;" src="{{$imgurl+'grade_bg.png'}}" mode="widthFix"></image></block></block></view><scroll-view scroll-x scroll-into-view="{{vids}}" scroll-with-animation="true"><block s-for="{{processData}}" s-for-item="item" s-for-index="ids" s-key="ids"><view class="process_wrap" data-current="{{ids}}" id="{{'v'+ids}}" data-event-opts="{{[['tap',[['swichNav',['$event']]]]]}}" bindtap="__e"><view class="process"><image src="{{item.vn}}"></image></view><text class="{{['process_name',item.grade<=grade?'color-434343':'']}}">{{item.name}}</text><view class="process_down"><view class="{{['process_line',item.grade<=grade?'border-434343':'border-ebebed']}}"></view><view class="{{['process_radius',currentTab==ids?'bg-e1c386':'bg-b9b9be']}}"></view><view class="{{['process_line',item.grade<grade?'border-434343':'border-ebebed']}}"></view></view></view></block></scroll-view><swiper class="swiper" current="{{currentTab}}" data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" bindchange="__e"><block s-for="{{processData}}" s-for-item="item" s-for-index="idxs" s-key="idxs"><block><swiper-item><view><block s-if="{{item['status']==1}}"><block><block s-if="{{item['cha']>0}}"><view class="open1_middle"><text>{{"距离本级还需消费"+item['cha']+"元，已消费"+item['yi']+"元"}}</text></view></block></block></block><view class="quanyi"><view></view><text>{{item['name']+"权益"}}</text><view></view></view><block s-if="{{item['coupon_flag']==1&&item['coupon_num']>0}}"><view class="open1_foot"><view><image src="{{$imgurl+'VIP1.png'}}"></image></view><text style="color:#434343;">{{"赠送优惠券 "+item['coupon_num']+"张"}}</text></view></block><block s-if="{{item['free_package']==1}}"><view class="open1_foot"><view><image src="{{$imgurl+'VIP1.png'}}"></image></view><text style="color:#434343;">会员包邮</text></view></block><block s-if="{{item['discount_flag']==1}}"><view class="open1_foot"><view><image src="{{$imgurl+'VIP1.png'}}"></image></view><text style="color:#434343;">{{"会员折扣 "+item['discount_grade']+"折"}}</text></view></block><block s-if="{{item['score_flag']==1}}"><view class="open1_foot"><view><image src="{{$imgurl+'VIP1.png'}}"></image></view><text style="color:#434343;">{{"买送积分 "+item['score_bei']+"倍"}}</text></view></block><block s-if="{{item['score_feedback_flag']==1}}"><view class="open1_foot"><view><image src="{{$imgurl+'VIP1.png'}}"></image></view><text style="color:#434343;">{{"积分回馈 "+item['score_feedback']}}</text></view></block><block s-if="{{item['score_feedback_flag']==0&&item['score_flag']==0&&item['discount_flag']==0&&item['coupon_flag']==0&&item['free_package']==0}}"><view class="open1_foot"><text style="color:#434343;">暂无会员权益</text></view></block><view class="descs"><view>等级说明：</view><block s-if="{{item.descs!=''}}"><view>{{item.descs}}</view></block><block s-else><view>暂无说明</view></block></view><block s-if="{{item['grade']>grade}}"><block><block s-if="{{item['status']==1}}"><block><view class="prompt"><text style="font-size:30rpx;">￥</text><text style="font-size:48rpx;">{{item['price']}}</text><text>/</text><text style="font-size:28rpx;">终身权益</text></view><block s-if="{{item['price_flag']==1}}"><view style="text-align:center;font-size:24rpx;color:#e1c386;">支付金额将充入余额</view></block><button class="btn1" data-grade="{{item['grade']}}" data-price="{{item['price']}}" data-event-opts="{{[['tap',[['open_grade',['$event']]]]]}}" bindtap="__e"><view>立即开通</view></button></block></block></block></block></view></swiper-item></block></block></swiper><block s-if="{{baseinfo.tabbar_t==1}}"><copyright vue-id="1" baseinfo="{{baseinfo}}"></copyright></block><myfooter vue-id="2" page_signs="{{page_signs}}" baseinfo="{{baseinfo}}"></myfooter></view></block>