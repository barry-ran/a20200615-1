<block s-if="{{$imgurl}}"><view><block s-if="{{needAuth}}"><auth vue-id="1" needAuth="{{needAuth}}" data-event-opts="{{[['^closeAuth',[['closeAuth']]],['^cell',[['cell']]]]}}" bind:closeAuth="__e" bind:cell="__e"></auth></block><block s-if="{{needBind}}"><bind-phone vue-id="2" needBind="{{needBind}}" data-event-opts="{{[['^closeBind',[['closeBind']]]]}}" bind:closeBind="__e"></bind-phone></block><block s-if="{{address_is==0}}"><block><image class="pageno_address" src="{{$imgurl+'no_store.png'}}"></image><view style="text-align:center;color:#666;">还没有收货地址哟~</view></block></block><block s-if="{{address_is==1}}"><block><scroll-view style="{{'height:'+(h+'px')+';'}}" scroll-y="true"><block s-for="{{myaddress}}" s-for-item="item" s-for-index="key" s-key="key"><view class="list"><view class="flex-row flex-shu-center flex-between pd1020" data-id="{{item.id}}" data-event-opts="{{[['tap',[['xuanzaddress',['$event']]]]]}}" bindtap="__e"><view style="color:#232323;"><text class="add_usename">{{item.name}}</text><text>{{item.mobile}}</text></view></view><view class="pd1020 user_address" data-id="{{item.id}}" data-event-opts="{{[['tap',[['xuanzaddress',['$event']]]]]}}" bindtap="__e">{{item.address+" "+item.more_address}}</view><view style="height:2rpx;background:#efefef;margin:20rpx 0 0;"></view><view class="flex-row flex-shu-center flex-between pd1020"><view class="flex-row flex-shu-center" data-id="{{item.id}}" data-event-opts="{{[['tap',[['moren_set',['$event']]]]]}}" bindtap="__e"><block s-if="{{item.is_mo==2}}"><image style="width:30rpx;margin-right:10rpx;" src="{{$imgurl+'change_h1.png'}}" mode="widthFix"></image></block><block s-if="{{item.is_mo==1}}"><image style="width:30rpx;margin-right:10rpx;" src="{{$imgurl+'btn_c_n.png'}}" mode="widthFix"></image></block><block s-if="{{item.is_mo==2}}"><view class="is_moadd">默认收货地址</view></block><block s-if="{{item.is_mo==1}}"><view class="is_moadd">设为默认收货地址</view></block></view><view class="flex-row flex-shu-center is_moadd"><view class="flex-row flex-shu-center is_moadd" data-id="{{item.id}}" data-event-opts="{{[['tap',[['add_address',['$event']]]]]}}" bindtap="__e"><view class="iconfont icon-x-bianji1 icon_both"></view><view class="mr30">编辑</view></view><view class="flex-row flex-shu-center is_moadd" data-id="{{item.id}}" data-event-opts="{{[['tap',[['deladdress',['$event']]]]]}}" bindtap="__e"><view class="iconfont icon-x-delete icon_both"></view><view>删除</view></view></view></view></view></block></scroll-view></block></block><view class="foot"><view data-event-opts="{{[['tap',[['add_address',['$event']]]]]}}" class="flex-row flex-shu-center" style="{{'width:'+('100%')+';'+('background:'+(baseinfo.base_color)+';')+('color:'+(baseinfo.tabbar_bg2)+';')+('border-radius:'+('80rpx')+';')}}" bindtap="__e"><view>新增地址</view></view></view><block s-if="{{choose==1}}"><view class="choose" style="{{'height:'+(choose_h+'px')+';'}}"><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]],['reset',[['formReset',['$event']]]]]}}" bindsubmit="__e" bindreset="__e"><view class="address_input"><view class="address_change"><view>{{changeAdd==1?'修改地址':'新增地址'}}</view><view data-event-opts="{{[['tap',[['choose_close',['$event']]]]]}}" class="iconfont icon-x-guanbi icon_close" bindtap="__e"></view></view><view><view class="flex_bflow line"><view class="type">收件人</view><input placeholder="请输入真实姓名" placeholder-style="color:#d7d7d7" name="realname" data-event-opts="{{[['input',[['input_name',['$event']]]]]}}" value="{{addressinfo.name}}" bindinput="__e"/></view><view class="flex_bflow line"><view class="type">手机号</view><input placeholder="请输入收件人手机号" placeholder-style="color:#d7d7d7" name="mobile" type="number" maxlength="11" data-event-opts="{{[['input',[['input_mobile',['$event']]]]]}}" value="{{addressinfo.mobile}}" bindinput="__e"/></view><view class="flex_bflow line" style="align-items:center;"><view class="type">所在地区</view><view data-event-opts="{{[['tap',[['toggleTab',['$event']]]]]}}" style="width:100%;text-align:right;" bindtap="__e">{{region[0]+"，"+region[1]+"，"+region[2]}}</view><w-picker class="vue-ref" vue-id="3" mode="{{mode}}" defaultVal="{{defaultVal}}" themeColor="#f00" data-ref="picker" data-event-opts="{{[['^confirm',[['onConfirm']]]]}}" bind:confirm="__e"></w-picker></view><view class="more__address"><view class="add_moreaddress">详细地址</view><input class="more_addinp" placeholder="请输入详细地址" placeholder-style="color:#d7d7d7" name="more_address" data-event-opts="{{[['input',[['input_address',['$event']]]]]}}" value="{{addressinfo.more_address}}" bindinput="__e"/></view></view><view class="be_moadd flex_bflow"><view>设为默认地址</view><switch style="transform:scale(0.7);" checked="{{is_checked}}" data-event-opts="{{[['change',[['switchChange',['$event']]]]]}}" bindchange="__e"></switch></view><view class="sub"><button class="ffmd" style="{{'background:'+(baseinfo.base_color)+';'}}" formType="submit">确定</button></view></view></form></view></block></view></block>