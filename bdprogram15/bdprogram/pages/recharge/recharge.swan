<block s-if="{{$imgurl&&baseinfo.recharge==1}}"><view><block s-if="{{needAuth}}"><auth vue-id="1" needAuth="{{needAuth}}" data-event-opts="{{[['^closeAuth',[['closeAuth']]],['^cell',[['cell']]]]}}" bind:closeAuth="__e" bind:cell="__e"></auth></block><block s-if="{{needBind}}"><bind-phone vue-id="2" needBind="{{needBind}}" data-event-opts="{{[['^closeBind',[['closeBind']]]]}}" bind:closeBind="__e"></bind-phone></block><view class="recharge_head" style="{{'background:'+(baseinfo.base_color)+';'+('color:'+('#fff')+';')}}"><view><view class="row_bt">当前余额</view><view><text>{{"¥"+guiz.user.money}}</text></view></view><view style="width:2rpx;height:80rpx;background:#fff;"></view><view><view>当前积分</view><view>{{guiz.user.score}}</view></view></view><view class="recharge_pro"><text></text><text>充值套餐</text><text></text></view><view class="bg_f mb10 recharge_main"><block s-for="{{guiz.list}}" s-for-item="item" s-for-index="key" s-key="key"><block><view class="coupon" data-money="{{item.money}}" data-event-opts="{{[['tap',[['ChargeMoney',['$event']]]]]}}" bindtap="__e"><view>{{"充"+item.money+"元"}}</view><view>{{item.allmoney+"元"+(item.getscore!=0?'+'+item.getscore+'积分':'')}}</view><view class="coupon_view">{{item.coupon_con!=null?'优惠券'+item.coupon_num+'张':''}}</view><block s-if="{{item.getmoney>0}}"><view class="coupon_zeng"><image src="{{$imgurl+'recharge_bg.png'}}" mode></image><text>{{"赠"+item.getmoney+"元"}}</text></view></block></view></block></block></view><view class="bg_f mb10"><view class="money_box"><input class="row_bc" type="digit" placeholder="请输入充值金额" data-event-opts="{{[['input',[['setmoney',['$event']]]]]}}" value="{{money}}" bindinput="__e"/><text>元</text></view></view><view class="xld"><button data-event-opts="{{[['tap',[['setsubmit',['$event']]]]]}}" class="wxzhi" style="{{'background:'+(baseinfo.base_color)+';'+('color:'+('#fff')+';')}}" bindtap="__e">百度支付</button></view></view></block>