<block s-if="{{$imgurl}}"><view><block s-if="{{storelists}}"><view class="store_bg"><block s-for="{{storelists}}" s-for-item="item" s-for-index="key" s-key="key"><view data-id="{{key}}" data-stid="{{item.id}}" data-event-opts="{{[['tap',[['chooseStore',['$event']]]]]}}" bindtap="__e"><view class="storelists"><view class="store_title">{{item.title}}</view><view class="flex-row store_dizhi"><view class="iconfont icon-c-dizhishixin store_add"></view><view class="store_address">{{item.province+item.city+item.country}}</view></view><view class="flex-row store_phone"><view class="iconfont icon-x-dianhua2 store_add"></view><view class="store_address">{{item.tel}}</view></view><block s-if="{{item.times}}"><view class="store_time"><view>{{"营业时间："+item.times}}</view></view></block><block s-if="{{stid==item.id}}"><view class="xuanz_gou"><view class="iconfont icon-x-gou" style="color:#fff;font-size:22rpx;margin-left:32rpx;margin-top:4rpx;"></view></view></block></view></view></block></view></block><block s-if="{{!storelists}}"><view><image class="pageno_address" src="{{$imgurl+'no_store.png'}}"></image><view style="text-align:center;color:#666;">还没有自取门店哟~</view></view></block></view></block>