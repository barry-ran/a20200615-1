<block s-if="{{$imgurl}}"><view style="position:fixed;overflow-y:scroll;top:0;left:0;right:0;bottom:0;"><block s-if="{{needAuth}}"><auth vue-id="1" needAuth="{{needAuth}}" data-event-opts="{{[['^closeAuth',[['closeAuth']]],['^cell',[['cell']]]]}}" bind:closeAuth="__e" bind:cell="__e"></auth></block><block s-if="{{needBind}}"><bind-phone vue-id="2" needBind="{{needBind}}" data-event-opts="{{[['^closeBind',[['closeBind']]]]}}" bind:closeBind="__e"></bind-phone></block><view class="toubu"><view class="yhq check"><view class="check_on">可领优惠券</view></view><view data-event-opts="{{[['tap',[['mycoupp',['$event']]]]]}}" class="wyhq" bindtap="__e">我的优惠券</view></view><view style="height:86rpx;"></view><view class="couponlist_box"><block s-for="{{couponlist}}" s-for-item="item" s-for-index="key" s-key="key"><view class="youhqs"><block s-if="{{item.isover_time==1}}"><block><view class="yuan_left"></view><view class="yuan_right"></view><view class="wkk"><view class="wkk_top"><view class="shujl" style="{{'color:'+(baseinfo.base_color)+';'}}"><text class="jiagq"><text style="font-size:42rpx;font-weight:normal;">￥</text>{{item.price+''}}</text><text class="limit_count" style="{{'display:'+(item.xz_count==0?'none':'inline-block')+';'+('border:'+('2rpx solid '+baseinfo.base_color)+';')}}">限领<block s-if="{{item.xz_count>0}}"><text>{{item.xz_count+"张"}}</text></block></text></view><view class="qitxx"><view class="yhq_t">{{''+item.title+''}}</view><view class="xiaozi"><block s-if="{{item.pay_money==0}}"><text>任意金额可用</text></block><block s-else><text>{{"满"+item.pay_money+"可用"}}</text></block></view><view class="xiaozi" style="margin-top:18rpx;">有效期：<block s-if="{{item.use_contents['use_type']==0&&item.use_contents['use_time']['use_btime']==0&&item.use_contents['use_time']['use_etime']==0}}"><text>永久有效</text></block><block s-if="{{item.use_contents['use_type']==0&&item.use_contents['use_time']['use_btime']==0&&item.use_contents['use_time']['use_etime']!=0}}"><text>{{item.use_contents['use_time']['use_etime']+"前有效"}}</text></block><block s-if="{{item.use_contents['use_type']==0&&item.use_contents['use_time']['use_btime']!=0&&item.use_contents['use_time']['use_etime']==0}}"><text>{{item.use_contents['use_time']['use_btime']+"后有效"}}</text></block><block s-if="{{item.use_contents['use_type']==0&&item.use_contents['use_time']['use_btime']!=0&&item.use_contents['use_time']['use_etime']!=0}}"><text>{{item.use_contents['use_time']['use_btime']+"至"+item.use_contents['use_time']['use_etime']}}</text></block><block s-if="{{item.use_contents['use_type']==1&&item.use_contents['use_time']!=1}}"><text>{{"领取当日 "+item.use_contents['use_time']+" 天内有效"}}</text></block><block s-if="{{item.use_contents['use_type']==1&&item.use_contents['use_time']==1}}"><text>领取当日有效</text></block><block s-if="{{item.use_contents['use_type']==2&&item.use_contents['use_time']!=1}}"><text>{{"领取次日 "+item.use_contents['use_time']+" 天内有效"}}</text></block><block s-if="{{item.use_contents['use_type']==2&&item.use_contents['use_time']==1}}"><text>领取次日有效</text></block></view></view><block s-if="{{item.kc!=-1}}"><view class="lijlq"><view class="lijlq_num" style="{{'color:'+(baseinfo.base_color)+';'}}">{{"已领"+item.pers+"%"}}</view><progress percent="{{item.pers}}" stroke-width="2" activeColor="{{baseinfo.base_color}}" backgroundColor="#eee"></progress></view></block></view><view class="include_types"><block s-if="{{item.use_goods_contents.type==1}}"><view class="include_items"><view class="{{['include_words',hide_type==1&&couid==key?'':'include_words_no']}}">{{'适用品类：'+item.use_goods_contents.contents+''}}</view><view class="{{['iconfont',hide_type==1&&couid==key?'icon-x-shang':'icon-x-xia']}}" data-couid="{{key}}" data-event-opts="{{[['tap',[['showType',['$event']]]]]}}" bindtap="__e"></view></view></block><block s-else><view class="include_items"><view class="include_words">适用品类：全类目</view></view></block><view><block s-if="{{kaiq==1}}"><block><block s-if="{{item.is_get==1}}"><button class="ljlq" style="{{'background:'+(baseinfo.base_color)+';'}}" data-id="{{item.id}}" data-event-opts="{{[['tap',[['getit_zj',['$event']]]]]}}" bindtap="__e">立即领取</button></block></block></block><block s-if="{{item.is_get==0}}"><button class="ljlq2">已领取</button></block><block s-if="{{item.kc<=0&&item.kc!=-1}}"><button class="ljlq2">已领完</button></block></view></view></view></block></block></view></block><block s-if="{{couponlist_lenght==0}}"><block><image class="pageno_address" src="{{$imgurl+'no_active.png'}}"></image><view style="text-align:center;color:#666;">还没有相关内容哟~</view></block></block></view><copyright vue-id="3" baseinfo="{{baseinfo}}"></copyright><myfooter vue-id="4" page_signs="{{page_signs}}" baseinfo="{{baseinfo}}"></myfooter></view></block>