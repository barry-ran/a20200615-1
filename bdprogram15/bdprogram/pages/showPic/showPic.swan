<block s-if="{{$imgurl}}"><view><block s-if="{{needAuth}}"><auth vue-id="1" needAuth="{{needAuth}}" data-event-opts="{{[['^closeAuth',[['closeAuth']]]]}}" bind:closeAuth="__e"></auth></block><block s-if="{{needBind}}"><bind-phone vue-id="2" needBind="{{needBind}}" data-event-opts="{{[['^closeBind',[['closeBind']]]]}}" bind:closeBind="__e"></bind-phone></block><view class="top_nav"><view data-event-opts="{{[['tap',[['navback',['$event']]]]]}}" class="top_nav_back" bindtap="__e"><view class="iconfont icon-x-back" style="{{'color:'+(baseinfo.base_tcolor)+';'}}"></view></view></view><view class="trump"><view class="trump_top"><image src="{{thumb}}" mode="aspectFill"></image><view class="trump_cont"><view class="trump_con_title">{{title}}</view><view class="trump_con_num">{{piclist_num+"张"}}</view></view><block s-if="{{baseinfo.share_open==1}}"><block><block s-if="{{get_share_gz==1}}"><view data-event-opts="{{[['tap',[['share111',['$event']]]]]}}" class="shareBtnPic" bindtap="__e"><text class="iconfont icon-x-fenxiang2"></text><text class="text">转发</text></view></block></block></block></view><view class="pic_imglist"><block s-for="{{picList}}" s-for-item="item" s-for-index="key" s-key="key"><block><navigator class="pic_imgitem" url="{{'/pages/showPiclist/showPiclist?id='+id+'&key='+key}}"><image src="{{item}}" mode="aspectFill"></image></navigator></block></block></view></view><block s-if="{{shareShow==1}}"><block><view class="mask"></view><view class="shareBox"><image src="{{$imgurl+'share_ok.png'}}" mode="widthFix"></image><block s-if="{{shareNotice==0}}"><view class="shareText">分享成功，<label class="_span">{{"+"+shareScore+"积分"}}</label></view></block><block s-if="{{shareNotice==1}}"><view class="shareText shareText2">您今日分享次数较多，本次不增加积分，感谢分享！</view></block><navigator class="shareBtn" open-type="redirectTo" url="/pages/usercenter/usercenter">查看我的积分</navigator><view data-event-opts="{{[['tap',[['shareClo',['$event']]]]]}}" class="shareClo" bindtap="__e"></view></view></block></block><block s-if="{{share==1}}"><view data-event-opts="{{[['tap',[['share_close',['$event']]]]]}}" class="mask" bindtap="__e"></view></block><block s-if="{{share==1}}"><view class="share_ceng" style="{{'z-index:'+(shareimg==1?'800':1000)+';'}}"><view class="share_con flex-row"><view class="share_con_box"><button class="flex-row" open-type="share" data-event-opts="{{[['tap',[['onShareAppMessage',['$event']]]]]}}" bindtap="__e"><view class="iconfont icon-x-fenxiang5" style="font-size:40rpx;color:#56bb3a;"></view><view style="margin-left:10rpx;color:#333;">分享给好友</view></button></view><view class="share_con_box"><view data-event-opts="{{[['tap',[['getShareImg',['$event']]]]]}}" class="flex-row" style="justify-content:center;" bindtap="__e"><view class="iconfont icon-x-tupian" style="font-size:40rpx;color:#e47b2f;"></view><view style="margin-left:10rpx;">生成分享海报</view></view></view></view></view></block><block s-if="{{shareimg==1}}"><view class="fx_box" style="{{'width:'+(img_w+20+'px')+';'+('height:'+(img_h+25+'px')+';')+('left:'+((system_w-img_w-10)/2+'px')+';')}}"><view class="fx_close"><view data-event-opts="{{[['tap',[['closeShare',['$event']]]]]}}" class="fx_X" bindtap="__e">x</view><view class="fx_content"><image class="haibao_img _img" style="{{'width:'+(img_w+'px')+';'+('height:'+(img_h+'px')+';')}}" src="{{shareimg_url}}"></image></view><button class="haibao_btn" type="primary" data-event-opts="{{[['tap',[['saveImg',['$event']]]]]}}" bindtap="__e">保存图片并分享</button></view></view></block></view></block>