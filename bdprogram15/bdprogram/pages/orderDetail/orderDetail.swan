<block s-if="{{$imgurl}}"><view class="bgcolor _div"><view class="order_detail_head" style="{{'background-image:'+('url('+$imgurl+'order_detail_head.jpg)')+';'+('background-size:'+('100%')+';')}}"><block s-if="{{datas.flag==0}}"><view>订单待付款</view></block><block s-if="{{datas.flag==1&&datas.nav==1}}"><view>订单待发货</view></block><block s-if="{{datas.flag==1&&datas.nav==2}}"><view>订单待消费</view></block><block s-if="{{datas.flag==2}}"><view>订单已完成</view></block><block s-if="{{datas.flag==3}}"><view>订单已过期</view></block><block s-if="{{datas.flag==4}}"><view>订单待收货</view></block><block s-if="{{datas.flag==5}}"><view>订单已取消</view></block><block s-if="{{datas.flag==6}}"><view>订单取消中</view></block><block s-if="{{datas.flag==7}}"><view>退货审核中</view></block><block s-if="{{datas.flag==8}}"><view>退货成功</view></block><block s-if="{{datas.flag==9}}"><view>退货失败</view></block></view><block s-if="{{datas.address>0}}"><block><view class="order_detial_person_info hbj mb10 "><view class="order_detail_address_img iconfont icon-x-dizhi2"></view><view class="person_info_left flex1"><view class="hbj"><view class="recive_person flex1">{{"收货人:"+datas.addressinfo.name}}</view><view class="recive_phonenum">{{datas.addressinfo.mobile}}</view></view><view class="recive_address">{{"收货地址："+datas.addressinfo.address+" "+datas.addressinfo.more_address}}</view></view></view></block></block><view class="order_list_productbox" style="background:#fff;padding:20rpx;"><block s-for="{{datas.jsondata}}" s-for-item="item" s-for-index="key" s-key="key"><block><view class="order_list_product hbj"><image class="product_img" src="{{item.proinfo.thumb}}" mode="aspectFill"></image><view class="order_list_product_center"><view class="order_product_title2">{{item.baseinfo.title}}</view></view><view class="flex1"></view><view><view class="order_product_price"><text>￥</text>{{item.proinfo.price}}</view><view class="order_product_count">{{"X"+item.num}}</view></view></view></block></block></view><view class="pricebox mb10"><view class="price_single hbj"><view class="price_single_left flex1">商品总价</view><view class="price_single_right">{{"￥"+datas.hjjg}}</view></view><block s-if="{{datas['discounts']>0}}"><block><view class="price_single hbj"><view class="price_single_left flex1">折扣</view><view class="price_single_right">{{datas['discounts']+"折"}}</view></view></block></block><block s-if="{{datas['discounts_price']>0}}"><block><view class="price_single hbj"><view class="price_single_left flex1">折扣优惠</view><view class="price_single_right">{{"- ￥"+datas['discounts_price']}}</view></view></block></block><block s-if="{{datas['yhInfo_mj']['money']>0}}"><block><view class="price_single hbj"><view class="price_single_left flex1">满减</view><view class="price_single_right">{{datas['yhInfo_mj']['msg']+" - ￥"+datas['yhInfo_mj']['money']}}</view></view></block></block><block s-if="{{datas['yhInfo_yhq']['money']>0}}"><block><view class="price_single hbj"><view class="price_single_left flex1">优惠券</view><view class="price_single_right">{{datas['yhInfo_yhq']['msg']+" - ￥"+datas['yhInfo_yhq']['money']}}</view></view></block></block><block s-if="{{datas['yhInfo_score']['money']>0}}"><block><view class="price_single hbj"><view class="price_single_left flex1">积分抵扣</view><view class="price_single_right">{{datas['yhInfo_score']['msg']+" - ￥"+datas['yhInfo_score']['money']}}</view></view></block></block><block s-if="{{datas.sid==0}}"><view class="price_single hbj"><view class="price_single_left flex1">运费价格</view><view class="price_single_right">{{"￥"+datas['yhInfo_yunfei']}}</view></view></block><view class="price_single hbj"><view class="price_single_left2 flex1">实付款<text>{{"(另外支付"+datas.payprice+"元,余额支付"+datas.pay_yue+"元)"}}</text></view><view class="price_single_right2"><text>￥</text>{{datas.true_price}}</view></view></view><block s-if="{{datas.store_info!=''}}"><view class="order_info mb10"><view class="price_single_left2">自提门店</view><view class="order_info_ddh_left mt-10">门店名称：<text>{{datas.store_info['store_name']}}</text></view><view class="order_info_ddh_left mt-10" data-tel="{{datas.store_info['store_tel']}}" data-event-opts="{{[['tap',[['makePhoneCallC',['$event']]]]]}}" bindtap="__e">门店电话：<text>{{datas.store_info['store_tel']}}<text style="font-size:20rpx;margin-left:10rpx;">[点击拨号]</text></text></view><view class="order_info_ddh_left mt-10">门店地址：<text>{{datas.store_info['store_address']}}</text></view><view class="order_info_ddh_left mt-10">营业时间：<text>{{datas.store_info['store_hours']==''?'请联系店家':datas.store_info['store_hours']}}</text></view></view></block><view class="order_info mb10"><view class="order_info_ddh hbj"><view class="order_info_ddh_left flex1">订单号：<text>{{datas.order_id}}</text></view><view class="copy_btn" id="{{datas.order_id}}" data-event-opts="{{[['tap',[['copy',['$event']]]]]}}" bindtap="__e">复制</view></view><view class="order_info_ddh_left mt-10">下单时间：<text>{{datas.creattime}}</text></view></view><block s-if="{{datas.beizhu_val&&datas.beizhu_val!='undefined'}}"><view class="detail_bz hbj mb10"><text class="iconfont icon-x-tishi1"></text>{{"备注："+datas.beizhu_val+''}}</view></block><view style="height:140px;"></view><view class="detail_btnbox hbj"><view class="flex1"></view><block s-if="{{datas.flag==0}}"><view class="detail_btn"><block s-if="{{datas.sid==0}}"><block><navigator url="{{'/pages/order_more/order_more?orderid='+datas.order_id+'&again=1'}}">立即付款</navigator></block></block><block s-if="{{datas.sid!=0}}"><block><navigator url="{{'/pagesPluginShop/goods_buy/goods_buy?orderid='+datas.order_id+'&again=1'}}">立即付款</navigator></block></block></view></block><block s-if="{{datas.flag==1}}"><form data-order="{{datas.order_id}}" report-submit="true" data-event-opts="{{[['submit',[['tuikuan',['$event']]]]]}}" bindsubmit="__e"><button class="detail_btn" style="background:#fff;line-height:50rpx;" form-type="submit">取消订单</button></form></block><block s-if="{{showhx==1}}"><view class="hx_con"><view data-event-opts="{{[['tap',[['hxhide',['$event']]]]]}}" class="mask" bindtap="__e"></view><view class="hexiao"><view class="hx_tit">请输入核销密码</view><view><input class="hx_ipt" password type="number" data-event-opts="{{[['input',[['hxmmInput',['$event']]]]]}}" value="{{hxmm}}" bindinput="__e"/></view><view><button data-event-opts="{{[['tap',[['hxmmpass',['$event']]]]]}}" class="hx_btn" bindtap="__e">确认消费</button></view></view><view data-event-opts="{{[['tap',[['hxhide',['$event']]]]]}}" class="hx_c" bindtap="__e"><image src="{{$imgurl+'c.png'}}" mode="aspectFit"></image></view></view></block><block s-if="{{showmask==1}}"><view class="mask"></view></block><block s-if="{{showmask==1}}"><view class="fill_info"><view style="text-align:center;color:#000;margin-bottom:20rpx;">填写退货信息</view><picker style="border:1px solid #eee;heigth:60rpx;" value="{{index}}" range="{{kuaidi}}" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" bindchange="__e"><view class="picker register_form_view1_input">{{kuaidi[index]}}</view></picker><view class="register_form_view1"><input class="register_form_view1_input" placeholder="快递号/信息" data-event-opts="{{[['input',[['changekdh',['$event']]]]]}}" bindinput="__e"/></view><view style="display:flex;flex-flow:row;"><view data-event-opts="{{[['tap',[['changekdinfo',['$event']]]]]}}" class="fillinfo_cancel" bindtap="__e">提交</view><view data-event-opts="{{[['tap',[['cancelkdinfo',['$event']]]]]}}" class="fillinfo_submit" bindtap="__e">取消</view></view></view></block><block s-if="{{datas.flag==4}}"><view class="detail_btn" id="{{datas.order_id}}" data-event-opts="{{[['tap',[['qrshouh',['$event']]]]]}}" bindtap="__e">确认收货</view></block><block s-if="{{datas.flag==4}}"><view class="detail_btn" id="{{datas.order_id}}" data-event-opts="{{[['tap',[['tuihuo',['$event']]]]]}}" bindtap="__e">申请退款</view></block><view data-event-opts="{{[['tap',[['makephonecall',['$event']]]]]}}" class="detail_btn" bindtap="__e">联系商家</view></view></view></block>