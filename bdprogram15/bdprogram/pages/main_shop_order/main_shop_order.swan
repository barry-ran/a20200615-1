ight"><view class="order_list_btn" data-order_id="{{item.order_id}}" data-event-opts="{{[['tap',[['cancelOrderNoPay',['$event']]]]]}}" bindtap="__e">取消订单</view><view class="order_list_price_btn" data-order_id="{{item.order_id}}" data-event-opts="{{[['tap',[['paybox',['$event']]]]]}}" bindtap="__e">立即支付</view></view></block><block s-if="{{item.status==1}}"><view class="allbtn_right"><block s-if="{{item.allow_all_refund==1}}"><view class="order_list_btn" data-order_id="{{item.order_id}}" data-event-opts="{{[['tap',[['toapplyall',['$event']]]]]}}" bindtap="__e">取消订单</view></block><view class="order_list_btn" data-kuaidi="{{item.express}}" data-kuaidihao="{{item.express_no}}" data-order_id="{{item.order_id}}" data-event-opts="{{[['tap',[['wlinfo',['$event']]]]]}}" bindtap="__e">查看物流</view></view></block><block s-if="{{item.status==2&&item.delivery_type==1}}"><view class="allbtn_right"><block s-if="{{item.express==-1}}"><block><view class="order_list_btn">商家配送</view></block></block><block s-else><block><view class="order_list_btn" data-kuaidi="{{item.express}}" data-kuaidihao="{{item.express_no}}" data-order_id="{{item.order_id}}" data-event-opts="{{[['tap',[['wlinfo',['$event']]]]]}}" bindtap="__e">查看物流</view></block></block><view class="order_list_price_btn" id="{{item.order_id}}" data-event-opts="{{[['tap',[['qrshouh',['$event']]]]]}}" bindtap="__e">确认收货</view></view></block><block s-if="{{item.status==2&&item.delivery_type==2}}"><view class="allbtn_right"><block s-if="{{item.allow_all_refund==1}}"><view class="order_list_btn" data-order_id="{{item.order_id}}" data-event-opts="{{[['tap',[['toapplyall',['$event']]]]]}}" bindtap="__e">取消订单</view></block><view class="order_list_price_btn" id="{{item.order_id}}" data-event-opts="{{[['tap',[['hxshow',['$event']]]]]}}" bindtap="__e">立即核销</view></view></block><view style="display:flex;justify-content:flex-end;"><block s-if="{{item.status==3}}"><view class="allbtn_right"><block s-if="{{item.express==-1}}"><block><view class="order_list_btn">商家配送</view></block></block><block s-else><block><block s-if="{{item.delivery_type==1}}"><view class="order_list_btn" data-kuaidi="{{item.express}}" data-kuaidihao="{{item.express_no}}" data-order_id="{{item.order_id}}" data-event-opts="{{[['tap',[['wlinfo',['$event']]]]]}}" bindtap="__e">查看物流</view></block></block></block><block s-if="{{item.assess==1&&item.is_in_supp==true}}"><view class="order_list_price_btn" data-order_id="{{item.order_id}}" data-event-opts="{{[['tap',[['goevaluate',['$event']]]]]}}" bindtap="__e">立即评价</view></block></view></block><block s-if="{{item.assess==2&&item.is_in_supp==true}}"><view class="allbtn_right"><view class="order_list_price_btn" data-order_id="{{item.order_id}}" data-event-opts="{{[['tap',[['goevaluate',['$event']]]]]}}" bindtap="__e">追加评价</view></view></block><block s-if="{{item.status==5}}"><view class="allbtn_right"><view class="order_list_btn" data-order_id="{{item.order_id}}" data-event-opts="{{[['tap',[['deleteOrder',['$event']]]]]}}" bindtap="__e">删除订单</view></view></block></view></view></view></block></view></block></block><block s-else><block><image class="pageno_address" src="{{$imgurl+'no_search.png'}}"></image><view style="text-align:center;color:#666;">还没有相关订单哟~</view></block></block></view><block s-if="{{showhx==1}}"><view class="hx_con"><view data-event-opts="{{[['tap',[['hxhide',['$event']]]]]}}" class="mask" bindtap="__e"></view><view class="share_con flex-row"><view class="share_con_box"><view data-event-opts="{{[['tap',[['gethxmima',['$event']]]]]}}" class="flex-row" style="border-right:2rpx solid #c6cbd9;align-items:center;justify-content:center;" bindtap="__e"><view class="iconfont icon-x-mima1" style="font-size:40rpx;color:#56bb3a;"></view><view style="margin-left:10rpx;">密码核销</view></view></view><view class="share_con_box"><view data-event-opts="{{[['tap',[['gethxImg',['$event']]]]]}}" class="flex-row" style="align-items:center;justify-content:center;" bindtap="__e"><view class="iconfont icon-x-erweima1" style="font-size:40rpx;color:#e47b2f;"></view><view style="margin-left:10rpx;">二维码核销</view></view></view></view><block s-if="{{hx_choose==1}}"><view><view data-event-opts="{{[['tap',[['hxhide1',['$event']]]]]}}" class="mask" style="z-index:920;" bindtap="__e"></view><view class="hexiao_inp"><view data-event-opts="{{[['tap',[['hxhide1',['$event']]]]]}}" class="hx_titc" bindtap="__e"><view class="iconfont icon-x-guanbi"></view></view><view class="hexiao_mima"><view class="hx_tit">请输入核销密码</view><view class="code"><block s-for="{{hxmm_list}}" s-for-item="item" s-for-index="index" s-key="index"><text class="{{[item.fs?'focus':'']}}" type="number">{{item.val}}</text></block></view><view class="code_input"><input type="number" focus="true" hover-class="none" maxlength="6" data-event-opts="{{[['input',[['hxmmInput',['$event']]]]]}}" value="{{hxmm}}" bindinput="__e"/></view><view class="hx_yes"><view data-event-opts="{{[['tap',[['hxmmpass',['$event']]]]]}}" class="hx_btn" style="{{'background:'+(baseinfo.base_color)+';'+('color:'+('#fff')+';')}}" bindtap="__e">确定</view></view></view></view></view></block><block s-if="{{hx_choose==2}}"><view><view data-event-opts="{{[['tap',[['hxhide1',['$event']]]]]}}" class="mask" style="z-index:920;" bindtap="__e"></view><view class="hexiao"><view data-event-opts="{{[['tap',[['hxhide1',['$event']]]]]}}" class="hx_titc" bindtap="__e"><view class="iconfont icon-x-guanbi"></view></view><view style="text-align:center;"><image class="hx_ewm" src="{{hx_ewm}}" mode="aspectFit"></image></view><view class="he_img_tips">请将二维码出示给工作人员</view></view></view></block></view></block><block s-if="{{showPay==1}}"><block><view data-event-opts="{{[['tap',[['payboxclose',['$event']]]]]}}" class="mask" bindtap="__e"></view><view class="pay_box"><view class="pay_function"><view>选择支付方式</view><view data-event-opts="{{[['tap',[['payboxclose',['$event']]]]]}}" class="iconfont icon-x-guanbi bsyyyd" bindtap="__e"></view></view><view class="pay_funs flex-row"><view class="pay_funsimg"><image src="{{$imgurl+'yue.png'}}" mode="aspectFit"></image></view><view class="pay_moneys flex_bflow"><view>{{"余额支付（剩余：￥"+mymoney+"元）"}}</view><block s-if="{{mymoney_pay==1}}"><view class="{{['iconfont icon-x-gou',choosepayf==0?'choose_jf':'no_jf']}}" data-pay_type="{{1}}" data-type="{{0}}" data-event-opts="{{[['tap',[['choosepay',['$event']]]]]}}" bindtap="__e"></view></block></view><block s-if="{{mymoney_pay==2}}"><view class="ye_less"></view></block></view><block s-if="{{pay_money>0}}"><block><view class="pay_funs flex-row"><view class="pay_funsimg"><image src="{{$imgurl+'baidu.png'}}" mode="aspectFit"></image></view><view class="pay_moneys flex_bflow"><view>百度支付</view><view class="{{['iconfont icon-x-gou',choosepayf==4?'choose_jf':'no_jf']}}" data-pay_type="{{2}}" data-type="{{4}}" data-event-opts="{{[['tap',[['choosepay',['$event']]]]]}}" bindtap="__e"></view></view></view></block></block><form report-submit="true" data-event-opts="{{[['submit',[['pay',['$event']]]]]}}" bindsubmit="__e"><button class="pay_submit" formType="submit">确定</button></form></view></block></block><myfooter vue-id="1" page_signs="{{page_signs}}" baseinfo="{{baseinfo}}"></myfooter></view></block>