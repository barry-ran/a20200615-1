<block s-if="{{$imgurl}}"><view><block s-if="{{needAuth}}"><auth vue-id="1" needAuth="{{needAuth}}" data-event-opts="{{[['^closeAuth',[['closeAuth']]]]}}" bind:closeAuth="__e"></auth></block><block s-if="{{needBind}}"><bind-phone vue-id="2" needBind="{{needBind}}" data-event-opts="{{[['^closeBind',[['closeBind']]]]}}" bind:closeBind="__e"></bind-phone></block><view class="imgs_swiper" style="{{'height:'+(system_h*2-80+'rpx')+';'}}"><swiper style="{{'height:'+(system_h*2-260+'rpx')+';'+('max-height:'+('910rpx')+';')}}" current="{{currentSwiper}}" circular="true" duration="1000" previous-margin="80rpx" next-margin="80rpx" data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" bindchange="__e"><block s-for="{{picList}}" s-for-item="item" s-for-index="key" s-key="key"><block><swiper-item class="imgs_swiper_item"><image class="{{['img_item',key==currentSwiper?'img_active':'']}}" src="{{item}}" mode="aspectFit" data-id="{{key}}"></image></swiper-item></block></block></swiper><view class="imageCount">{{currentSwiper+1+"/"+picList.length}}</view></view><view class="list_title"><view class="item_title">{{title}}</view></view></view></block>