<block s-if="{{$imgurl}}"><view class="_div"><block s-if="{{needAuth}}"><auth vue-id="1" needAuth="{{needAuth}}" data-event-opts="{{[['^closeAuth',[['closeAuth']]],['^cell',[['cell']]]]}}" bind:closeAuth="__e" bind:cell="__e"></auth></block><block s-if="{{needBind}}"><bind-phone vue-id="2" needBind="{{needBind}}" data-event-opts="{{[['^closeBind',[['closeBind']]]]}}" bind:closeBind="__e"></bind-phone></block><view class="pro_head"><view class="wrap" style="{{('height:'+minHeight+'px;')}}"><swiper class="slide" style="height:100%;" indicator-active-color="{{baseinfo.base_color2}}" interval="3000" duration="1000" autoplay="{{autoplay}}" indicator-color="rgba(0, 0, 0, .3)" data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" bindchange="__e"><block s-for="{{datas.slide}}" s-for-item="item" s-for-index="index" s-key="index"><block><swiper-item><image class="slide-image" src="{{item}}" width="100%" mode="widthFix" data-event-opts="{{[['load',[['swiperLoad',['$event']]]]]}}" bindload="__e"></image><block s-if="{{!isplay&&pic_video&&index==0}}"><view><image class="play-image" src="{{$imgurl+'play_audio.png'}}" mode="aspectFill" data-event-opts="{{[['tap',[['playvideo',['$event']]]]]}}" bindtap="__e"></image></view></block><block s-if="{{isplay}}"><view style="height:100%;"><image class="play-image1" src="{{$imgurl+'c.png'}}" mode="widthFix" data-event-opts="{{[['tap',[['endvideo',['$event']]]]]}}" bindtap="__e"></image><video style="margin:0;height:100%;width:100%;overflow:hidden;" src="{{pic_video}}" objectFit="cover" autoplay="true"></video></view></block></swiper-item></block></block></swiper><block s-if="{{!isplay}}"><view class="dots hbj" style="{{'width:'+((dlength-1)*14+dlength*14+'rpx')+';'}}"><block s-for="{{datas.slide}}" s-for-item="item" s-for-index="index" s-key="index"><block><view class="{{['dot',index==currentSwiper?'active':'']}}"></view></block></block></view></block></view><block s-if="{{autoplay}}"><view class="pro_tit">{{datas.title}}</view></block></view><view class="price"><view class="price1" style="{{'color:'+(baseinfo.base_color2)+';'+('border:'+('2rpx solid '+baseinfo.base_color2)+';')}}">所需积分</view><view class="price2" style="{{('color:'+baseinfo.base_color2)}}">{{datas.price}}</view><view class="price3 pline">{{"¥"+datas.market_price}}</view><block s-if="{{datas.pro_kc>=0}}"><view class="price3">{{"库存量："+datas.pro_kc}}</view></block><view class="sale_num">{{"已兑："+datas.sale_num}}</view></view><block s-if="{{datas.labels[0]}}"><view class="biaoq"><block s-for="{{datas.labels}}" s-for-item="item" s-for-index="key" s-key="key"><view class="biaoq_t"><image class="biaoq_p" src="{{$imgurl+'pro_icon.png'}}"></image>{{item+''}}</view></block></view></block><block s-if="{{datas.pro_xz>0}}"><view class="youhuiq"><block s-if="{{datas.pro_xz>0}}"><view class="youhdiv">{{"每人限购"+datas.pro_xz}}</view></block></view></block><view class="spxq"><view class="p_title"><view class="pcon active" style="{{('color:'+baseinfo.base_color2)}}">兑换详情</view></view><view class="xqnr"><rich-text nodes="{{datas.product_txt}}"></rich-text></view></view><view class="pro_footer_bg"></view><view class="pro_footer"><view class="pro_f1 pro_f_home"><navigator open-type="redirectTo" url="/pages/index/index"><image src="{{$imgurl+'i_home.png'}}"></image><text>首页</text></navigator></view><view class="pro_f1 pro_f_star" data-name="{{datas.id}}" data-event-opts="{{[['tap',[['collect',['$event']]]]]}}" bindtap="__e"><block s-if="{{sc==0}}"><image src="{{$imgurl+'i_like.png'}}"></image></block><block s-if="{{sc==1}}"><image src="{{$imgurl+'u_star.png'}}"></image></block><text>收藏</text></view><view data-event-opts="{{[['tap',[['makePhoneCall',['$event']]]]]}}" class="pro_f1 pro_f_tel" bindtap="__e"><image src="{{$imgurl+'i_tel.png'}}"></image><text>客服</text></view><form report-submit="true" data-event-opts="{{[['submit',[['save',['$event']]]]]}}" bindsubmit="__e"><block s-if="{{datas.pro_kc>0||datas.pro_kc==-1}}"><button class="pro_f1 pro_f_buy" style="{{'background:'+(baseinfo.base_color2)+';'+('color:'+(baseinfo.tabbar_bg2)+';')}}" formType="submit">立即兑换</button></block></form><block s-if="{{datas.pro_kc==0}}"><view class="pro_f1 pro_f_buy_t">您来晚了，商品已被兑换完</view></block></view></view></block>