<block s-if="{{$imgurl}}"><view><block s-if="{{list.length>0}}"><block><view class="show_box"><block s-for="{{list}}" s-for-item="item" s-for-index="index" s-key="index"><block><view class="show_item flex_box" data-rid="{{item.room_id}}" data-index="{{index}}" data-event-opts="{{[['tap',[['toShow',['$event']]]]]}}" bindtap="__e"><view class="item_imgBox"><image class="show_img" src="{{item.thumb}}" mode="aspectFill"></image><view class="show_status" style="{{'font-size:'+('20rpx')+';'+('background:'+(item.live_status=='直播中'?'#2a8eeb':item.live_status=='未开始'?'#e33c28':'#bdbdbd')+';')}}">{{item.live_status}}</view></view><view class="show_infoBox"><view class="info_title">{{item.name}}</view><view class="info_smallBox"><view class="info_time">{{"开播时间："+item.start_time}}</view><view class="info_name">{{"直播人："+item.anchor_name}}</view></view><view class="info_nums flex_box"><view class="iconfont icon-huo"></view><view>{{item.visit_num}}</view></view></view></view></block></block></view></block></block><block s-if="{{list.length==0}}"><block><image class="pageno_address" src="{{$imgurl+'no_active.png'}}"></image><view style="text-align:center;color:#666;">还没有相关内容哟~</view></block></block><myfooter vue-id="1" page_signs="{{page_signs}}" baseinfo="{{baseinfo}}"></myfooter></view></block>