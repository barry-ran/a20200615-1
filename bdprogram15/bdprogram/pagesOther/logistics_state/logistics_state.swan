<block s-if="{{$imgurl}}"><view><view class="logistics_headbox hbj"><view class="logistics_pro_img"><image class="product_img" src="{{pro.pro_thumb?pro.pro_thumb:$imgurl+'product_img.png'}}" mode="aspectFill"></image><block s-if="{{pro}}"><block><view class="logistics_pro_num">{{"共"+pro.num+"件"}}</view></block></block></view><view style="width:78%;"><view class="logistics_wlzt">快递状态：<text>{{flag}}</text></view><view class="logistics_wl">承运来源：<text>{{kuaidi}}</text></view><view class="logistics_wl flex_bflow"><view>快递单号：<text>{{kuaidihao}}</text></view><view><text data-event-opts="{{[['tap',[['copy',['$event']]]]]}}" style="color:#E95D3C;margin-left:6rpx;" bindtap="__e">复制</text></view></view></view></view><view class="logistics_detail"><block s-if="{{status==-1}}"><view>暂无物流信息！</view></block><block s-for="{{list}}" s-for-item="item" s-for-index="key" s-key="key"><view class="logistics_detail_single hbj2"><view class="logistics_detail_single_left"><view class="iconfont icon-x-wancheng logistics_yuan2"></view><view class="logistics_yuan"></view><view class="logistics_line"></view></view><block s-if="{{type=='kdniao'}}"><block><view class="logistics_detail_right"><view class="logistics_detail_right_view1">{{item.AcceptStation}}</view><view class="logistics_detail_right_view2">{{item.AcceptTime}}</view></view></block></block><block s-if="{{type=='ali'}}"><block><view class="logistics_detail_right"><view class="logistics_detail_right_view1">{{item.status}}</view><view class="logistics_detail_right_view2">{{item.time}}</view></view></block></block></view></block></view></view></block>