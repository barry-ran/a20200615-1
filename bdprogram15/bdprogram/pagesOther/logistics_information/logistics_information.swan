<block s-if="{{$imgurl}}"><view><view class="page"><block s-for="{{info_list}}" s-for-item="item" s-for-index="index" s-key="index"><block><block s-if="{{index!='not'}}"><block><view class="logistics" data-order_item_id="{{item['order'][0]['order_item_id']}}" data-kuaidi="{{item.express}}" data-kuaidihao="{{item.express_no}}" data-event-opts="{{[['tap',[['toState',['$event']]]]]}}" bindtap="__e"><view class="flex_bflow"><view><text class="iconfont icon-x-baoguo"></text><text class="logistics_flag">{{item.flag}}</text></view><view data-event-opts="{{[['tap',[['',['$event']]]]]}}" class="logistics_kuaidi" catchtap="__e">{{item.express+"："+item.express_no}}</view></view><view class="logistics_img"><block s-for="{{item.order}}" s-for-item="itm" s-for-index="idm" s-key="idm"><block><image src="{{itm.pro_thumb}}" mode="scaleToFill"></image></block></block></view></view></block></block><block s-else><block><view class="logistics"><view class="flex_bflow"><view><text class="iconfont icon-x-baoguo"></text><text class="logistics_flag">未发货</text></view><view class="logistics_kuaidi">暂无</view></view><view class="logistics_img"><block s-for="{{info_list.not}}" s-for-item="its" s-for-index="idx" s-key="idx"><block><image src="{{its.pro_thumb}}" mode="scaleToFill"></image></block></block></view></view></block></block></block></block></view></view></block>