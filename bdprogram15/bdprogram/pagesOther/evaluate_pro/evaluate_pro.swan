<block s-if="{{$imgurl}}"><view class="_div"><block s-for="{{lists_child}}" s-for-item="pro" s-for-index="key" s-key="key"><block><view class="page_box"><view class="assess_header hbj"><image class="assess_header_img" src="{{pro.pro_thumb?pro.pro_thumb:$host+'/diypage/resource/images/diypage/default/default.jpg'}}" mode="aspectFill"></image><block s-if="{{lists.assess==1}}"><view class="assess_header_right hbj"><block s-for="{{assessList}}" s-for-item="item" s-for-index="index" s-key="index"><view class="{{['assess_header_text',assess_duo[key]['id']==item.id?'assess_header_text_on':'']}}" data-id="{{item.id}}" data-dkey="{{key}}" data-event-opts="{{[['tap',[['chooseAssess_duo',['$event']]]]]}}" bindtap="__e"><text class="{{['iconfont',item.icon]}}"></text>{{item.name}}</view></block></view></block><block s-else><view class="assess_header_right hbj" style="color:#b1b1b1;">追评无法重新选择评价等级</view></block></view><view style="position:relative;"><textarea class="assess_textarea" maxlength="200" placeholder="说说你的使用心得，分享给他们吧!" data-dkey="{{key}}" data-event-opts="{{[['input',[['evaluate_duo_bt',['$event']]]]]}}" bindinput="__e"></textarea><view class="nowcount">{{evaluate_duo[key]['nowcount']+"/200"}}</view></view><view class="assess_picturebox" style="border-bottom:2rpx solid #EEE;"><block s-if="{{imgs_duo[key].length>0}}"><block><block s-for="{{imgs_duo[key]}}" s-for-item="item" s-for-index="index" s-key="index"><view class="assess_picture"><image class="assess_picture_img" src="{{item}}" mode="aspectFill"></image><view class="assess_picture_icon iconfont icon-x-guanbi2" data-dkey="{{key}}" data-index="{{index}}" data-event-opts="{{[['tap',[['delimg_duo',['$event']]]]]}}" bindtap="__e"></view></view></block></block></block><view class="assess_picture"><image class="assess_picture_img2" src="{{$imgurl+'camera.png'}}" mode="aspectFill" data-dkey="{{key}}" data-event-opts="{{[['tap',[['chooseimg_duo',['$event']]]]]}}" bindtap="__e"></image></view></view><view class="assess_buy hbj"><block s-if="{{lists.assess==1}}"><view class="{{['assess_buy_left iconfont icon-x-dui2',anonymous_duo[key].anonymous==1?'assess_buy_left_on':'']}}" data-dkey="{{key}}" data-event-opts="{{[['tap',[['chooseAnonymous_duo',['$event']]]]]}}" bindtap="__e"></view></block><block s-if="{{lists.assess==1}}"><view class="assess_buy_center" data-dkey="{{key}}" data-event-opts="{{[['tap',[['chooseAnonymous_duo',['$event']]]]]}}" bindtap="__e">匿名</view></block><view class="assess_buy_right" style="{{(lists.assess==1?'':'text-align:left')}}">你的评价可以帮助其他小伙伴哦！</view></view></view></block></block><view data-event-opts="{{[['tap',[['submit_duo',['$event']]]]]}}" class="assess_submit" bindtap="__e">提交</view></view></block>