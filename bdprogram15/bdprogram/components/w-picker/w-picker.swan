<view class="w-picker"><view class="{{['mask _div',(showPicker)?'show':'']}}" catchtouchmove="true" data-event-opts="{{[['tap',[['maskTap',['$event']]]]]}}" bindtap="__e"></view><view class="{{['w-picker-cnt',(showPicker)?'show':'']}}"><view class="w-picker-hd" catchtouchmove="true"><view data-event-opts="{{[['tap',[['pickerCancel',['$event']]]]]}}" class="w-picker-btn" bindtap="__e">取消</view><view data-event-opts="{{[['tap',[['pickerConfirm',['$event']]]]]}}" class="w-picker-btn" style="{{'color:'+(themeColor)+';'}}" bindtap="__e">确定</view></view><block s-if="{{mode=='date'||mode=='dateTime'||mode=='yearMonth'}}"><view class="w-picker-view"><picker-view indicator-style="height: 40px;" value="{{pickVal}}" data-event-opts="{{[['change',[['bindChange',['$event']]]]]}}" bindchange="__e"><picker-view-column><block s-for="{{data.years}}" s-for-item="item" s-for-index="index" s-key="index"><view class="item">{{item+"年"}}</view></block></picker-view-column><picker-view-column><block s-for="{{data.months}}" s-for-item="item" s-for-index="index" s-key="index"><view class="item">{{item+"月"}}</view></block></picker-view-column><block s-if="{{mode!='yearMonth'}}"><picker-view-column><block s-for="{{data.days}}" s-for-item="item" s-for-index="index" s-key="index"><view class="item">{{item+"日"}}</view></block></picker-view-column></block><block s-if="{{mode=='dateTime'}}"><picker-view-column><block s-for="{{data.hours}}" s-for-item="item" s-for-index="index" s-key="index"><view class="item">{{item+"时"}}</view></block></picker-view-column></block><block s-if="{{mode=='dateTime'}}"><picker-view-column><block s-for="{{data.minutes}}" s-for-item="item" s-for-index="index" s-key="index"><view class="item">{{item+"分"}}</view></block></picker-view-column></block></picker-view></view></block><block s-if="{{mode=='time'}}"><view class="w-picker-view"><picker-view indicator-style="height: 40px;" value="{{pickVal}}" data-event-opts="{{[['change',[['bindChange',['$event']]]]]}}" bindchange="__e"><picker-view-column><block s-for="{{data.hours}}" s-for-item="item" s-for-index="index" s-key="index"><view class="item">{{item+"时"}}</view></block></picker-view-column><picker-view-column><block s-for="{{data.minutes}}" s-for-item="item" s-for-index="index" s-key="index"><view class="item">{{item+"分"}}</view></block></picker-view-column></picker-view></view></block><block s-if="{{mode=='region'}}"><view class="w-picker-view"><picker-view indicator-style="height: 40px;" value="{{pickVal}}" data-event-opts="{{[['change',[['bindChange',['$event']]]]]}}" bindchange="__e"><picker-view-column><block s-for="{{data.provinces}}" s-for-item="item" s-for-index="index" s-key="index"><view class="item">{{item.label}}</view></block></picker-view-column><picker-view-column><block s-for="{{data.citys}}" s-for-item="item" s-for-index="index" s-key="index"><view class="item">{{item.label}}</view></block></picker-view-column><picker-view-column><block s-for="{{data.areas}}" s-for-item="item" s-for-index="index" s-key="index"><view class="item">{{item.label}}</view></block></picker-view-column></picker-view></view></block></view></view>