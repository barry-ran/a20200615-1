<view><block s-if="{{type=='showArt'}}"><navigator url="{{'/pages/'+type+'/'+type+'?id='+item.id}}"><block s-if="{{type=='showArt'}}"><view class="re_box"><image src="{{item.thumb}}" mode="aspectFill"></image><block s-if="{{item.price>0&&item.art_type!=1}}"><view class="listVbg"><block s-if="{{item.art_type==2}}"><image src="{{$imgurl+'play_video.png'}}"></image></block><block s-if="{{item.art_type==3}}"><image src="{{$imgurl+'play_radio.png'}}"></image></block></view></block><view class="flex_box title_box"><view class="title">{{item.title}}</view><view class="box_desc">{{item.desc==null?'暂无简介':item.desc}}</view><text>{{item.creattime}}</text></view><view class="flex_box fl_look"><view class="iconfont icon-c-kan"></view><block s-if="{{item.hits==null}}"><text class="fl_num">0</text></block><block s-if="{{item.hits!=null}}"><text class="fl_num">{{item.hits}}</text></block></view><block s-if="{{item.price>0}}"><text class="listVBtn">付费</text></block></view></block></navigator></block><block s-if="{{type=='showPic'}}"><navigator url="{{'/pages/'+type+'/'+type+'?id='+item.id}}"><block s-if="{{type=='showPic'}}"><view class="re_box"><image src="{{item.thumb}}" mode="aspectFill"></image><view class="pic_info_box"><view class="title">{{item.title}}</view><view class="picture_nums">{{item.pic_num+"张"}}</view></view></view></block></navigator></block></view>