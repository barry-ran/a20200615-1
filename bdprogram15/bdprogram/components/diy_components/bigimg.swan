<view class="_div"><view style="{{'padding:'+(bigimg.style.paddingtop*2+'rpx 0 ')+';'+('background:'+(bigimg.params.styledata==1?'url('+bigimg.params.backgroundimg+')'+' '+bigimg.params.positiony+' '+bigimg.params.positionx+' '+bigimg.params.repeat+' '+bigimg.style.background:bigimg.style.background)+';')+('background-size:'+(bigimg.params.size==1?'cover':bigimg.style.sizew+'%'+bigimg.params.size==1?'':bigimg.style.sizeh+'%')+';')+('margin-top:'+(bigimg.style.mt*2+'rpx')+';')}}"><block s-for="{{bigimg.data}}" s-for-item="item" s-for-index="key" s-key="key"><block><block s-if="{{item.linktype!='server'}}"><view style="{{'padding:'+('0 '+bigimg.style.paddingleft*2+'rpx')+';'+('position:'+('relative')+';')}}" data-linktype="{{item.linktype}}" data-link="{{item.linkurl}}" data-event-opts="{{[['tap',[['redirectto',['$event']]]]]}}" bindtap="__e"><image style="{{'width:'+('100%')+';'+('display:'+('block')+';')+('border-radius:'+(bigimg.style.borderradius*2+'rpx')+';')}}" src="{{item.imgurl}}" mode="widthFix"></image><view style="position:absolute;left:80rpx;top:64rpx;width:78%;"><view style="font-size:28rpx;color:#fff;">{{item.title}}</view><view style="font-size:24rpx;color:#fff;margin-top:10rpx;">{{item.text}}</view></view></view></block><block s-if="{{item.linktype=='server'}}"><button style="background:none;border-radius:0;padding:0;line-height:40rpx;"><view style="{{'padding:'+('0 '+bigimg.style.paddingleft*2+'rpx')+';'}}" data-linktype="{{item.linktype}}" data-link="{{item.linkurl}}" data-event-opts="{{[['tap',[['makephonecall',['$event']]]]]}}" bindtap="__e"><image style="{{'width:'+('100%')+';'+('display:'+('block')+';')+('position:'+('relative')+';')+('border-radius:'+(bigimg.style.borderradius*2+'rpx')+';')}}" src="{{item.imgurl}}" mode="widthFix"></image><view style="position:absolute;left:80rpx;top:64rpx;width:78%;"><view style="font-size:28rpx;color:#fff;text-align:left;">{{item.title}}</view><view style="font-size:24rpx;color:#fff;margin-top:10rpx;text-align:left;">{{item.text}}</view></view></view></button></block></block></block></view></view>