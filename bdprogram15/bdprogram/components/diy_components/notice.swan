<view class="_div"><view class="hbj" style="{{'padding:'+(notice.style.paddingtop*2+'rpx '+notice.style.paddingleft*2+'rpx ')+';'+('background:'+(notice.params.styledata==1?'url('+notice.params.backgroundimg+')'+' '+notice.params.positiony+' '+notice.params.positionx+' '+notice.params.repeat+' '+notice.style.background:notice.style.background)+';')+('background-size:'+(notice.params.size==1?'cover':notice.style.sizew+'%'+' '+notice.params.size==1?'':notice.style.sizeh+'%')+';')+('margin-top:'+(notice.style.mt*2+'rpx')+';')}}"><view class="notice-icon" style="position:relative;"><view class="{{['notice-icon2','iconfont',notice.params.iconurl]}}" style="{{'float:'+('left')+';'+('font-size:'+('36rpx')+';')+('color:'+(notice.style.iconcolor)+';')+('vertical-align:'+('top')+';')+('padding-right:'+('10rpx')+';')}}"></view><block s-if="{{notice.params.navstyle==1}}"><view style="width:2rpx;background:#e2e2e2;height:30rpx;margin-left:8rpx;"></view></block></view><swiper class="swiper_container" vertical="true" autoplay="true" circular="true" interval="{{notice.params.speed*1000}}"><block s-for="{{notice.data}}" s-for-item="item" s-for-index="key" s-key="key"><block><swiper-item><block s-if="{{item.linktype!='server'}}"><view class="swiper_item"><text class="swiper_item_text" style="{{'color:'+(notice.style.color)+';'}}" data-linktype="{{item.linktype}}" data-link="{{item.linkurl}}" data-event-opts="{{[['tap',[['redirectto',['$event']]]]]}}" bindtap="__e">{{item.title}}</text></view></block><block s-if="{{item.linktype=='server'}}"><button style="background:none;padding:0;border-radius:0;text-align:left;"><view class="swiper_item"><text class="swiper_item_text" style="{{'color:'+(notice.style.color)+';'}}" data-linktype="{{item.linktype}}" data-link="{{item.linkurl}}" data-event-opts="{{[['tap',[['makephonecall',['$event']]]]]}}" bindtap="__e">{{item.title}}</text></view></button></block></swiper-item></block></block></swiper></view></view>