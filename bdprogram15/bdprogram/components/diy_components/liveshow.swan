<view class="_div"><view class="show_page_liveshow" style="{{'background:'+(liveshow.params.styledata==1?'url('+liveshow.params.backgroundimg+')'+' '+liveshow.params.positiony+' '+liveshow.params.positionx+' '+liveshow.params.repeat+' '+liveshow.style.background:liveshow.style.background)+';'+('background-size:'+(liveshow.params.size==1?'cover':liveshow.style.sizew+'%'+liveshow.params.size==1?'':liveshow.style.sizeh+'%')+';')+('margin-top:'+(liveshow.style.mt*2+'rpx')+';')+('padding:'+(liveshow.style.paddingtop*2+'rpx '+liveshow.style.paddingleft*2+'rpx')+';')+('box-sizing:'+('border-box')+';')}}"><block s-if="{{liveshow.style.liststyle=='block one'}}"><block><block s-for="{{liveshow.data}}" s-for-item="item" s-for-index="key" s-key="key"><view class="display-inline-block" style="width:100%;" data-rid="{{item.room_id}}" data-event-opts="{{[['tap',[['redirectto',['$event']]]]]}}" bindtap="__e"><view class="ls_imgBox"><image class="show_img" style="{{'width:'+(750-4*liveshow.style.paddingleft+'rpx')+';'}}" src="{{item.thumb}}" mode="widthFix"></image><view class="ls_status" style="{{'background:'+(item.live_status=='直播中'?'#2a8eeb':item.live_status=='未开始'?'#e33c28':'#bdbdbd')+';'}}">{{item.live_status}}</view><view class="ls_vnum"><view class="iconfont icon-huo"></view><view>{{item.visit_num}}</view></view></view><view class="show_info"><view class="show_name">{{item.name}}</view><view class="info_box"><view class="show_time">{{"开播时间："+item.start_time}}</view><view class="show_anchor">{{item.anchor_name}}</view></view></view></view></block></block></block><block s-if="{{liveshow.style.liststyle=='block'}}"><block><block s-for="{{liveshow.data}}" s-for-item="item" s-for-index="key" s-key="key"><view class="block_two" data-rid="{{item.room_id}}" data-event-opts="{{[['tap',[['redirectto',['$event']]]]]}}" bindtap="__e"><view class="ls_imgBox"><image class="show_img" style="width:100%;" src="{{item.thumb}}" mode="widthFix"></image><view class="ls_status" style="{{'font-size:'+('20rpx')+';'+('background:'+(item.live_status=='直播中'?'#2a8eeb':item.live_status=='未开始'?'#e33c28':'#bdbdbd')+';')}}">{{item.live_status}}</view><view class="ls_vnum" style="font-size:20rpx;"><view class="iconfont icon-huo" style="font-size:24rpx;"></view><view>{{item.visit_num}}</view></view></view><view class="show_info"><view class="show_name">{{item.name}}</view><view class="show_time">{{"直播人："+item.anchor_name}}</view><view class="show_time">{{item.start_time}}</view></view></view></block></block></block></view></view>