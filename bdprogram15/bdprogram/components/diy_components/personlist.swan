<block s-if="{{$imgurl}}"><view class="_div"><view class="fwzxBtn" style="{{'padding:'+(personlist.style.pdh*2+'rpx'+' '+personlist.style.pdw*2+'rpx')+';'+('background:'+(personlist.params.styledata==1?'url('+personlist.params.backgroundimg+')'+' '+personlist.params.positiony+' '+personlist.params.positionx+' '+personlist.params.repeat+' '+personlist.style.background:personlist.style.background)+';')+('background-size:'+(personlist.params.size==1?'cover':personlist.style.sizew+'%'+personlist.params.size==1?'':personlist.style.sizeh+'%')+';')+('margin-top:'+(personlist.style.mt*2+'rpx')+';')+('box-sizing:'+('border-box')+';')}}"><block s-for="{{personlist.data}}" s-for-item="item" s-for-index="key" s-key="key"><block><view class="hbj" style="padding:30rpx 0;border-bottom:2rpx solid #eee;" data-linktype="{{item.linktype}}" data-link="{{'/pagesCards/card_info/card_info?id='+item.id}}" data-event-opts="{{[['tap',[['redirectto',['$event']]]]]}}" bindtap="__e"><image style="width:108rpx;height:108rpx;border-radius:50%;" src="{{item.pic}}"></image><view style="margin-left:20rpx;flex:1;"><view style="font-size:28rpx;color:#434343;">{{item.realname}}</view><view class="hbj"><view style="font-size:24rpx;color:#838383;">{{item.job}}</view><view style="height:24rpx;background:#eee;width:2rpx;margin:0 16rpx;"></view><view style="font-size:24rpx;color:#838383;">{{item.age==0?"保密":item.age+"岁"}}</view><view style="height:24rpx;background:#eee;width:2rpx;margin:0 16rpx;"></view><view style="font-size:24rpx;color:#838383;">{{item.province+item.city}}</view></view><view class="hbj"><block s-if="{{item.score>0}}"><image style="width:28rpx;margin-right:12rpx;" src="{{$imgurl+'personlist_star.png'}}" mode="widthFix"></image></block><block s-if="{{item.score>1}}"><image style="width:28rpx;margin-right:12rpx;" src="{{$imgurl+'personlist_star.png'}}" mode="widthFix"></image></block><block s-if="{{item.score>2}}"><image style="width:28rpx;margin-right:12rpx;" src="{{$imgurl+'personlist_star.png'}}" mode="widthFix"></image></block><block s-if="{{item.score>3}}"><image style="width:28rpx;margin-right:12rpx;" src="{{$imgurl+'personlist_star.png'}}" mode="widthFix"></image></block><block s-if="{{item.score>4}}"><image style="width:28rpx;margin-right:12rpx;" src="{{$imgurl+'personlist_star.png'}}" mode="widthFix"></image></block><view style="font-size:24rpx;color:#838383;">{{item.score+"分"}}</view></view></view><view><view><block s-if="{{item.contract==1}}"><view style="display:inline-block;width:100rpx;height:44rpx;line-height:44rpx;text-align:center;background-color:#7ad4d2;color:#fff;font-size:28rpx;border-radius:8rpx;margin-right:10rpx;">签约</view></block><block s-if="{{item.auth==1}}"><view style="display:inline-block;width:100rpx;height:44rpx;line-height:44rpx;text-align:center;background-color:#ff6f6e;color:#fff;font-size:28rpx;border-radius:8rpx;">认证</view></block></view><view style="font-size:24rpx;color:#838383;text-align:right;margin-top:10rpx;">{{item.visit+"次访问"}}</view></view></view></block></block></view></view></block>