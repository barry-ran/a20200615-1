<block s-if="{{$imgurl}}"><view class="_div"><view style="{{'background:'+(wenlist.params.styledata==1?'url('+wenlist.params.backgroundimg+')'+' '+wenlist.params.positiony+' '+wenlist.params.positionx+' '+wenlist.params.repeat+' '+wenlist.style.background:wenlist.style.background)+';'+('background-size:'+(wenlist.params.size==1?'cover':wenlist.style.sizew+'%'+wenlist.params.size==1?'':wenlist.style.sizeh+'%')+';')+('margin-top:'+(wenlist.style.mt*2+'rpx')+';')+('padding:'+('0 '+wenlist.params.navstyle==1?wenlist.style.paddingleft*2:''+'rpx')+';')+('overflow-x:'+('hidden')+';')}}"><block s-for="{{wenlist.data}}" s-for-item="item" s-for-index="key" s-key="key"><block><view class="hbj" style="{{'padding:'+(wenlist.style.paddingtop*2+'rpx 0')+';'+('position:'+('relative')+';')+('background-color:'+('#fff')+';')+('border-bottom:'+('2rpx solid #eee')+';')}}" data-linktype="page" data-link="{{item.linkurl}}" data-event-opts="{{[['tap',[['redirectto',['$event']]]]]}}" bindtap="__e"><block s-if="{{item.fid==1}}"><view class="up1"><text>供</text></view></block><block s-if="{{item.fid==2}}"><view class="up2"><text>求</text></view></block><block s-if="{{item.video!=''}}"><view style="{{'position:'+('absolute')+';'+('right:'+(wenlist.style.paddingleft*2+'rpx')+';')+('bottom:'+(wenlist.style.paddingtop*2+'rpx')+';')+('font-size:'+('30rpx')+';')+('color:'+('#FF4A47')+';')}}"></view></block><image style="{{'width:'+('168rpx')+';'+('margin-left:'+('20rpx')+';')+('height:'+(wenlist.style.pich==2?'168':''+wenlist.style.pich==3?'84':''+wenlist.style.pich==4||wenlist.style.pich==1?'126rpx':''+'rpx')+';')+('border-radius:'+(wenlist.style.radius+'%')+';')}}" src="{{item.thumb}}" mode="{{wenlist.style.pich==1?'widthFix':'aspectFill'}}"></image><view style="{{'margin-left:'+('30rpx')+';'+('width:'+(750-wenlist.style.paddingleft*4-30-168+'rpx')+';')}}"><view style="{{'font-size:'+('28rpx')+';'+('color:'+(wenlist.style.color)+';')+('height:'+('40rpx')+';')+('overflow:'+('hidden')+';')+('text-overflow:'+('ellipsis')+';')+('white-space:'+('nowrap')+';')}}">{{item.title}}</view><view style="color:#939393;font-size:24rpx;line-height:32rpx;height:70rpx;overflow:hidden;text-overflow:ellipsis;"><text>{{item.content}}</text></view><view class="hbj" style="margin-top:10rpx;"><block s-if="{{wenlist.params.show2==1}}"><block><view style="font-size:24rpx;color:#434343;"><view class="iconfont iconfont1 icon-x-jilu"></view>{{"访问量: "+(item.hits!=null?item.hits:0)+''}}</view><view style="background:#eee;margin:0 6px;width:2rpx;height:24rpx;"></view></block></block><block s-if="{{wenlist.params.show3==1}}"><block><view style="font-size:24rpx;color:#434343;"><view class="iconfont iconfont1 icon-x-pingjia2"></view>{{"评论: "+item.comment+''}}</view></block></block><view data-event-opts="{{[['tap',[['',['$event']]]]]}}" catchtap="__e"><view class="iconfont icon-x-dianhua10 supply_phone" style="{{'color:'+(item.fid==1?'#ff6f6e':'#7ad4d2')+';'}}" data-phone="{{item.telphone}}" data-event-opts="{{[['tap',[['makephone',['$event']]]]]}}" bindtap="__e"></view></view></view></view></view></block></block></view></view></block>