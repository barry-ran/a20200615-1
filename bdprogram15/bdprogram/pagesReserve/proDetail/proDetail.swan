<block s-if="{{$imgurl}}"><view><block s-if="{{needAuth}}"><auth vue-id="1" needAuth="{{needAuth}}" data-event-opts="{{[['^closeAuth',[['closeAuth']]],['^cell',[['cell']]]]}}" bind:closeAuth="__e" bind:cell="__e"></auth></block><block s-if="{{needBind}}"><bind-phone vue-id="2" needBind="{{needBind}}" data-event-opts="{{[['^closeBind',[['closeBind']]]]}}" bind:closeBind="__e"></bind-phone></block><view class="pro_head"><view class="wrap" style="{{('height:'+system_w+'px;max-height:'+system_w+'px')}}"><swiper class="slide" style="height:100%;" indicator-active-color="{{baseinfo.base_color2}}" interval="3000" duration="1000" autoplay="{{autoplay}}" indicator-color="rgba(0, 0, 0, .3)" data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" bindchange="__e"><block s-for="{{picList}}" s-for-item="item" s-for-index="index" s-key="index"><block><swiper-item><image class="slide-image" src="{{item}}" width="100%" mode="widthFix" data-event-opts="{{[['load',[['swiperLoad',['$event']]]]]}}" bindload="__e"></image><block s-if="{{!isplay&&pic_video&&index==0}}"><view><image class="play-image" src="{{$imgurl+'play_audio.png'}}" mode="aspectFill" data-event-opts="{{[['tap',[['playvideo',['$event']]]]]}}" bindtap="__e"></image></view></block><block s-if="{{isplay}}"><view style="height:100%;"><image class="play-image1" src="{{$imgurl+'c.png'}}" mode="widthFix" data-event-opts="{{[['tap',[['endvideo',['$event']]]]]}}" bindtap="__e"></image><video style="margin:0;height:100%;width:100%;overflow:hidden;" src="{{pic_video}}" objectFit="contain" autoplay="true"></video></view></block></swiper-item></block></block></swiper><block s-if="{{!isplay}}"><view class="dots hbj" style="{{'width:'+((dlength-1)*14+dlength*14+'rpx')+';'}}"><block s-for="{{picList}}" s-for-item="item" s-for-index="index" s-key="index"><block><view class="{{['dot',index==currentSwiper?'active':'']}}" style="margin:auto;"></view></block></block></view></block></view></view><view class="mt-30 hbj pd-0-35"><view class="price" style="{{('color:'+baseinfo.base_color2)}}"><text>￥</text>{{datas.price}}</view><view class="original_price">{{datas.market_price?'￥'+datas.market_price:''}}</view><view class="flex1"><block s-if="{{datas.discount_price>0}}"><view style="{{('margin-left:10rpx;color:'+baseinfo.base_color2)}}">会员价:<text style="font-size:24rpx;">￥</text>{{datas.discount_price}}</view></block></view><block s-if="{{baseinfo.share_open==1}}"><block><view data-event-opts="{{[['tap',[['open_share',['$event']]]]]}}" bindtap="__e"><image class="share_img" src="{{$imgurl+'btn_share.png'}}" mode="widthFix"></image></view></block></block></view><view class="goods_title pd-0-35 mt-20">{{datas.title}}</view><view class="goods_title goods_desc pd-0-35 mt-20">{{datas.desc}}</view><view class="gap"></view><block s-if="{{datas['labels'].length>0}}"><block><view class="choosebox2 hbj pd-0-35"><block s-for="{{datas.labels}}" s-for-item="item" s-for-index="__i0__" s-key="id"><block><block s-if="{{item[0]!=''&&item[1]!=''}}"><view class="choose_card"><image class="hook" src="{{$imgurl+'hook.jpg'}}" mode="widthFix"></image>{{''+item[0]+"："+item[1]+''}}</view></block></block></block></view><view class="gap"></view></block></block><block s-if="{{tableis==0}}"><block><view class="choose_type hbj pd-0-35"><view class="choose_type_left" style="{{('background:'+baseinfo.base_color2)}}"></view><view class="choose_type_right">以下几个价格类型可选择</view></view><view class="gap"></view><block s-if="{{datas.more_type}}"><view><block s-for="{{datas.more_type}}" s-for-item="item" s-for-index="index" s-key="index"><block><view class="appoint_single hbj"><view class="appoint_single_left"><view class="specificate">{{item[0]}}</view><view class="appoint_title">{{item[3]}}</view><view class="appoint_stock">{{"库存数量："+item[2]}}</view></view><view class="flex1"></view><view class="zbj"><view class="appoint_price" style="{{('color:'+baseinfo.base_color2)}}"><text>￥</text>{{item[1]}}</view><block s-if="{{item[5]>0}}"><view style="{{('font-size:24rpx;font-weight:100;color:'+baseinfo.base_color2)}}">{{"会员价:￥"+item[5]}}</view></block><block s-if="{{datas.is_sale==1}}"><view class="appoint_card" style="{{('background:'+baseinfo.base_color2)}}" data-id="{{datas.id}}" data-testprice="{{item[1]}}" data-testkey="{{index}}" data-event-opts="{{[['tap',[['toyuyueDG',['$event']]]]]}}" bindtap="__e">{{"立即"+datas.buy_type}}</view></block></view></view><view class="gap"></view></block></block></view></block></block></block><view class="p_title"><view class="{{['pcon',nowcon=='con'?'active':'']}}" style="{{(nowcon=='con'?'color:'+baseinfo.base_color2:'')}}" data-id="con" data-event-opts="{{[['tap',[['tabChange',['$event']]]]]}}" bindtap="__e">商品详情<view style="{{('background:'+nowcon=='con'?baseinfo.base_color2:'')}}" class="_i"></view></view><view style="flex:1;"></view><view class="{{['pcon',nowcon=='comment'?'active':'']}}" style="{{(nowcon=='comment'?'color:'+baseinfo.base_color2:'')}}" data-id="comment" data-event-opts="{{[['tap',[['goevaluate',['$event']]]]]}}" bindtap="__e">客户评论<view style="{{('background:'+nowcon=='comment'?baseinfo.base_color2:'')}}" class="_i"></view></view></view><view class="pd-0-35" style="margin:20rpx 0;"><rich-text nodes="{{product_txt}}"></rich-text></view><block s-if="{{tableis==0}}"><block><view style="height:100rpx;"></view><view class="buybox hbj"><navigator class="shopcart zbj" open-type="redirect" url="/pages/index/index"><image class="home_icon" src="{{$imgurl+'i_home.png'}}"></image><view class="home">首页</view></navigator><view class="shopcart zbj" data-name="{{datas.id}}" data-event-opts="{{[['tap',[['collect',['$event']]]]]}}" bindtap="__e"><block s-if="{{sc==0}}"><image class="home_icon" src="{{$imgurl+'i_like.png'}}"></image></block><block s-if="{{sc==1}}"><image class="home_icon" src="{{$imgurl+'u_star.png'}}"></image></block><view class="home">收藏</view></view><view data-event-opts="{{[['tap',[['makePhoneCall',['$event']]]]]}}" class="shopcart zbj" bindtap="__e"><image class="home_icon" src="{{$imgurl+'i_tel.png'}}"></image><view class="home">客服</view></view><view data-event-opts="{{[['tap',[['show',['$event']]]]]}}" class="join_shopcart" style="{{'background:'+(baseinfo.tabbar_bg3)+';'+('color:'+(baseinfo.tabbar_bg1)+';')}}" bindtap="__e">{{'￥'+datas.price+''}}</view><block s-if="{{datas.is_sale==1}}"><view class="buy" style="{{('flex:1;display:block;background:'+baseinfo.base_color2+';color:'+baseinfo.tabbar_bg2)}}" open-type="redirect" data-id="{{datas.id}}" data-event-opts="{{[['tap',[['toyuyue',['$event']]]]]}}" bindtap="__e">{{"立即"+datas.buy_type}}</view></block><block s-if="{{datas.is_sale==0}}"><view class="buy" style="background:#f6f6f6;">即将开始</view></block></view></block></block><block s-if="{{tableis==1}}"><block><block s-if="{{bottom_edit==0}}"><view style="height:90rpx;"></view></block><block s-if="{{bottom_edit==1}}"><view style="height:180rpx;"></view></block><block s-if="{{bottom_edit==1}}"><view class="appoint_edit hbj" style="{{'background:'+(baseinfo.tabbar_bg3)+';'+('color:'+(baseinfo.tabbar_bg1)+';')}}"><view class="appoint_edit_left flex1">已选<text>{{appoint_num}}</text>个，应付<text>{{appoint_price}}</text>元</view><navigator class="appoint_edit_right" url="{{'/pagesReserve/appointPage/appointPage?appoint_date='+appoint_date+'&NowSelectStr='+NowSelectStr+'&tableid='+datas.tableid+'&id='+datas.id+'&startdate='+datas.start_date}}">编辑</navigator></view></block><block s-if="{{bottom_edit==0}}"><view class="appoint_bottom2" style="{{'background:'+(baseinfo.base_color2)+';'+('color:'+(baseinfo.tabbar_bg2)+';')}}" data-tableid="{{datas.tableid}}" data-id="{{datas.id}}" data-afterdays="{{datas.afterdays}}" data-startdate="{{datas.start_date}}" data-event-opts="{{[['tap',[['xuanzuo',['$event']]]]]}}" bindtap="__e">{{datas.foottitle?datas.foottitle:"立即选座"}}</view></block><block s-if="{{bottom_edit==1}}"><view data-event-opts="{{[['tap',[['topay',['$event']]]]]}}" class="appoint_bottom2" style="{{'background:'+(baseinfo.base_color2)+';'+('color:'+(baseinfo.tabbar_bg2)+';')}}" bindtap="__e">立即支付</view></block></block></block><block s-if="{{share==1}}"><view data-event-opts="{{[['tap',[['share_close',['$event']]]]]}}" class="mask" bindtap="__e"></view></block><block s-if="{{share==1}}"><view class="share_ceng" style="{{'z-index:'+(shareimg==1?'800':1000)+';'}}"><view class="share_con flex-row"><view class="share_con_box"><button class="flex-row" open-type="share" data-event-opts="{{[['tap',[['onShareAppMessage',['$event']]]]]}}" bindtap="__e"><view class="iconfont icon-x-fenxiang5" style="font-size:40rpx;color:#56bb3a;"></view><view style="margin-left:10rpx;color:#333;">分享给好友</view></button></view><view class="share_con_box"><view data-event-opts="{{[['tap',[['getShareImg',['$event']]]]]}}" class="flex-row" style="justify-content:center;" bindtap="__e"><view class="iconfont icon-x-tupian" style="font-size:40rpx;color:#e47b2f;"></view><view style="margin-left:10rpx;">生成分享海报</view></view></view></view></view></block><block s-if="{{shareimg==1}}"><view class="fx_box" style="{{'width:'+(img_w+20+'px')+';'+('height:'+(img_h+25+'px')+';')+('left:'+((system_w-img_w-10)/2+'px')+';')}}"><view class="fx_close"><view data-event-opts="{{[['tap',[['closeShare',['$event']]]]]}}" class="fx_X" bindtap="__e">x</view><view class="fx_content"><image class="haibao_img _img" style="{{'width:'+(img_w+'px')+';'+('height:'+(img_h+'px')+';')}}" src="{{shareimg_url}}"></image></view><button class="haibao_btn" type="primary" data-event-opts="{{[['tap',[['saveImg',['$event']]]]]}}" bindtap="__e">保存图片并分享</button></view></view></block></view></block>