<block s-if="{{$imgurl}}"><view class="_div"><view class="seathead hbj"><view class="seathead_left">{{table.name}}</view><view class="flex1"></view><picker mode="date" value="{{date}}" start="{{start}}" end="2100-01-01" data-event-opts="{{[['change',[['bindDateChange',['$event']]]]]}}" bindchange="__e"><view class="hbj"><view class="seathead_date">{{date}}</view><view class="seathead_right">切换<text class="iconfont icon-x-qiehuan"></text></view></view></picker></view><view class="seat_state"><view class="seat_state_view"><image src="{{$imgurl+'seat2.png'}}" mode="aspectFill"></image>可选</view><view class="seat_state_view"><image src="{{$imgurl+'seat3.png'}}" mode="aspectFill"></image>已被选</view><view class="seat_state_view"><image src="{{$imgurl+'seat4.png'}}" mode="aspectFill"></image>不可选</view></view><scroll-view class="seat_content" scroll-x><view class="seat_content_column"><view class="seat_label_first mb-30"></view><block s-for="{{table.rowstr}}" s-for-item="item" s-for-index="key" s-key="key"><view class="seat_label mb-30">{{item}}</view></block></view><block s-for="{{table.columnstr}}" s-for-item="item" s-for-index="index2" s-key="index2"><view class="seat_content_column"><view class="seat_label mb-30">{{item}}</view><block s-for="{{table.rowstr}}" s-for-item="item1" s-for-index="index1" s-key="index1"><view class="seat_content_imgbox mb-30"><image data-num="{{index1+1+'a'+(index2+1)}}" src="{{$imgurl+'seat2.png'}}" mode="aspectFill" data-event-opts="{{[['tap',[['selectThis',['$event']]]]]}}" bindtap="__e"></image><block s-for="{{table.selectstr}}" s-for-item="disableNumItem" s-for-index="index3" s-key="index3"><block><block s-if="{{disableNumItem==index1+1+'a'+(index2+1)}}"><block><image src="{{$imgurl+'seat4.png'}}" mode="aspectFill"></image></block></block></block></block><block s-for="{{NowSelect}}" s-for-item="NowSelectItem" s-for-index="index4" s-key="index4"><block><block s-if="{{NowSelectItem==index1+1+'a'+(index2+1)}}"><block><image data-num="{{index1+1+'a'+(index2+1)}}" src="{{$imgurl+'seat1.png'}}" mode="aspectFill" data-event-opts="{{[['tap',[['removeThis',['$event']]]]]}}" bindtap="__e"></image></block></block></block></block><block s-for="{{otherSelect}}" s-for-item="otherSelectItem" s-for-index="index5" s-key="index5"><block><block s-if="{{otherSelectItem==index1+1+'a'+(index2+1)}}"><block><image src="{{$imgurl+'seat3.png'}}" mode="aspectFill"></image></block></block></block></block></view></block></view></block></scroll-view><view style="height:224rpx;"></view><scroll-view class="seat_selectedbox" scroll-x><view class="seat_select" style="color:#32B16C;">已选：</view><block s-for="{{selected}}" s-for-item="item" s-for-index="kk" s-key="kk"><block><view class="seat_select">{{item.row+"，"+item.column}}</view></block></block></scroll-view><navigator class="seat_true" open-type="redirect" url="{{'/pagesReserve/proDetail/proDetail?id='+id+'&NowSelectStr='+NowSelectStr+'&appoint_date='+date_+'&bottom_edit=1'}}">{{table.name}}</navigator></view></block>