<block s-if="{{$imgurl}}"><view class="_div"><view class="ddym"><view class="wddd">预约订单</view><view class="gztb"><view class="dizd" data-id="9" data-event-opts="{{[['tap',[['chonxhq',['$event']]]]]}}" bindtap="__e"><view><image class="dx1" src="{{$imgurl+'ord_all.png'}}"></image></view>全部</view><view class="dizd" data-id="0" data-event-opts="{{[['tap',[['chonxhq',['$event']]]]]}}" bindtap="__e"><view><image class="dx2" src="{{$imgurl+'ord_wait.png'}}"></image></view>未付款</view><view class="dizd" data-id="3" data-event-opts="{{[['tap',[['chonxhq',['$event']]]]]}}" bindtap="__e"><view><image class="dx2" src="{{$imgurl+'ord_dqr.png'}}"></image></view>待确认</view><view class="dizd" data-id="1" data-event-opts="{{[['tap',[['chonxhq',['$event']]]]]}}" bindtap="__e"><view><image class="dx3" src="{{$imgurl+'ord_pay.png'}}"></image></view>待消费</view><view class="dizd" data-id="2" data-event-opts="{{[['tap',[['chonxhq',['$event']]]]]}}" bindtap="__e"><view><image class="dx4" src="{{$imgurl+'ord_over.png'}}"></image></view>已完成</view></view></view><block s-if="{{type==9}}"><view class="zuijdd">{{'最近订单 '+allnum+''}}</view></block><block s-if="{{type==0}}"><view class="zuijdd">待支付订单</view></block><block s-if="{{type==3}}"><view class="zuijdd">待确认订单</view></block><block s-if="{{type==1}}"><view class="zuijdd">待消费订单</view></block><block s-if="{{type==2}}"><view class="zuijdd">已完成订单</view></block><view><block s-for="{{orderinfo}}" s-for-item="item" s-for-index="index" s-key="index"><view class="qxiq"><block s-if="{{item.is_more==1}}"><view style="position:relative;"><navigator url="{{'/pagesReserve/orderDetail/orderDetail?id='+item.order_id+'&tsid='+item.tsid}}"><view class="thumbdx"><image src="{{item.thumb}}"></image></view><view class="jbxx"><view class="bbts">{{''+item.product+''}}</view><block s-if="{{item.is_more==0}}"><view class="zts">{{'数量：'+item.num+''}}</view></block><view class="zts">{{'总价：￥'+item.true_price+''}}</view><block s-if="{{item.flag==-2}}"><view class="piaos" style="color:red;">无效</view></block><block s-if="{{item.flag==-1}}"><view class="piaos" style="color:red;">已关闭</view></block><block s-if="{{item.flag==0}}"><view class="piaos" style="color:#FF4544;">待付款</view></block><block s-if="{{item.flag==1}}"><view class="piaos">待消费</view></block><block s-if="{{item.flag==2}}"><view class="piaos" style="color:green;">已消费</view></block><block s-if="{{item.flag==3}}"><view class="piaos" style="color:red;">待确认</view></block><block s-if="{{item.flag==5}}"><view class="piaos" style="color:red;">已取消</view></block><block s-if="{{item.flag==6}}"><view class="piaos" style="color:red;">退款中</view></block><block s-if="{{item.flag==8}}"><view class="piaos" style="color:red;">已退款</view></block><block s-if="{{item.flag==9}}"><view class="piaos" style="color:red;">拒绝退款</view></block><view data-event-opts="{{[['tap',[['',['$event']]]]]}}" catchtap="__e"><block s-if="{{item.flag==0&&item.tsid==0}}"><view class="piaos1" data-pid="{{item.pid}}" data-orderid="{{item.order_id}}" data-event-opts="{{[['tap',[['toyuyue',['$event']]]]]}}" bindtap="__e">立即付款</view></block><block s-if="{{item.flag==0&&item.tsid>0}}"><view class="piaos1" data-pid="{{item.pid}}" data-orderid="{{item.order_id}}" data-str="{{item.select_str}}" data-date="{{item.appoint_date}}" data-event-opts="{{[['tap',[['toyuyue2',['$event']]]]]}}" bindtap="__e">立即付款</view></block></view></view></navigator><form report-submit="true" data-pid="{{item.pid}}" data-orderid="{{item.order_id}}" data-event-opts="{{[['submit',[['refund_lv',['$event']],['',['$event']]]]]}}" catchsubmit="__e"><block s-if="{{item.flag==1||item.flag==3}}"><button class="piaos1" style="padding:0 20rpx;bottom:10rpx;height:50rpx !important;line-height:50rpx;" formType="submit">申请退款</button></block></form><block s-if="{{item.flag==2&&item.assess==1}}"><block><view class="piaos1" data-order="{{item.order_id}}" data-type="reserve" data-event-opts="{{[['tap',[['goevaluate',['$event']]]]]}}" bindtap="__e">我要评价</view></block></block><block s-if="{{item.flag==2&&item.assess==2}}"><block><view class="piaos1" style="color:green;border-color:green;">已评价</view></block></block></view></block><block s-if="{{item.flag==1&&item.emp_id}}"><view style="margin-top:20rpx;font-size:26rpx;">{{'上门服务：'+item.emp_name+"-"+item.emp_mobile+''}}<text style="color:blue;margin-left:20rpx;" data-str="{{item.emp_name+item.emp_mobile}}" data-event-opts="{{[['tap',[['copy',['$event']]]]]}}" bindtap="__e">复制</text></view></block></view></block><block s-if="{{orderinfo_length==0}}"><block><image class="pageno_address" src="{{$imgurl+'no_search.png'}}"></image><view style="text-align:center;color:#666;">还没有相关订单哟~</view></block></block></view><block s-if="{{morePro}}"><view data-event-opts="{{[['tap',[['showMore',['$event']]]]]}}" class="more3" style="{{'color:'+(baseinfo.base_color2)+';'}}" bindtap="__e">加载更多...</view></block><myfooter vue-id="1" page_signs="{{page_signs}}" baseinfo="{{baseinfo}}"></myfooter></view></block>