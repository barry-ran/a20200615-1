<block s-if="{{$imgurl&&baseinfo.can_fs==1}}"><view><block s-if="{{needAuth}}"><auth vue-id="1" needAuth="{{needAuth}}" data-event-opts="{{[['^closeAuth',[['closeAuth']]],['^cell',[['cell']]]]}}" bind:closeAuth="__e" bind:cell="__e"></auth></block><block s-if="{{needBind}}"><bind-phone vue-id="2" needBind="{{needBind}}" data-event-opts="{{[['^closeBind',[['closeBind']]]]}}" bind:closeBind="__e"></bind-phone></block><view style="height:15rpx;background:#F8F8F8;"></view><block s-if="{{rid==0}}"><view class="relase_picker hbj"><picker style="width:100%;" value="{{index}}" range="{{funcTitleArr}}" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" bindchange="__e"><view style="font-size:30rpx;color:#434343;">{{'选择发布类型：'+funcTitleArr[index]+''}}</view></picker><image class="relase_yjt" src="{{$imgurl+'relase_yjt.png'}}" mode="aspectFill"></image></view></block><block s-if="{{rid==0}}"><view style="height:15rpx;background:#F8F8F8;"></view></block><view class="title"><text>标题：</text><input type="text" name="title" data-event-opts="{{[['input',[['getInputTitle',['$event']]]]]}}" value="{{title!=''&&title!='undefined'?title:''}}" bindinput="__e"/></view><block s-if="{{rid==0}}"><view style="height:15rpx;background:#F8F8F8;"></view></block><view class="relase_title" style="display:flex;"><view>发布信息:</view><view class="flex1"></view><view style="color:#a8a8a8;">{{cons_len+"/2000"}}</view></view><textarea class="relase_textarea" placeholder="说你想说的" maxlength="2000" placeholder-class="placeholderstyle" data-event-opts="{{[['input',[['getcons',['$event']]]]]}}" value="{{cons}}" bindinput="__e"></textarea><block s-if="{{isiPhone==0}}"><view class="relase_imgbox"><block s-for="{{release_img}}" s-for-item="item" s-for-index="ke" s-key="ke"><view class="relase_img_single"><image class="relase_img" src="{{item}}" mode="aspectFill"></image><view class="relase_close_box" data-index="{{ke}}" data-event-opts="{{[['tap',[['delimg',['$event']]]]]}}" bindtap="__e"><view class="relase_close"><image src="{{$imgurl+'del.png'}}" mode="aspectFill"></image></view></view></view></block><view data-event-opts="{{[['tap',[['chooseImg',['$event']]]]]}}" class="relase_img_single" style="overflow:hidden;left:0;top:20rpx;" bindtap="__e"><image class="relase_img_single_img" src="{{$imgurl+'camera.png'}}" mode="aspectFill"></image></view></view></block><block s-if="{{isiPhone==1}}"><view class="relase_imgbox"><block s-for="{{release_img}}" s-for-item="item" s-for-index="key" s-key="key"><view class="relase_img_single"><image class="relase_img" src="{{item}}" mode="aspectFill"></image><view class="relase_close_box" data-index="{{key}}" data-event-opts="{{[['tap',[['delimg',['$event']]]]]}}" bindtap="__e"><view class="relase_close"><image src="{{$imgurl+'del.png'}}" mode="aspectFill"></image></view></view></view></block><view data-event-opts="{{[['tap',[['chooseImg',['$event']]]]]}}" class="relase_img_single" style="overflow:hidden;left:0;top:40rpx;" bindtap="__e"><image class="relase_img_single_img" src="{{$imgurl+'camera.png'}}" mode="aspectFill"></image></view></view></block><view style="height:15rpx;background:#F8F8F8;"></view><view data-event-opts="{{[['tap',[['getlocation',['$event']]]]]}}" class="release_position hbj" bindtap="__e"><view class="release_position_icon iconfont icon-local"></view><view class="release_position_text flex1">所在位置<text>{{address}}</text></view><image class="release_position_img" src="{{$imgurl+'relase_yjt.png'}}" mode="aspectFill"></image></view><view class="release_position hbj"><view class="release_position_icon iconfont icon-dianhua"></view><view class="release_position_text">电话号码</view><input class="release_position_input flex1" placeholder="填写手机号码" placeholder-class="placeholderstyle" type="number" data-event-opts="{{[['input',[['getInputTel',['$event']]]]]}}" value="{{telphone!=''&&telphone!='undefined'?telphone:''}}" bindinput="__e"/></view><view style="height:15rpx;background:#F8F8F8;"></view><block s-if="{{release_money!=0&&rid==0}}"><block><view class="day_money hbj"><view class="day_money_left">发布费用</view><view class="day_money_right"><text>￥</text>{{release_money}}</view></view></block></block><block s-if="{{release_money!=0&&rid==0}}"><view class="relase_day hbj"><view class="all_money_left">账户余额</view><view class="all_money_right"><text>￥</text>{{userMoney?userMoney:0}}</view></view></block><form report-submit="true" data-event-opts="{{[['submit',[['releasePay',['$event']]]]]}}" bindsubmit="__e"><button class="relase_btn" formType="submit">发布</button></form></view></block>