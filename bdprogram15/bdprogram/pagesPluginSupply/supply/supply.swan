<block s-if="{{$imgurl&&baseinfo.can_fs==1}}"><view><block s-if="{{needAuth}}"><auth vue-id="1" needAuth="{{needAuth}}" data-event-opts="{{[['^closeAuth',[['closeAuth']]],['^cell',[['cell']]]]}}" bind:closeAuth="__e" bind:cell="__e"></auth></block><block s-if="{{needBind}}"><bind-phone vue-id="2" needBind="{{needBind}}" data-event-opts="{{[['^closeBind',[['closeBind']]]]}}" bind:closeBind="__e"></bind-phone></block><view class="nav"><view class="{{['nav_con',type==1?'active':'']}}" data-type="1" data-event-opts="{{[['tap',[['changeType',['$event']]]]]}}" bindtap="__e">供应</view><view class="{{['nav_con',type==2?'active':'']}}" data-type="2" data-event-opts="{{[['tap',[['changeType',['$event']]]]]}}" bindtap="__e">求购</view></view><block s-if="{{supplyAll.length>0}}"><block><block s-for="{{supplyAll}}" s-for-item="item" s-for-index="idx" s-key="idx"><block><view class="dynamic_single" data-rid="{{item.id}}" data-event-opts="{{[['tap',[['goContent',['$event']]]]]}}" bindtap="__e"><view class="hbj"><image class="dynamic_tx" src="{{item.avatar}}" mode="aspectFill"></image><view class="dynamic_view1_center"><view class="dynamic_name"><text class="dynamic_name_text1">{{item.nickname}}</text><text class="dynamic_name_text2">{{item.createtime}}</text></view><view class="forum_cardbox hbj"><block s-if="{{item.stick==1}}"><view class="forum_card_single" style="background:#2F74FD;">置顶</view></block><block s-if="{{item.hot==1}}"><view class="forum_card_single" style="background:#E12735;">推荐</view></block></view></view><view class="flex1"></view><view data-event-opts="{{[['tap',[['',['$event']]]]]}}" catchtap="__e"><block s-if="{{item.telphone!=''&&item.telphone!='undefined'}}"><image class="forum_phone" src="{{$imgurl+'phone.png'}}" data-tel="{{item.telphone}}" mode="aspectFill" data-event-opts="{{[['tap',[['makephone',['$event']]]]]}}" bindtap="__e"></image></block></view></view><view class="dynamic_title">{{item.title}}</view><view class="dynamic_content" style="overflow:hidden;text-overflow:ellipsis;display:-webkit-box;-webkit-line-clamp:4;-webkit-box-orient:vertical;">{{item.content}}</view><block s-if="{{item.address!=''}}"><view class="forum_address hbj"><image class="forum_address_left" src="{{$imgurl+'position.png'}}" mode="aspectFill"></image><view class="forum_address_right">{{item.address}}</view></view></block><view class="dynamic_imgbox"><block s-for="{{item.img}}" s-for-item="itemimg" s-for-index="k" s-key="k"><block><image class="dynamic_img" src="{{itemimg}}" mode="aspectFill"></image></block></block></view><view data-event-opts="{{[['tap',[['',['$event']]]]]}}" class="dynamic_single_bot hbj" catchtap="__e"><view class="dynamic_single_bot_view"><text class="dynamic_single_bot_text1 iconfont icon-c-kan"></text><text class="dynamic_single_bot_text2">{{item.hits}}</text></view><view class="dynamic_single_bot_view" data-rid="{{item.id}}" data-index="{{idx}}" data-event-opts="{{[['tap',[['changeCollection',['$event']]]]]}}" bindtap="__e"><text class="dynamic_single_bot_text1 iconfont icon-c-xin1" style="{{(item.is_collect==1?'color:#FF0000':'')}}"></text><text class="dynamic_single_bot_text2">{{item.collection}}</text></view><view class="dynamic_single_bot_view"><text class="dynamic_single_bot_text1 iconfont icon-x-pinglun"></text><text class="dynamic_single_bot_text2">{{item.comment}}</text></view></view><block s-if="{{item.commentList.length>0}}"><view class="dzplbox"><view class="sjx"></view><block s-if="{{item.commentList.length>0}}"><view class="plhbox"><block s-for="{{item.commentList}}" s-for-item="comment" s-for-index="k1" s-key="k1"><block><view class="plh"><text>{{comment.nickname+":"}}</text>{{''+comment.content+''}}</view></block></block></view></block></view></block></view></block></block></block></block><block s-else><block><image class="pageNotice" src="{{$imgurl+'notice.png'}}"></image><view class="pageNoticeT">暂无内容</view></block></block><view data-event-opts="{{[['tap',[['goRelease',['$event']]]]]}}" class="release_btn" style="line-height:88rpx;" bindtap="__e">发布</view><view data-event-opts="{{[['tap',[['gomydata',['$event']]]]]}}" class="release_btn" style="bottom:300rpx;line-height:88rpx;" bindtap="__e">我的</view><block s-if="{{baseinfo.tabbar_t==1}}"><copyright vue-id="3" baseinfo="{{baseinfo}}"></copyright></block></view></block>